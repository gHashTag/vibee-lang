{
  "name": "language-999",
  "displayName": "Language 999",
  "description": "Syntax highlighting and LSP support for Language 999 with ternary logic",
  "version": "1.0.0",
  "publisher": "999os",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "999",
        "aliases": ["Language 999", "999"],
        "extensions": [".999"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "999",
        "scopeName": "source.999",
        "path": "./syntaxes/999.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gHashTag/vibee-lang"
  },
  "keywords": [
    "999",
    "ternary",
    "logic",
    "coptic",
    "balanced-ternary"
  ]
}
