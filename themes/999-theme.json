{
  "name": "999 Language Highlighting",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4"
  },
  "tokenColors": [
    {
      "scope": "comment.line.999",
      "settings": {
        "foreground": "#6a9955",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string.quoted.double.999",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "constant.character.escape.999",
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
      "scope": "keyword.control.999",
      "settings": {
        "foreground": "#569cd6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language.999",
      "settings": {
        "foreground": "#4ec9b0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.character.entity.999",
      "settings": {
        "foreground": "#c586c0"
      }
    },
    {
      "scope": "keyword.operator.999",
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": "storage.type.999",
      "settings": {
        "foreground": "#4ec9b0"
      }
    },
    {
      "scope": "storage.type.builtin.999",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "entity.name.function.999",
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "scope": "support.function.builtin.999",
      "settings": {
        "foreground": "#4ec9b0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation.operator.999",
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": "punctuation.accessor.999",
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": "constant.numeric.integer.999",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": "variable.other.999",
      "settings": {
        "foreground": "#d4d4d4"
      }
    }
  ]
}
