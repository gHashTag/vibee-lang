      - Go
      - Python
      - TypeScript
      - Gleam
      - WASM

  # –ú–µ—Ç—Ä–∏–∫–∏ –¥–ª—è –æ—Ü–µ–Ω–∫–∏ fitness
  FitnessMetrics:
    # –í—Ä–µ–º—è –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏ –∫–æ–¥–∞ (–º—Å)
    gen_time_ms: float
    # –í—Ä–µ–º—è –∫–æ–º–ø–∏–ª—è—Ü–∏–∏ (–º—Å)
    compile_time_ms: float
    # –í—Ä–µ–º—è –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è (–º—Å)
    runtime_ms: float
    # –†–∞–∑–º–µ—Ä –±–∏–Ω–∞—Ä–Ω–∏–∫–∞ (–ö–ë)
    binary_size_kb: float
    # –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ –ø–∞–º—è—Ç–∏ (–ú–ë)
    memory_mb: float
