    crossover_rate: float
    # –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ —ç–ª–∏—Ç–Ω—ã—Ö –æ—Å–æ–±–µ–π
    elite_count: int
    # –†–∞–∑–º–µ—Ä —Ç—É—Ä–Ω–∏—Ä–∞ –¥–ª—è –æ—Ç–±–æ—Ä–∞
    tournament_size: int
    # –í–µ—Å–∞ fitness —Ñ—É–Ω–∫—Ü–∏–∏
    weights: FitnessWeights
