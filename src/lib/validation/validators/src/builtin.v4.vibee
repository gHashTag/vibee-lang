@test \"credit_card_validator\" · // Valid credit cards (Luhn algorithm)
  assert(result.is_ok(CreditCard.from_string("4532015112830366")))  // Visa
  assert(result.is_ok(CreditCard.from_string("5425233430109903")))  // Mastercard

  // Invalid credit cards
  assert(result.is_error(CreditCard.from_string("1234567890123456")))  // Fails Luhn
  assert(result.is_error(CreditCard.from_string("4532015112830367")))  // Wrong checksum


@test \"date_validator\" · // Valid dates
  assert(result.is_ok(Date.from_string("2024-01-15")))
  assert(result.is_ok(Date.from_string("2023-12-31")))
  assert(result.is_ok(Date.from_string("2024-02-29")))  // Leap year

  // Invalid dates
  assert(result.is_error(Date.from_string("2024-13-01")))  // Invalid month
  assert(result.is_error(Date.from_string("2024-02-30")))  // Invalid day
  assert(result.is_error(Date.from_string("2023-02-29")))  // Not a leap year
  assert(result.is_error(Date.from_string("24-01-15")))  // Wrong format


