  assert(result.is_ok(Phone.from_string("+123456789012345")))

  // Invalid phones
  assert(result.is_error(Phone.from_string("123")))  // Too short
  assert(result.is_error(Phone.from_string("12345678901234567")))  // Too long
  assert(result.is_error(Phone.from_string("abc1234567")))  // Contains letters


@test \"age_validator\" · // Valid ages
  assert(result.is_ok(Age.from_int(0)))
  assert(result.is_ok(Age.from_int(25)))
  assert(result.is_ok(Age.from_int(150)))

  // Invalid ages
  assert(result.is_error(Age.from_int(-1)))
  assert(result.is_error(Age.from_int(151)))
  assert(result.is_error(Age.from_int(1000)))


