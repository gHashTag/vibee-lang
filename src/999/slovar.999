// ============================================
// СОКРАЩЕНИЯ ДЛЯ ЧАСТЫХ КОНСТРУКЦИЙ
// ============================================

// Однобуквенные для локальных переменных:
// ⲁ, ⲃ, ⲅ, ⲇ, ⲉ - первые 5
// ⲓ, ⲕ, ⲛ - индексы циклов
// ⲭ, ⲩ, ⲍ - координаты
// ⲣ - результат
// ⲧ - временная

// Двухбуквенные для параметров:
// ⲁⲣ - аргумент
// ⲃⲗ - блок
// ⲕⲗ - ключ
// ⲍⲛ - значение
// ⲫⲛ - функция
// ⲥⲧ - строка
// ⲥⲡ - список

// ============================================
// ОПЕРАТОРЫ
// ============================================

// Арифметика: + - * / %
// Сравнение: == != < > <= >=
// Логика: && || !
// Присваивание: = += -= *= /=
// Доступ: . [] ()
// Диапазон: ..
// Стрелка: ->

// ============================================
// ПРИМЕР КОДА НА ЧИСТОМ 999
// ============================================

// Структура точки
Ⲏ Ⲧⲭⲕ:
  ⲭ: Ⲋ
  ⲩ: Ⲋ

// Функция расстояния
Ⲫ ⲣⲁⲥⲥⲧ(ⲁ: Ⲧⲭⲕ, ⲃ: Ⲧⲭⲕ) -> Ⲇ:
  Ⲙ ⲇⲭ = ⲃ.ⲭ - ⲁ.ⲭ
  Ⲙ ⲇⲩ = ⲃ.ⲩ - ⲁ.ⲩ
  Ⲣ ⲥⲕⲣⲧ(ⲇⲭ * ⲇⲭ + ⲇⲩ * ⲇⲩ)

// Функция суммы списка
Ⲫ ⲥⲩⲙⲙⲁ(ⲥⲡ: [Ⲋ]) -> Ⲋ:
  Ⲙ ⲣ = 0
  Ⲯ ⲭ ⲥⲡ:
    ⲣ += ⲭ
  Ⲣ ⲣ

// Функция фильтрации
Ⲫ ⲫⲓⲗⲩⲧⲣ(ⲥⲡ: [Ⲋ], ⲫⲛ: Ⲫ(Ⲋ) -> Ⲃ) -> [Ⲋ]:
  Ⲙ ⲣ: [Ⲋ] = []
  Ⲯ ⲭ ⲥⲡ:
    Ⲝ ⲫⲛ(ⲭ):
      ⲣ += ⲭ
  Ⲣ ⲣ

// Функция отображения
Ⲫ ⲙⲁⲡ(ⲥⲡ: [Ⲋ], ⲫⲛ: Ⲫ(Ⲋ) -> Ⲋ) -> [Ⲋ]:
  Ⲙ ⲣ: [Ⲋ] = []
  Ⲯ ⲭ ⲥⲡ:
    ⲣ += ⲫⲛ(ⲭ)
  Ⲣ ⲣ

// Главная функция
Ⲫ ⲅⲗⲁⲃⲛⲁⲁ() -> Ⲋ:
  Ⲙ ⲧ1 = Ⲧⲭⲕ { ⲭ: 0, ⲩ: 0 }
  Ⲙ ⲧ2 = Ⲧⲭⲕ { ⲭ: 3, ⲩ: 4 }
  
  Ⲙ ⲇ = ⲣⲁⲥⲥⲧ(ⲧ1, ⲧ2)
  ⲡⲉⲭⲁⲧⲩ(ⲇ)  // 5.0
  
  Ⲙ ⲥⲡ = [1, 2, 3, 4, 5]
  Ⲙ ⲥ = ⲥⲩⲙⲙⲁ(ⲥⲡ)
  ⲡⲉⲭⲁⲧⲩ(ⲥ)  // 15
  
  Ⲙ ⲭⲉⲧ = ⲫⲓⲗⲩⲧⲣ(ⲥⲡ, ⲭ -> ⲭ % 2 == 0)
  ⲡⲉⲭⲁⲧⲩ(ⲭⲉⲧ)  // [2, 4]
  
  Ⲙ ⲕⲃ = ⲙⲁⲡ(ⲥⲡ, ⲭ -> ⲭ * ⲭ)
  ⲡⲉⲭⲁⲧⲩ(ⲕⲃ)  // [1, 4, 9, 16, 25]
  
  Ⲣ 0

