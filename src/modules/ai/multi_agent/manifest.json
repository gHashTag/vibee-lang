{
  "name": "multi_agent",
  "version": "1.0.0",
  "description": "Coordination of multiple agents",
  "category": "ai",
  "depends_on": ["agent", "memory", "event", "context"],
  "exports": [
    "create_team",
    "add_agent",
    "remove_agent",
    "delegate_task",
    "broadcast_message",
    "Team",
    "AgentMessage"
  ],
  "commands": ["team.create", "team.add", "team.delegate"],
  "events": {
    "emits": ["team.message", "team.task_complete", "team.error"],
    "listens": ["agent.started", "agent.stopped"]
  }
}