/// Agent team
type Team = {
  id: String,
  name: String,
  agent_ids: List<String>,
  leader_id: String,
  communication_mode: String,  // "hierarchical", "democratic", "round_robin"
  created_at: Int
}

/// Message between agents
type AgentMessage = {
  id: String,
  from_agent: String,
  to_agent: String,
  content: Value,
  message_type: String,  // "task", "query", "response", "broadcast"
  timestamp: Int
}

/// Create a new agent team
/// @param name - Team name
/// @param mode - Communication mode
/// @returns Created team
fn create_team(name: String, communication_mode: String) -> Result<Team, String>

/// Add agent to team
/// @param team_id - Team ID
/// @param agent_id - Agent ID to add
fn add_agent(team_id: String, agent_id: String) -> Result<Team, String>

/// Remove agent from team
/// @param team_id - Team ID
/// @param agent_id - Agent ID to remove
fn remove_agent(team_id: String, agent_id: String) -> Result<Team, String>

/// Delegate task to agent
/// @param team_id - Team ID
/// @param agent_id - Target agent
/// @param task - Task description
/// @returns Task ID
fn delegate_task(team_id: String, agent_id: String, task: String) -> Result<String, String>

/// Broadcast message to all agents
/// @param team_id - Team ID
/// @param message - Message content
fn broadcast_message(team_id: String, message: Value) -> Result<Nil, String>

event team.message(AgentMessage)
event team.task_complete(String, Value)
event team.error(String, String)