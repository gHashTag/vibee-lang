{
  "name": "cache",
  "version": "1.0.0",
  "description": "Caching layer (Memory, Redis, Memcached, File)",
  "category": "web",
  "dependencies": ["state"],
  "exports": [
    "get(key: String) -> Result<Value, String>",
    "set(key: String, value: Value, ttl: Int) -> Result<Nil, String>",
    "delete(key: String) -> Result<Nil, String>",
    "exists(key: String) -> Bool",
    "clear() -> Result<Nil, String>",
    "get_stats() -> CacheStats"
  ],
  "commands": [
    {"name": "cache.get", "params": ["key: String"]},
    {"name": "cache.set", "params": ["key: String", "value: Value", "ttl: Int"]},
    {"name": "cache.delete", "params": ["key: String"]},
    {"name": "cache.clear", "params": []},
    {"name": "cache.stats", "params": []}
  ],
  "events": {
    "emits": ["cache:hit", "cache:miss", "cache:set", "cache:deleted", "cache:cleared"],
    "listens": ["http:request", "state:changed"]
  }
}
