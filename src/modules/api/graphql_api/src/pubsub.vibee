// === SUBSCRIPTION MANAGEMENT ===

@spec subscribe_lead_forwarded(client_id: str, subscription_id: str, filter_chat_id: str?) → Nil
impl: subscription_id, filter_chat_id · subscribe_lead_forwarded:
  subscription_id, topic_lead_forwarded, filter_chat_id · ffi_subscribe

@spec subscribe_lead_status_changed(client_id: str, subscription_id: str, filter_lead_id: int?) → Nil
impl: subscription_id, filter_lead_id · subscribe_lead_status_changed:
  let filter = case filter_lead_id:
    ☐id) → ☐int.$(id))
    ∅-> ∅subscription_id, topic_lead_status_changed, filter · ffi_subscribe

@spec subscribe_trigger_detected(client_id: str, subscription_id: str, filter_chat_id: str?) → Nil
impl: subscription_id, filter_chat_id · subscribe_trigger_detected:
  subscription_id, topic_trigger_detected, filter_chat_id · ffi_subscribe

@spec subscribe_forward_failed(client_id: str, subscription_id: str, filter_chat_id: str?) → Nil
impl: subscription_id, filter_chat_id · subscribe_forward_failed:
  subscription_id, topic_forward_failed, filter_chat_id · ffi_subscribe

subscribe_funnel_stats(client_id: str, subscription_id: str) → Nil:client_id, subscription_id):
  subscription_id, topic_funnel_stats, ∅· ffi_subscribe

unsubscribe(client_id: str, subscription_id: str) → Nil:client_id, subscription_id):
  subscription_id · ffi_unsubscribe

unsubscribe_client(client_id: str) → Nil:client_id):
  ffi_unsubscribe_client(client_id)

// === PUBLISHING ===

publish_lead_forwarded(event: PubSubEvent) → Nil:event):
  event · ffi_publish

publish_lead_status_changed(event: PubSubEvent) → Nil:event):
  event · ffi_publish

publish_trigger_detected(event: PubSubEvent) → Nil:event):
  event · ffi_publish

publish_forward_failed(event: PubSubEvent) → Nil:event):
  event · ffi_publish

publish_funnel_stats_updated(event: PubSubEvent) → Nil:event):
  event · ffi_publish

// === EVENT RETRIEVAL ===

get_pending_events(client_id: str, subscription_id: str) → [Dynamic]:client_id, subscription_id):
  subscription_id · ffi_get_pending_events

clear_pending_events(client_id: str, subscription_id: str) → Nil:client_id, subscription_id):
  subscription_id · ffi_clear_pending_events

