@spec json_success_response(data: Json) → Response(ResponseData) {
  /// Create JSON success response
  given: JSON data
  when: Returning success
  then: Returns 200 response with CORS
}

@impl {
  response_new(200)
  · response_set_header("content-type", "application/json")
  · response_set_header("access-control-allow-origin", "*")
  · response_set_body(mist_bytes(bytes_tree_from_string(json_to_string(data))))
}

@spec json_error_response(status: int, message: str) → Response(ResponseData) {
  /// Create JSON error response
  given: Status code and error message
  when: Returning error
  then: Returns error response with CORS
}

@impl {
  let body = json_object([("error", json_string(message))])
  response_new(status)
  · response_set_header("content-type", "application/json")
  · response_set_header("access-control-allow-origin", "*")
  · response_set_body(mist_bytes(bytes_tree_from_string(json_to_string(body))))
}

