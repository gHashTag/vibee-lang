@impl {
  let query = request_get_query(req)

  let telegram_id = match query {
    ✅params) → "telegram_id" · find_param
    ❌_) → ∅}

  match telegram_id {
    ∅-> "telegram_id is required" · json_error_response
    ☐tg_id) → {
      let app_id = env_get("INSTAGRAM_APP_ID") · result_unwrap("")
      let redirect_uri = env_get("INSTAGRAM_REDIRECT_URI")
        · result_unwrap("https://vibee-player.fly.dev/instagram/callback")

      match app_id {
        "" → "Instagram App ID not configured" · json_error_response
        _ → {
          let auth_url = "{OAUTH_BASE}?client_id={app_id}&redirect_uri={uri_encode(redirect_uri)}&scope=instagram_basic,instagram_content_publish,pages_show_list,pages_read_engagement&response_type=code&state={tg_id}"

          json_success_response(json_object([
            ("auth_url", json_string(auth_url))
          ]))
        }
      }
    }
  }
}

