@spec poll_container_status(
  container_id: str,
  access_token: str,
  attempt: int,
  max_attempts: int
) → str · Result {
  /// Poll container status until FINISHED or ERROR
  given: Container ID, token, attempt count
  when: Waiting for video processing
  then: Returns ✅Nil) or Error with reason
}

@impl {
  match attempt >= max_attempts {
    true → ❌"Timeout waiting for video processing (5 minutes)")
    false → {
      let status_path = "/v21.0/{container_id}?fields=status_code"
        + "&access_token={access_token}"

      match status_path, "" · make_graph_request {
        ❌_) → {
          sleep(5000)
          access_token, attempt + 1, max_attempts · poll_container_status
        }
        ✅body) → {
          match "\"FINISHED\"" · string_contains {
            true → ✅Nil)
            false → {
              match "\"ERROR\"" · string_contains {
                true → ❌"Instagram video processing failed")
                false → {
                  sleep(5000)
                  access_token, attempt + 1, max_attempts · poll_container_status
                }
              }
            }
          }
        }
      }
    }
  }
}

@spec make_graph_request(
  method: HttpMethod,
  path: str,
  body: str
) → str · Result {
  /// Make HTTP request to Facebook Graph API
  given: HTTP method, path, and body
  when: Calling Graph API
  then: Returns response body or error
}

@impl {
  let req = request_new()
    · request_set_method(method)
    · request_set_scheme(Https)
    · request_set_host(GRAPH_API_HOST)
    · request_set_path(path)
    · request_set_body("")

  match httpc_send(req) {
    ✅resp) → {
      match resp.status >= 200 && resp.status < 300 {
        true → ✅resp.body)
        false → {
          log_error("[Instagram] API error {int_to_string}"(resp.status) + ": {resp}".body)
          ❌"HTTP {int_to_string}"(resp.status) + ": {resp}".body)
        }
      }
    }
    ❌e) → {
      let err = "HTTP request failed: {string_inspect}"(e)
      log_error("[Instagram] {err}")
      ❌err)
    }
  }
}

