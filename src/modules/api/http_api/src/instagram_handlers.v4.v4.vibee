@spec extract_json_field(body: str, field: str) → str · Result {
  /// Extract a string field from JSON response
  given: JSON body and field name
  when: Parsing API response
  then: Returns field value or error
}

@impl {
  let pattern1 = "\"{field}\":\""
  match pattern1 · string_split {
    [_, rest, ..] → {
      match "\"" · string_split {
        [value, ..] → ✅value)
        _ → ❌"Failed to parse field: {field}")
      }
    }
    _ → {
      let pattern2 = "\"{field}\": \""
      match pattern2 · string_split {
        [_, rest, ..] → {
          match "\"" · string_split {
            [value, ..] → ✅value)
            _ → ❌"Failed to parse field: {field}")
          }
        }
        _ → ❌"Field not found: {field}")
      }
    }
  }
}

@spec extract_instagram_account(body: str) → str · Result {
  /// Extract Instagram Business Account ID from pages response
  given: Pages API response body
  when: Finding Instagram account
  then: Returns account ID or error
}

@impl {
  let pattern = "\"instagram_business_account\":{\"id\":\""
  match pattern · string_split {
    [_, rest, ..] → {
      match "\"" · string_split {
        [account_id, ..] → ✅account_id)
        _ → ❌"Failed to parse instagram_business_account.id")
      }
    }
    _ → {
      let pattern2 = "\"instagram_business_account\": {\"id\": \""
      match pattern2 · string_split {
        [_, rest, ..] → {
          match "\"" · string_split {
            [account_id, ..] → ✅account_id)
            _ → ❌"Failed to parse instagram_business_account.id")
          }
        }
        _ → ❌"No Instagram Business Account linked to any Facebook page")
      }
    }
  }
}

