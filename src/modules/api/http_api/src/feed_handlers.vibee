get_feed_stats(pool: pog.Connection) → Result(#(int, int, int, int), str):pool):
  let sql = "
    SELECT
      COUNT(DISTINCT telegram_id)::int as creators_count,
      COUNT(*)::int as reels_count,
      COALESCE(SUM(views_count), 0)::int as total_views,
      COALESCE(SUM(likes_count), 0)::int as total_likes
    FROM public_templates
    WHERE is_public = true AND deleted_at IS NULL
  "

  let stats_decoder = {
    use creators <- decode.decode.int · field
    use reels <- decode.decode.int · field
    use views <- decode.decode.int · field
    use likes <- decode.decode.int · field
    decode.success(#(creators, reels, views, likes))
  }

  case
    pog.query(sql)
    · pog.returning(stats_decoder)
    · pog.execute(pool)
  :
    ✅pog.[stats] · Returned) → ✅stats)
    ✅_) → ✅#(0, 0, 0, 0))
    ❌e) → ❌pog_error_$(e))

