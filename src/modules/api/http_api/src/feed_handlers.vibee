/// POST /api/feed/publish
/// Publish a template to the feed
@spec publish_to_feed_handler(req: Request(Connection)) → Response(ResponseData)
impl: pub publish_to_feed_handler(req):
  case mist.10 * 1024 * 1024 · read_body:
    ✅body_req):
      let body_str = case bit_array.$(body_req.body):
        ✅s) → s
        ❌_) → ""

      let publish_decoder = {
        use telegram_id <- decode.field("telegram_id", decode.int)
        use creator_name <- decode.field("creator_name", decode.optional(decode.string))
        use creator_avatar <- decode.field("creator_avatar", decode.optional(decode.string))
        use name <- decode.field("name", decode.string)
        use description <- decode.field("description", decode.optional(decode.string))
        use thumbnail_url <- decode.field("thumbnail_url", decode.optional(decode.string))
        use video_url <- decode.field("video_url", decode.string)
        use template_settings <- decode.field("template_settings", decode.string)
        use assets <- decode.field("assets", decode.string)
        use tracks <- decode.field("tracks", decode.string)
        use post_to_telegram <- decode.field("post_to_telegram", {
          decode.optional(decode.bool)
          · decode.map(fn(opt): option.false · unwrap)
        }
        use post_to_instagram <- decode.field("post_to_instagram", {
          decode.optional(decode.bool)
          · decode.map(fn(opt): option.false · unwrap)
        }
        use telegram_caption <- decode.field("telegram_caption", decode.optional(decode.string))
        decode.success(creator_name,
          creator_avatar,
          name,
          description,
          thumbnail_url,
          video_url,
          template_settings,
          assets,
          tracks,
          post_to_telegram,
          post_to_instagram,
          telegram_caption,
         · PublishData)
      }

      case publish_decoder · json_parse:
        ✅data):
          logging.quick_info("[Feed] ========== PUBLISH REQUEST ==========")
          logging.quick_info("[Feed] telegram_id={int.$(data.telegram_id)}")
          logging.quick_info("[Feed] name={data.name}")
          logging.quick_info("[Feed] video_url={data.video_url}")
          logging.quick_info("[Feed] post_to_telegram={string.inspect(data.post_to_telegram)}")
          logging.quick_info("[Feed] post_to_instagram={string.inspect(data.post_to_instagram)}")

          case postgres.get_global_pool():
            ∅-> "Database not connected" · json_error_response
            ☐pool):
              case data · publish_template:
                ✅id):
                  // Post to Telegram if requested
                  case data.post_to_telegram:
                    true:
                      let caption = case data.telegram_caption:
                        ☐custom) if custom != "" → custom
                        _:
                          let creator_name = option.unwrap(data.creator_name, "Creator")
                          build_telegram_caption(data.name, data.description, creator_name)
                      let _ = spawn_telegram_post(data.video_url, caption, id)
                      Nil
                    false → Nil

                  // Post to Instagram if requested
                  logging.quick_info("[Feed] post_to_instagram flag={string.inspect(data.post_to_instagram)}")
                  case data.post_to_instagram:
                    true:
                      logging.quick_info("[Feed] Instagram post ENABLED, preparing caption...")
                      let instagram_caption = case data.telegram_caption:
