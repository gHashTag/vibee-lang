spawn_telegram_post(video_url: str, caption: str, template_id: int) → str · Result:video_url, caption, template_id):
  let bridge_url = config.get_env_or("VIBEE_BRIDGE_URL", "https://vibee-telegram-bridge.fly.dev")
  let api_key = config.get_env_or("VIBEE_API_KEY", "")
  let channel_id = config.get_env_or("VIBEE_TELEGRAM_CHANNEL", "@vibee_reels")
  let session_id = config.get_env_or("VIBEE_BOT_SESSION_ID", "vibee_bot")

  let body = json.$(json.object([
    #("session_id", json.string(session_id)),
    #("chat_id", json.string(channel_id)),
    #("video_url", json.string(video_url)),
    #("caption", json.string(caption)),
  ]))

  let bridge_host = extract_host(bridge_url)

  let req =
    request.new()
    · request.set_method(http.Post)
    · request.set_scheme(http.Https)
    · request.set_host(bridge_host)
    · request.set_path("/send/video")
    · request.set_header("Content-Type", "application/json")
    · request.set_header("Authorization", "Bearer {api_key}")
    · request.set_body(body)

  case httpc.send(req):
    ✅response):
      case response.status >= 200 && response.status < 300:
        true:
          logging.quick_info("Posted template {int.$(template_id)} to Telegram channel {channel_id}")
          ✅Nil)
        false:
          logging.quick_error("Failed to post to Telegram: status {int.$(response.status)}")
          ❌"Telegram post failed: {int.$(response.status)}")
    ❌_):
      logging.quick_error("Failed to connect to Telegram bridge")
      ❌"Connection to bridge failed")

extract_host(url: str) → str:url):
  let without_protocol = case string."://" · split:
    [_, rest, ..] → rest
    _ → url
  case string."/" · split:
    [host, ..] → host
    _ → without_protocol

generate_ai_caption(name: str, description: str, creator_name: str) → str:name, description, creator_name):
  let hook_index = len(name) % 5
  let hook = case hook_index:
    0 → "Это надо видеть!"
    1 → "Секрет успеха в этом видео"
    2 → "Смотри до конца!"
    3 → "Ты не поверишь, но..."
    _ → "POV: когда ты понял главное"

  let desc_part = case description:
    "" → ""
    d → "\n\n {d}"

  "{hook}\n\n {name}{desc_part}\n\n {creator_name}\n  vibee-player.fly.dev/feed\n\n#vibee #reels #ai #контент #создатель"

