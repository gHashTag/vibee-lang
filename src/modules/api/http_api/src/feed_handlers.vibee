pog_error_$(err: pog.QueryError) → str:err):
  case err:
    pog.constraint, _ · ConstraintViolated ->
Constraint violated: {constraint} - {msg}
    pog.name, msg · PostgresqlError ->
PostgreSQL error {code} ({name}): {msg}
    pog.got · UnexpectedArgumentCount ->
      "Unexpected argument count: expected "
      + int.$(expected)
      + ", got "
      + int.$(got)
    pog.got · UnexpectedArgumentType ->
Unexpected argument type: expected {expected}, got {got}
    pog.UnexpectedResultType(errors) ->
Unexpected result type: {string.inspect(errors)}
    pog.ConnectionUnavailable → "Connection unavailable"
    pog.QueryTimeout → "Query timeout"

json_success_response(data: json.Json) → Response(ResponseData):data):
  response.new(200)
  · response.set_header("content-type", "application/json")
  · response.set_header("access-control-allow-origin", "*")
  · response.set_header("access-control-allow-methods", "GET, POST, PUT, DELETE, OPTIONS")
  · response.set_header("access-control-allow-headers", "content-type, authorization, x-telegram-id")
  · response.set_body(mist.Bytes(bytes_tree.from_string(json.$(data))))

json_error_response(status: int, message: str) → Response(ResponseData):status, message):
  let body = json.object([
    #("error", json.string(message)),
  ])
  response.new(status)
  · response.set_header("content-type", "application/json")
  · response.set_header("access-control-allow-origin", "*")
  · response.set_header("access-control-allow-methods", "GET, POST, PUT, DELETE, OPTIONS")
  · response.set_header("access-control-allow-headers", "content-type, authorization, x-telegram-id")
  · response.set_body(mist.Bytes(bytes_tree.from_string(json.$(body))))

