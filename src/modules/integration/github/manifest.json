{
  "name": "github",
  "version": "1.0.0",
  "description": "GitHub API integration for issues, pull requests, and webhooks",
  "category": "integration",
  "depends_on": ["event"],
  "exports": [
    "GitHubConfig",
    "GitHubIssue",
    "GitHubUser",
    "GitHubPR",
    "GitHubCommit",
    "create_client",
    "get_issue",
    "create_issue",
    "close_issue",
    "get_pr",
    "create_pr",
    "list_commits",
    "create_webhook"
  ],
  "commands": ["github.issue", "github.pr"],
  "events": {
    "emits": ["github.issue", "github.pr", "github.push", "github.error"],
    "listens": []
  }
}
