{
  "name": "stripe",
  "version": "1.0.0",
  "description": "Stripe payments integration for customers, subscriptions, and payment intents",
  "category": "integration",
  "depends_on": ["event"],
  "exports": [
    "StripeConfig",
    "StripeCustomer",
    "StripePaymentIntent",
    "StripeSubscription",
    "StripeProduct",
    "create_client",
    "create_customer",
    "get_customer",
    "create_payment_intent",
    "confirm_payment",
    "create_subscription",
    "cancel_subscription",
    "create_product"
  ],
  "commands": ["stripe.payment", "stripe.subscription"],
  "events": {
    "emits": ["stripe.payment", "stripe.subscription", "stripe.error"],
    "listens": []
  }
}
