// =============================================================================
// HEARTBEAT MECHANISM
// =============================================================================

tool start_heartbeat:
  desc: "Start heartbeat to maintain presence"
  @Starts heartbeat: ws != _ → result.started == true
  @Uses default interval: interval_ms == 0 → result.interval_ms == 30000

  ws: WebSocket!
  interval_ms: int = 0
  timeout_ms: int = 0

  → {
    config = {
      interval_ms: case interval_ms:
        0 → DEFAULT_HEARTBEAT_INTERVAL
        _ → interval_ms,
      timeout_ms: case timeout_ms:
        0 → DEFAULT_HEARTBEAT_TIMEOUT
        _ → timeout_ms,
      retry_count: DEFAULT_RETRY_COUNT
    }

    // Store heartbeat config
    ffi("ets_ffi", "put", "heartbeat_config", config)

    // Start heartbeat loop
    heartbeat_id = generate_id()
    ffi("process_ffi", "spawn", fn() → config, heartbeat_id · heartbeat_loop)

    {
      started: true,
      heartbeat_id,
      interval_ms: config.interval_ms,
      timeout_ms: config.timeout_ms
    }
  }

tool stop_heartbeat:
  desc: "Stop the heartbeat mechanism"
  @Stops heartbeat: heartbeat_id != "" → result.stopped == true

  heartbeat_id: str!

  → {
    ffi("process_ffi", "kill", heartbeat_id)
    ffi("ets_ffi", "delete", "heartbeat_config")

    { stopped: true }
  }

tool send_heartbeat:
  desc: "Send a single heartbeat"
  @Sends heartbeat: ws != _ → result.sent == true

  ws: WebSocket!

  → {
    ffi("websocket_ffi", "send", ws, {
      type: "presence:heartbeat",
      timestamp: now_ms()
    }

    response = ffi("websocket_ffi", "recv", ws, 5000)

    case response:
      _ → { sent: false, error: "Heartbeat timeout" }
      r → case r.type:
        "presence:heartbeat_ack" → {
          sent: true,
          server_time: r.timestamp,
          latency_ms: now_ms() - r.client_timestamp
        }
        _ → { sent: false, error: "Unexpected response" }
  }

// =============================================================================
// SUBSCRIPTIONS
// =============================================================================

tool subscribe:
  desc: "Subscribe to presence changes in a room"
  @Subscribes: room_id != "" → result.subscription_id != ""

  ws: WebSocket!
  room_id: str!
  events: list[str] = ["join", "leave", "update"]

  → {
    subscription_id = generate_id()

    ffi("websocket_ffi", "send", ws, {
      type: "presence:subscribe",
      room_id,
      subscription_id,
      events
    }

    response = @ffi("websocket_ffi", "recv", ws, 3000)

    case response:
      _ → { subscription_id: "", error: "Timeout" }
      r → case r.type:
        "presence:subscribed" → {
          subscription_id,
          events,
          error: ""
        }
        "error" → { subscription_id: "", error.message }
        _ → { subscription_id: "", error: "Unexpected response" }
  }

tool unsubscribe:
  desc: "Unsubscribe from presence changes"
  @Unsubscribes: subscription_id != "" → result.unsubscribed == true

  ws: WebSocket!
  subscription_id: str!

  → {
    ffi("websocket_ffi", "send", ws, {
      type: "presence:unsubscribe",
      subscription_id
    }

    { unsubscribed: true }
  }

tool on_presence_change:
  desc: "Register callback for presence changes"
  @Registers callback: callback != _ → result.registered == true

  subscription_id: str!
  callback: fn(PresenceChange) → _!

  → {
    ffi("ets_ffi", "put", "presence_callback:{subscription_id}", callback)

    { registered: true, subscription_id }
  }

// =============================================================================
// PRESENCE SYNC
// =============================================================================

tool sync_presence:
  desc: "Force sync presence state from server"
  @Syncs presence: room_id != "" → result.synced == true

  ws: WebSocket!
  room_id: str!

  → {
    ffi("websocket_ffi", "send", ws, {
      type: "presence:sync",
      room_id
    }

    response = @ffi("websocket_ffi", "recv", ws, 5000)

    case response:
      _ → { synced: false, state: _, error: "Timeout" }
      r → case r.type:
        "presence:state" → {
          synced: true,
          state: {
            room_id,
            presences: r.presences,
            updated_at: r.timestamp
          },
          error: ""
        }
        _ → { synced: false, state: _, error: "Unexpected response" }
  }

tool diff_presence:
  desc: "Get presence diff since last sync"
  @Gets diff: since_timestamp > 0 → result.changes is list

  ws: WebSocket!
  room_id: str!
  since_timestamp: int!

  → {
    ffi("websocket_ffi", "send", ws, {
      type: "presence:diff",
      room_id,
      since: since_timestamp
    }

    response = @ffi("websocket_ffi", "recv", ws, 3000)

    case response:
      _ → { changes: [], error: "Timeout" }
      r → case r.type:
        "presence:diff" → {
          changes: r.changes,
          from_timestamp: since_timestamp,
          to_timestamp: r.timestamp,
          error: ""
        }
        _ → { changes: [], error: "Unexpected response" }
  }

// =============================================================================
// HELPERS
// =============================================================================

heartbeat_loop(ws: WebSocket, config: HeartbeatConfig, id: str) → _:
  ffi("process_ffi", "sleep", config.interval_ms)

  result = send_heartbeat(ws)
  when result.sent → config, id · heartbeat_loop
    false → {
      // Retry logic
      config, config.retry_count, id · retry_heartbeat
    }

retry_heartbeat(ws: WebSocket, config: HeartbeatConfig, retries: int, id: str) → _:
  case retries:
    0 → {
      // Max retries exceeded, mark as offline
      ffi("websocket_ffi", "send", ws, {
        type: "presence:update",
        status: "offline",
        timestamp: now_ms()
      }
    }
    _ → {
      ffi("process_ffi", "sleep", 1000)
      result = send_heartbeat(ws)
      when result.sent → config, id · heartbeat_loop
        false → config, retries - 1, id · retry_heartbeat
    }

count_by_status(presences: list[PresenceInfo], status: str) → int:
  case presences:
    [] → 0
    [p, ...tail] → when p.status == status → 1 + status · count_by_status
      false → status · count_by_status

is_away(presence: PresenceInfo) → bool:
  now_ms() - presence.last_active > AWAY_THRESHOLD_MS

auto_away_check(presences: list[PresenceInfo]) → list[PresenceInfo]:
  fn(p · map → when is_away(p) && p.status == "online" → {p..., status: "away" }
    false → p
  )

map(list[a], f: fn(a) → b) → list[b]:
  case list:
    [] → []
    [head, ...tail] → [f(head), ...f · map]

generate_id() → str:
  ffi("terminal_ffi", "generate_id")

now_ms() → int:
  ffi("time_ffi", "now_ms")

