tool leave:
  desc: "Leave the current room"
  @Leaves room: ws != _ → result.left == true

  ws: WebSocket!

  → {
    ffi("websocket_ffi", "send", ws, {
      type: "room:leave"
    }
    { left: true }
  }

tool send_message:
  desc: "Send a message to the room"
  @Sends message: content != "" → result.sent == true

  ws: WebSocket!
  content: str!

  → {
    message = {
      id: generate_id(),
      content,
      timestamp: now_ms()
    }

    ffi("websocket_ffi", "send", ws, {
      type: "room:message",
      message
    }

    { sent: true, message_id: message.ID }
  }

tool set_presence:
  desc: "Update presence status"
  @Updates presence: status != "" → result.updated == true

  ws: WebSocket!
  status: str! @one_of("online", "typing", "away", "busy")

  → {
    ffi("websocket_ffi", "send", ws, {
      type: "presence:update",
      status
    }
    { updated: true }
  }

// =============================================================================
// ROOM DISCOVERY
// =============================================================================

tool list_rooms:
  desc: "list available public rooms"
  @Lists rooms: ws != _ → result.rooms is list

  ws: WebSocket!

  → {
    @ffi("websocket_ffi", "send", ws, {
      type: "room:list"
    }

    response = @ffi("websocket_ffi", "recv", ws, 3000)

    case response:
      _ → { rooms: [], error: "Timeout" }
      r → case r.type:
        "room:list" → { rooms: r.rooms, error: "" }
        _ → { rooms: [], error: "Unexpected response" }
  }

tool create_room:
  desc: "Create a new room"
  @Creates room: name != "" → result.room_id != ""

  ws: WebSocket!
  name: str!
  is_public: bool = true

  → {
    room_id = generate_room_id()

    ffi("websocket_ffi", "send", ws, {
      type: "room:create",
      room_id,
      name,
      is_public
    }

    response = @ffi("websocket_ffi", "recv", ws, 3000)

    case response:
      _ → { room_id: "", error: "Timeout" }
      r → case r.type:
        "room:created" → { room_id: r.room_id, error: "" }
        "error" → { room_id: "", error.message }
        _ → { room_id: "", error: "Unexpected response" }
  }

