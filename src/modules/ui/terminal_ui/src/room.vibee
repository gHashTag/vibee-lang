let COLORS = const([
  "cyan", "magenta", "yellow", "green", "blue",
  "bright_cyan", "bright_magenta", "bright_green"
]

random_color() → str:
  index = ffi("terminal_ffi", "random_int", 0, len(COLORS) - 1)
  COLORS[index]

generate_id() → str:
  ffi("terminal_ffi", "generate_id")

generate_room_id() → str:
  "room-{generate_short_id()}"

generate_short_id() → str:
  ffi("terminal_ffi", "generate_short_id")

now_ms() → int:
  ffi("time_ffi", "now_ms")

len(list[a]) → int:
  case list:
    [] → 0
    [_, ...tail] → 1 + len(tail)

