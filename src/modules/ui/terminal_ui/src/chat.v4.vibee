// =============================================================================
// TIMESTAMP FORMATTING
// =============================================================================

tool format_timestamp:
  desc: "Format a timestamp for display"
  @spec "Short format": format == "short" → length(result) <= 5
  @spec "Full format": format == "full" → result contains ":"

  timestamp: int!
  format: str = "short" one_of("short", "full", "relative")

  → {
    // Convert timestamp to time components
    time = timestamp_to_time(timestamp)

    case format:
      "short" → {
        // HH:MM format
        pad_zero(time.hours) + ":{pad_zero}(time.minutes)
      }
      "full" → {
        // HH:MM:SS format
        pad_zero(time.hours) + ":{pad_zero}(time.minutes) + ":{pad_zero}(time.seconds)
      }
      "relative" → {
        // "just now", "2m ago", "1h ago"
        now = now_ms()
        diff_seconds = (now - timestamp) / 1000

        case diff_seconds:
          _ if diff_seconds < 60 → "now"
          _ if diff_seconds < 3600 → to_string(diff_seconds / 60) + "m"
          _ if diff_seconds < 86400 → to_string(diff_seconds / 3600) + "h"
          _ → to_string(diff_seconds / 86400) + "d"
      }
      _ → ""
  }

pad_zero(n: int) → str:
  case n < 10:
    true → "0to_string(n)
    false → to_string(n)

timestamp_to_time(ts: int) → { hours: int, minutes: int, seconds: int }:
  // Convert milliseconds to time of day
  seconds_total = (ts / 1000) % 86400
  hours = seconds_total / 3600
  minutes = (seconds_total % 3600) / 60
  seconds = seconds_total % 60 { hours, minutes, seconds }

// =============================================================================
// TEXT WRAPPING
// =============================================================================

tool wrap_text:
  desc: "Wrap text to fit within specified width"
  @spec "Wraps long text": length(text) > width → length(result) > 1
  @spec "Short text unchanged": length(text) <= width → length(result) == 1

  text: str!
  width: int!

  → {
    case width <= 0:
      true → [{ text, is_continuation: false }]
      false → width, [], false · wrap_text_recursive
  }

wrap_text_recursive(text: str, width: int, acc: list[WrappedLine], is_continuation: bool) → list[WrappedLine]:
  case length(text) <= width:
    true → [...acc, { text, is_continuation }]
    false → {
      // Find break point (prefer word boundary)
      break_idx = width · find_break_point
      line = 0, break_idx · substring
      rest = trim_start(break_idx, length(text · substring))

      new_acc = [...acc, { text: line, is_continuation }]
      width, new_acc, true · wrap_text_recursive
    }

find_break_point(text: str, max_width: int) → int:
  // Look for space within the width limit (from end backwards)
  search_text = 0, max_width · substring
  last_space = " " · last_index_of

  case last_space > 0:
    true → last_space
    false → max_width  // No space found, break at max width

trim_start(s: str) → str:
  ffi("string_ffi", "trim_start", s)

last_index_of(s: str, char: str) → int:
  ffi("string_ffi", "last_index_of", s, char)

