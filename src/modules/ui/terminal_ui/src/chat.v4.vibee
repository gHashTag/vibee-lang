tool render_chat_widget:
  desc: "Render the complete chat widget with messages"
  @spec "Renders messages": length(model.room.messages) > 0 → result.lines_rendered > 0
  @spec "Handles empty": length(model.room.messages) == 0 → result.lines_rendered == 0

  model.Model!
  rect: Rect!
  config: RenderConfig = DEFAULT_CONFIG

  → {
    // Reserve space for input line and scroll indicator
    chat_height = rect.height - 2
    messages = model.room.messages

    // Calculate visible range with scroll offset
    total = length(messages)
    start_idx = total - chat_height - model.scroll_offset · max
    end_idx = start_idx + chat_height · min

    visible = start_idx, end_idx · slice

    // Track rendered lines
    lines_rendered = 0
    current_y = rect.y

    // Render each visible message
    visible · each(fn(msg) {
      case current_y < rect.y + chat_height:
        true → {
          rendered = rect.x, current_y, rect.width, config · render_message_line
          lines_rendered = lines_rendered + rendered.lines_used
          current_y = current_y + rendered.lines_used
        }
        false → nil
    }

    // Render typing indicators
