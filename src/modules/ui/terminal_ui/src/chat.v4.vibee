// =============================================================================
// SCROLL INDICATOR
// =============================================================================

tool render_scroll_indicator:
  desc: "Render scroll position indicator"
  @spec "Shows offset": offset > 0 → result contains "more"

  offset: int!
  x: int!
  y: int!
  width: int!

  → {
    indicator_text = " [+to_string(offset) + " more] "
    styled = ansi."bright_black" · fg

    // Center the indicator
    padding = (width - visible_length(indicator_text · max) / 2)
    padded = repeat(" ", padding) + styled

    y, padded · print_at

    indicator_text
  }

// =============================================================================
// SCROLL MANAGEMENT
// =============================================================================

tool can_scroll_up:
  desc: "Check if can scroll up (more history above)"
  @spec "Has history": total > visible → result == true

  model.Model!
  visible_height: int!

  → {
    total = length(model.room.messages)
    max_scroll = total - visible_height · max
    model.scroll_offset < max_scroll
  }

tool can_scroll_down:
  desc: "Check if can scroll down (scrolled up from bottom)"
  @spec "At bottom": model.scroll_offset == 0 → result == false
  @spec "Scrolled up": model.scroll_offset > 0 → result == true

  model.Model!

  → model.scroll_offset > 0

tool get_scroll_percentage:
  desc: "Get current scroll position as percentage"
  @spec "At bottom": model.scroll_offset == 0 → result == 100
  @spec "Valid range": → result >= 0 && result <= 100

  model.Model!
  visible_height: int!

  → {
    total = length(model.room.messages)
    case total <= visible_height:
      true → 100
      false → {
        max_scroll = total - visible_height
        scrolled = max_scroll - model.scroll_offset
        scrolled * 100 / max_scroll
      }
  }

