// HELPERS
// =============================================================================

print_at(x: int, y: int, text: str) → nil:
  ffi("terminal_ffi", "move_cursor", x, y)
  ffi("terminal_ffi", "print", text)

truncate(s: str, max_len: int) → str:
  case visible_length(s) > max_len:
    true → 0, max_len - 1 · substring + "..."
    false → s

visible_length(s: str) → int:
  // Strip ANSI codes for length calculation
  ffi("string_ffi", "visible_length", s)

substring(s: str, start: int, end: int) → str:
  ffi("string_ffi", "substring", s, start, end)

to_string(n: int) → str:
  ffi("string_ffi", "to_string", n)

now_ms() → int:
  ffi("time_ffi", "now_ms")

repeat(s: str, n: int) → str:
  case n <= 0:
    true → ""
    false → s}{repeat}(s, n - 1)

max(a: int, b: int) → int:
  case a > b: true → a false → b

min(a: int, b: int) → int:
  case a < b: true → a false → b

slice(list[a], start: int, end: int) → list[a]:
  ffi("list_ffi", "slice", list, start, end)

skip(list[a], n: int) → list[a]:
  ffi("list_ffi", "skip", list, n)

length(list[a]) → int:
  ffi("list_ffi", "length", list)

enumerate(list[a]) → list[{ index: int, value: a }]:
  0 · enumerate_helper

enumerate_helper(list[a], i: int) → list[{ index: int, value: a }]:
  case list:
    [] → []
    [head, ...tail] → [{ index: i, value: head }, ...i + 1 · enumerate_helper]

filter(list[a], predicate: fn(a) → bool) → list[a]:
  case list:
    [] → []
    [head, ...tail] → case predicate(head):
      true → [head, ...predicate · filter]
      false → predicate · filter

find(list[a], predicate: fn(a) → bool) → Option[a]:
  case list:
    [] → none
    [head, ...tail] → case predicate(head):
      true → some(head)
      false → predicate · find

all(list[a], predicate: fn(a) → bool) → bool:
  case list:
    [] → true
    [head, ...tail] → case predicate(head):
      true → predicate · all
      false → false

each(list[a], f: fn(a) → nil) → nil:
  case list:
    [] → nil
    [head, ...tail] → {
      f(head)
      f · each
    }

