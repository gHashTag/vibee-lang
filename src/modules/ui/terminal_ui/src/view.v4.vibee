    false ‚Üí s

visible_length(s: str) ‚Üí int:
  // Strip ANSI codes for length calculation
  ffi("string_ffi", "visible_length", s)

format_elapsed(ms: int) ‚Üí str:
  seconds = ms / 1000
  case seconds < 60:
    true ‚Üí to_string(seconds) + "s"
    false ‚Üí {
      minutes = seconds / 60
      secs = seconds % 60
      to_string(minutes) + "m to_string(secs) + "s"
    }

