render_chat(model: Model, rect: Rect) ‚Üí nil:
  // Leave room for input at bottom
  chat_height = rect.height - 2
  messages = model.room.messages

  // Get visible messages (with scroll offset)
  total = length(messages)
  start_idx = total - chat_height - model.scroll_offset ¬∑ max
  end_idx = start_idx + chat_height ¬∑ min

  visible = start_idx, end_idx ¬∑ slice

  // Render messages
  visible ¬∑ enumerate ¬∑ each(fn(item) {
    y = rect.y + item.index
    msg = item.value

    case y < rect.y + chat_height:
      true ‚Üí rect.x, y, rect.width ¬∑ render_message
      false ‚Üí nil
  }

  // Scroll indicator
  case model.scroll_offset > 0:
    true ‚Üí {
      indicator = ansi.fg(" [+{to_string}(model.scroll_offset) + " more] ", "bright_black")
      print_at(rect.x, rect.y + chat_height, indicator)
    }
    false ‚Üí nil

