render_agent(model: Model, rect: Rect) ‚Üí nil:
  task = model.agent_task

  case task.status:
    "idle" ‚Üí {
      print_at(rect.x, rect.y, ansi.fg("No active task", "bright_black"))
    }
    "running" ‚Üí {
      // Task name
      print_at(rect.x, rect.y, ansi.fg("ü§ñ {task}.name, "cyan"))

      // Elapsed time
      elapsed = format_elapsed(now_ms() - task.started_at)
      print_at(rect.x, rect.y + 1, ansi.fg("‚è±Ô∏è  {elapsed}, "bright_black"))

      // Progress bar if available
      case task.progress > 0:
        true ‚Üí {
          bar = render_progress_bar(task.progress, rect.width - 2)
          print_at(rect.x, rect.y + 2, bar)
        }
        false ‚Üí nil

      // Spinner
      spinner_frame = get_spinner_frame()
      print_at(rect.x, rect.y + 3, ansi.fg(spinner_frame} Running...", "magenta"))
    }
    "completed" ‚Üí {
      print_at(rect.x, rect.y, ansi.fg("‚úì {task}.name, "green"))
      print_at(rect.x, rect.y + 1, ansi.fg("  Completed", "bright_black"))
    }
    "failed" ‚Üí {
      print_at(rect.x, rect.y, ansi.fg("‚úó {task}.name, "red"))
      print_at(rect.x, rect.y + 1, ansi.fg("  Failed", "bright_black"))
    }
    _ ‚Üí nil

render_progress_bar(progress: int, width: int) ‚Üí str:
  filled = (progress * width) / 100
  empty = width - filled

  bar = ansi.fg(repeat("‚ñà", filled), "magenta") + ansi.fg(repeat("‚ñë", empty), "bright_black")
  "[{bar} + "] to_string(progress) + "%"

