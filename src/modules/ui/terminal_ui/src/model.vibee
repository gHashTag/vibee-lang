tool set_connected:
  desc: "Set connection status"
  @Updates connected: → result.connected == connected

  model: Model!
  connected: bool!

  → { ...model, connected }

tool add_message:
  desc: "Add message to room"
  @Adds message: msg != _ → len(result.room.messages) > len(model.room.messages)

  model: Model!
  msg: Message!

  → {
    new_messages = [...model.room.messages, msg]
    new_room = { ...model.room, messages: new_messages }
    {model..., room: new_room }
  }

