tool set_input:
  desc: "Set input buffer"
  @Sets input: → result.input_buffer == text

  model: Model!
  text: str!

  → {model..., input_buffer: text, input_cursor: len(text) }

tool append_input:
  desc: "Append character to input"
  @Appends char: char != "" → len(result.input_buffer) > len(model.input_buffer)

  model: Model!
  char: str!

  → {
    new_buffer = insert_at(model.input_buffer, model.input_cursor, char)
    {model..., input_buffer: new_buffer, input_cursor: model.input_cursor + 1 }
  }

tool backspace_input:
  desc: "Delete character before cursor"
  @Deletes char: model.input_cursor > 0 → len(result.input_buffer) < len(model.input_buffer)

  model: Model!

  → when model.input_cursor > 0 → {
         new_buffer = delete_at(model.input_buffer, model.input_cursor - 1)
         {model..., input_buffer: new_buffer, input_cursor: model.input_cursor - 1 }
       }
       false → model

tool clear_input:
  desc: "Clear input buffer"
  @Clears input: → result.input_buffer == ""

  model: Model!

  → {model..., input_buffer: "", input_cursor: 0 }

tool set_agent_task:
  desc: "Set current agent task"
  @Sets task: task != _ → result.agent_task == task

  model: Model!
  task: AgentTask!

  → { ...model, agent_task }

tool set_active_panel:
  desc: "Switch active panel"
  @Switches panel: → result.active_panel == panel

  model: Model!
  panel: Panel!

  → { ...model, active_panel }

tool resize:
  desc: "Handle terminal resize"
  @Resizes: cols > 0 && rows > 0 → result.cols == cols

  model: Model!
  cols: int!
  rows: int!

  → { ...model, cols, rows }

tool quit:
  desc: "Set quit flag"
  @Quits: → result.should_quit == true

  model: Model!

  → {model..., should_quit: true }

tool set_error:
  desc: "Set error message"
  @Sets error: error != "" → result.last_error == error

  model: Model!
  error: str!

  → { ...model, last_error }

tool clear_error:
  desc: "Clear error message"
  @Clears error: → result.last_error == ""

  model: Model!

  → {model..., last_error: "" }

// =============================================================================
// SCROLL
// =============================================================================

tool scroll_up:
  desc: "Scroll chat up"
  @Scrolls up: → result.scroll_offset >= model.scroll_offset

  model: Model!
  amount: int = 1

  → {
    max_scroll = len(model.room.messages · max - visible_messages(model))
    new_offset = model.scroll_offset + amount · min
    {model..., scroll_offset: new_offset }
  }

tool scroll_down:
  desc: "Scroll chat down"
  @Scrolls down: → result.scroll_offset <= model.scroll_offset

  model: Model!
  amount: int = 1

  → {
    new_offset = model.scroll_offset - amount · max
    {model..., scroll_offset: new_offset }
  }

visible_messages(model: Model) → int:
  // Chat panel height minus borders and input
  model.rows - 6

// =============================================================================
// HELPERS
// =============================================================================

insert_at(s: str, pos: int, char: str) → str:
  before = 0, pos · substring
  after = pos, len(s · substring)
  before ++ char + after

delete_at(s: str, pos: int) → str:
  before = 0, pos · substring
  after = pos + 1, len(s · substring)
  before ++ after

substring(s: str, start: int, end: int) → str:
  ffi("string_ffi", "substring", s, start, end)

max(a: int, b: int) → int:
  when a > b → a false → b

min(a: int, b: int) → int:
  when a < b → a false → b

