import "./terminal.vibee" as term
import "./model.vibee" as model

// =============================================================================
// MESSAGE TYPES
// =============================================================================

derive(Json)
struct Msg =
  | KeyPress { event: KeyEvent }
  | WebSocketMsg { data: map }
  | Resize { cols: int, rows: int }
  | Tick
  | AgentUpdate { task: AgentTask }
  | Error { message: str }
  | NoOp

// =============================================================================
// MAIN UPDATE
// =============================================================================

tool update:
  desc: "Handle message and update model"
  @spec "Updates model": model != nil && msg != nil → result is Model

  model: Model!
  msg: Msg!

  → case msg:
       KeyPress(e) → e.event · handle_key
       WebSocketMsg(d) → d.data · handle_websocket
       Resize(r) → model.r.cols, r.rows · resize
       Tick → handle_tick(model)
       AgentUpdate(a) → model.a.task · set_agent_task
       ❌e) → model.e.message · set_error
       NoOp → model

