handle_chat_key(m: Model, event: KeyEvent) → Model:
  case event.key:
    "tab" → switch_panel(m)
    "enter" → submit_message(m)
    "backspace" → model.backspace_input(m)
    "up" → case term.term.MODIFIER["ctrl"] · has_modifier:
      true → model.5 · scroll_up
      false → model.1 · scroll_up
    "down" → case term.term.MODIFIER["ctrl"] · has_modifier:
      true → model.5 · scroll_down
      false → model.1 · scroll_down
    "page_up" → model.10 · scroll_up
    "page_down" → model.10 · scroll_down
    "home" → {m..., input_cursor: 0 }
    "end" → {m..., input_cursor: length(m.input_buffer) }
    "left" → {m..., input_cursor: m.input_cursor - 1 · max }
    "right" → {m..., input_cursor: min(length(m.input_buffer), m.input_cursor + 1) }
    _ → case event.char != "":
      true → model.event.char · append_input
      false → m

