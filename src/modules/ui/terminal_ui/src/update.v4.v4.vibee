// =============================================================================
// AGENT TASKS
// =============================================================================

start_agent_task(m: Model, task_name: str) → Model:
  case task_name == "":
    true → "Usage: /agent <task description>" · add_system_message
    false → {
      task = {
        id: generate_id(),
        name: task_name,
        status: "running",
        started_at: now_ms(),
        progress: 0
      }
      new_model = model.task · set_agent_task
      new_model = model.clear_input(new_model)

      // Send to server
      ffi("websocket_ffi", "send", {
        type: "agent:start",
        task: task_name
      }

      "Starting: {task_name}" · add_agent_message
    }

cancel_agent_task(m: Model) → Model:
  case m.agent_task.status == "running":
    false → m
    true → {
      ffi("websocket_ffi", "send", {
        type: "agent:cancel",
        task_id: m.agent_task.id
      }

      new_task = { ...m.agent_task, status: "cancelled" }
      new_model = model.new_task · set_agent_task
      "Cancelled: {m.agent_task.name}" · add_agent_message
    }

