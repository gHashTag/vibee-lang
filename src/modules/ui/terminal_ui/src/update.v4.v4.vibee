      new_model = model.task · set_agent_task
      "Started: {data.name}" · add_agent_message
    }

    "agent:progress" → {
      new_task = { ...m.agent_task, progress: data.progress }
      model.new_task · set_agent_task
    }

    "agent:completed" → {
      new_task = { ...m.agent_task, status: "completed" }
      new_model = model.new_task · set_agent_task
      "Completed: {data.result}" · add_agent_message
    }

    "agent:failed" → {
      new_task = { ...m.agent_task, status: "failed" }
      new_model = model.new_task · set_agent_task
      "Failed: {data.error}" · add_agent_message
    }

    "error" → {
      model.data.message · set_error
    }

    _ → m

