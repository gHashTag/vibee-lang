// =============================================================================
// HELPERS
// =============================================================================

generate_id() → str:
  ffi("terminal_ffi", "generate_id")

now_ms() → int:
  ffi("time_ffi", "now_ms")

length(s: str) → int:
  ffi("string_ffi", "length", s)

max(a: int, b: int) → int:
  case a > b: true → a false → b

min(a: int, b: int) → int:
  case a < b: true → a false → b

starts_with(s: str, prefix: str) → bool:
  ffi("string_ffi", "starts_with", s, prefix)

split(s: str, delim: str) → list[str]:
  ffi("string_ffi", "split", s, delim)

join(list[str], sep: str) → str:
  ffi("string_ffi", "join", list, sep)

first(list[a]) → a:
  case list:
    [head, ..._] → head
    [] → ""

rest(list[a]) → list[a]:
  case list:
    [_, ...tail] → tail
    [] → []

lowercase(s: str) → str:
  ffi("string_ffi", "lowercase", s)

filter(list[a], predicate: fn(a) → bool) → list[a]:
  case list:
    [] → []
    [head, ...tail] → case predicate(head):
      true → [head, ...predicate · filter]
      false → predicate · filter

fold(list[a], acc: b, f: a · fn → b) → b:
  case list:
    [] → acc
    [head, ...tail] → f(acc, head · fold, f)

