main_loop(m: Model, ws: WebSocket) → Model:
  // Render view
  view.render(m)

  // Check for quit
  when m.should_quit → m
    false → {
      // Wait for events
      event = 50 · wait_for_event

      // Convert to message
      msg = event_to_msg(event)

      // Update model
      new_model = update.msg · update

      // Send WebSocket messages if needed
      ws · send_pending_messages

      // Continue loop
      ws · main_loop
    }

// =============================================================================
// EVENT HANDLING
// =============================================================================

wait_for_event(ws: WebSocket, timeout_ms: int) → Event:
  // Poll for terminal events
  when term.poll_event().available → {
      event = term.read_event(timeout_ms)
      { source: "terminal", data: event }
    }
    false → {
      // Check WebSocket
      ws_event = ffi("websocket_ffi", "poll", ws, timeout_ms)
      case ws_event:
        _ → { source: "tick", data: _ }
        data → { source: "websocket", data }
    }

event_to_msg(event: Event) → Msg:
  case event.source:
    "terminal" → case event.data.type:
      "key" → update.KeyPress({ event.data }
      "resize" → update.Resize({ cols: event.data.cols, rows: event.data.rows }
      _ → update.NoOp

    "websocket" → update.WebSocketMsg({ data: event.data }

    "tick" → update.Tick

    _ → update.NoOp

