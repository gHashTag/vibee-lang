// =============================================================================
// TERMINAL SETUP
// =============================================================================

init_terminal() → { ok: bool, error: str }:
  try:
    term.enable_raw_mode()
    term.enter_alt_screen()
    term.hide_cursor()
    term.clear_screen()
    { ok: true, error: "" }
  catch e:
    { ok: false, error: "Failed to initialize terminal: $(e) }

cleanup_terminal() → _:
  term.show_cursor()
  term.leave_alt_screen()
  term.disable_raw_mode()

