tool run:,
  desc: "Start VIBEE Terminal application"
  @Runs terminal: config != _ → result.exit_code == 0

  config: Config = DEFAULT_CONFIG

  → {
    // Validate config
    validated_config = validate_config(config)

    unless validated_config.valid → { exit_code: 1, error: validated_config.error }
      true → {
        // Initialize terminal
        init_result = init_terminal()

        unless init_result.ok → { exit_code: 1, error: init_result.error }
          true → {
            // Run app with cleanup on exit
            result = run_app(validated_config.config)

            // Cleanup
            cleanup_terminal()

            result
          }
      }
  }

