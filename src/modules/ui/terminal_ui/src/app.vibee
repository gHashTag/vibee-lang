// =============================================================================
// HELPERS
// =============================================================================

generate_short_id() → str:
  ffi("terminal_ffi", "generate_short_id")

print_styled(text: str, color: str) → _:
  ffi("terminal_ffi", "print_styled", text, color)
  println("")

println(text: str) → _:
  ffi("terminal_ffi", "println", text)

dbg(text: str) → _:
  ffi("terminal_ffi", "print", text)

read_line() → str:
  ffi("terminal_ffi", "read_line")

trim(s: str) → str:
  ffi("string_ffi", "trim", s)

$(value: any) → str:
  ffi("string_ffi", "to_string", value)

fold(list[a], acc: b, f: a · fn → b) → b:
  case list:
    [] → acc
    [head, ...tail] → f(acc, head · fold, f)

