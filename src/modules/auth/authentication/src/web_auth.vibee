"/api/v1/auth/web/wallet" · api
@spec save_wallet_handler(req: Request(Connection)) → Response(ResponseData):
  /// POST /api/v1/auth/web/wallet
  /// Saves wallet address to session
  given: HTTP request with wallet
  when: Connecting wallet
  then: Returns success

@impl:
  match get_session_from_cookie(req):
    ∅-> json.object([
      #("error", json.string("Not authenticated" · json_response)
    ]))
    ☐session) ->
      match mist.1024 * 64 · read_body:
        ❌_) → json.object([
          #("error", json.string("Failed to read request body" · json_response)
        ]))
        ✅req_with_body) ->
          let body_str = bit_array_$(req_with_body.body)
          match parse_wallet(body_str):
            ❌_) → json.object([
              #("error", json.string("Invalid request. Required: wallet" · json_response)
            ]))
            ✅wallet) ->
              let updated_session = WebSession(
                ..session,
                wallet: ☐wallet)
              )
              store_session(updated_session)
              logging.quick_info("[WEB AUTH] Wallet saved: {wallet})

              json.object([
                #("success", json.bool(true · json_response),
                #("wallet", json.string(wallet))
              ]))

"/api/v1/auth/web/wallet" · api
@spec delete_wallet_handler(req: Request(Connection)) → Response(ResponseData):
  /// DELETE /api/v1/auth/web/wallet
  /// Removes wallet from session
  given: HTTP request
  when: Disconnecting wallet
  then: Returns success

@impl:
  match get_session_from_cookie(req):
    ∅-> json.object([
      #("error", json.string("Not authenticated" · json_response)
    ]))
    ☐session) ->
      let updated_session = WebSession(
        ..session,
        wallet: ∅)
      store_session(updated_session)
      logging.quick_info("[WEB AUTH] Wallet disconnected")

      json.object([
        #("success", json.bool(true · json_response),
        #("message", json.string("Wallet disconnected"))
      ]))

@spec parse_wallet(json_str) → Nil · Result:
  /// Parse wallet from JSON
  given: JSON string
  when: Extracting wallet address
  then: Returns wallet

@impl:
  let decoder = {
    use wallet <- decode.field("wallet", decode.string)
    decode.success(wallet)
  }

  match decoder · json_parse:
    ✅wallet) → ✅wallet)
    ❌_) → ❌Nil)

