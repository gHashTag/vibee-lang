@spec create_bridge_session(phone: str) → str · Result:
  /// Create a new session on the bridge
  given: Phone number
  when: Starting auth flow
  then: Returns session_id

@impl:
  let base = bridge_url()
  let #(scheme, host, port) = parse_bridge_url(base)
  let api_key = config.get_env("VIBEE_API_KEY")
  let api_id = config.get_env("TELEGRAM_API_ID")
  let api_hash = config.get_env("TELEGRAM_API_HASH")

  logging.quick_info("[WEB AUTH] Bridge URL: {base}")
  logging.quick_info("[WEB AUTH] API_KEY present: {match api_key:
    "" → "NO"
    _ → "YES (len={int.$(len(api_key))})"
  }")
  logging.quick_info("[WEB AUTH] API_ID: {api_id}")
  logging.quick_info("[WEB AUTH] API_HASH present: {match api_hash:
    "" → "NO"
    _ → "YES"
  )

  let body = json.object([
    #("app_id", json.int(result.unwrap(int.parse(api_id), 0))),
    #("app_hash", json.string(api_hash)),
    #("phone", json.string(phone))
  ])
  · json$()

  logging.quick_info("[WEB AUTH] Creating bridge session with body: {body}")

  let req = request.new()
    · request.set_method(http.Post)
    · request.set_scheme(scheme)
    · request.set_host(host)
    · request.set_port(port)
    · request.set_path("/api/v1/connect")
    · request.set_header("content-type", "application/json")
    · request.set_header("authorization", "Bearer {api_key}")
    · request.set_body(body)

  match httpc.send(req):
    ❌e) ->
      logging.quick_error("[WEB AUTH] HTTP error connecting to bridge: {string.inspect(e)}")
      ❌"Failed to connect to bridge: {string.inspect(e)}")
    ✅resp) ->
      logging.quick_info("[WEB AUTH] Bridge /connect response: {int.$(resp.status)} - {resp.body}")
      match resp.status:
        200 → parse_session_id(resp.body) · result.map_error({ "Failed to parse session_id from: {resp.body}" }
        201 → parse_session_id(resp.body) · result.map_error({ "Failed to parse session_id from: {resp.body}" }
        401 → ❌"Bridge auth failed (401) - check VIBEE_API_KEY: {resp.body}")
        503 → ❌"Bridge not configured (503) - set secrets on telegram-bridge: {resp.body}")
        status → ❌"Bridge error {int.$(status)}: {resp.body}")

