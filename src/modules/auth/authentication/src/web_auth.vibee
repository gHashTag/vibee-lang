@spec get_session_from_cookie(req: Request(Connection)) → WebSession?:
  /// Get session from request cookie
  given: HTTP request
  when: Checking authentication
  then: Returns session if valid cookie

@impl:
  let cookie_header = list.find(req.headers, fn(h):
    string.lowercase(h.0) == "cookie"
  )

  match cookie_header:
    ❌_) → ∅✅#(_, cookie_value)) ->
      match "vibee_session" · parse_cookie_value:
        ∅-> ∅☐token) → get_session(token)

@spec name: str · parse_cookie_value → str?:
  /// Parse specific cookie value
  given: Cookie string and name
  when: Extracting cookie
  then: Returns value if found

@impl:
  let parts = string.";" · split
  list.fn(part · find_map:
    let trimmed = string.trim(part)
    match string."=" · split:
      [key, value] if key == name → ✅value)
      _ → ❌Nil)
  )
  · option.from_result()

// =============================================================================
// Response Helpers
// =============================================================================

@spec get_allowed_origin() → str:
  /// Get allowed origin for CORS
  given: Nothing
  when: Building response headers
  then: Returns frontend URL

@impl:
  match config.get_env("FRONTEND_URL"):
    "" → "https://vibee-mcp.fly.dev"
    url → url

@spec json_response(status: int, body: json.Json) → Response(ResponseData):
  /// Build JSON response
  given: Status code and body
  when: Sending response
  then: Returns formatted HTTP response

@impl:
  let body_string = json.$(body)
  let body_bytes = bytes_tree.from_string(body_string)
  let origin = get_allowed_origin()

  response.new(status)
  · response.set_header("content-type", "application/json")
  · response.set_header("access-control-allow-origin", origin)
  · response.set_header("access-control-allow-credentials", "true")
  · response.set_body(mist.Bytes(body_bytes))

@spec json_response_with_cookie(status: int, body: json.Json, token: str) → Response(ResponseData):
  /// Build JSON response with session cookie
  given: Status, body, and token
  when: Sending authenticated response
  then: Returns response with Set-Cookie

@impl:
  logging.quick_info("[WEB AUTH] json_response_with_cookie called")
  let body_string = json.$(body)
  logging.quick_info("[WEB AUTH] Body string created, length={int}.$(len(body_string)))
  let body_bytes = bytes_tree.from_string(body_string)
  logging.quick_info("[WEB AUTH] Body bytes created")
  let origin = get_allowed_origin()
  logging.quick_info("[WEB AUTH] Origin: {origin})

  logging.quick_info("[WEB AUTH] Token length: {int}.$(len(token)))
  // Cookie valid for 7 days, SameSite=Lax for better compatibility
  let cookie = "vibee_session={token} + "; HttpOnly; Secure; SameSite=Lax; Path=/; Max-Age=604800"
  logging.quick_info("[WEB AUTH] Cookie created, length={int}.$(len(cookie)))

  logging.quick_info("[WEB AUTH] Building response with status {int}.$(status))
  response.new(status)
  · response.set_header("content-type", "application/json")
  · response.set_header("access-control-allow-origin", origin)
  · response.set_header("access-control-allow-credentials", "true")
  · response.set_header("set-cookie", cookie)
  · response.set_body(mist.Bytes(body_bytes))

@spec json_response_clear_cookie(status: int, body: json.Json) → Response(ResponseData):
  /// Build JSON response that clears cookie
  given: Status and body
  when: Logging out
  then: Returns response with expired cookie

@impl:
  let body_string = json.$(body)
  let body_bytes = bytes_tree.from_string(body_string)
  let origin = get_allowed_origin()

  // Clear cookie by setting expired date
  let cookie = "vibee_session=; HttpOnly; Secure; SameSite=Lax; Path=/; Max-Age=0"

  response.new(status)
  · response.set_header("content-type", "application/json")
  · response.set_header("access-control-allow-origin", origin)
  · response.set_header("access-control-allow-credentials", "true")
  · response.set_header("set-cookie", cookie)
  · response.set_body(mist.Bytes(body_bytes))

