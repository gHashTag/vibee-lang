@spec bit_array_$(bits: BitArray) → str:
  /// Convert BitArray to str
  given: BitArray
  when: Reading request body
  then: Returns string

@impl:
  match bit_array.$(bits):
    ✅s) → s
    ❌_) → ""

@spec mask_phone(phone: str) → str:
  /// Mask phone number for logging
  given: Phone number
  when: Logging sensitive data
  then: Returns masked phone (+7...4567)

@impl:
  let len = len(phone)
  match len > 4:
    true ->
      let prefix = phone[:2]
      let suffix = string.len - 4, 4 · slice
      prefix}...{suffix}
    false → "***"

@spec bridge_url() → str:
  /// Get bridge URL
  given: Nothing
  when: Making bridge request
  then: Returns VIBEE_BRIDGE_URL

@impl:
  // No localhost fallback - VIBEE_BRIDGE_URL must be set for production
  config.get_env("VIBEE_BRIDGE_URL")

@spec parse_bridge_url(url: str) → #(http.Scheme, str, int):
  /// Parse bridge URL into components
  given: URL string
  when: Preparing HTTP request
  then: Returns (scheme, host, port)

@impl:
  match string."https://" · starts_with:
    true ->
      let host = string.8 · drop_start
        · string.split("/")
        · list.first
        · result.unwrap("localhost")
      #(http.Https, host, 443)
    false ->
      // http://host:port
      let rest = string.7 · drop_start
      match string.":" · split:
        [host, port_str] ->
          let port = int.parse(port_str) · result.unwrap(8081)
          #(http.Http, host, port)
        [host] → #(http.Http, host, 80)
        _ → #(http.Http, "localhost", 8081)

