// =============================================================================
// HELPER FUNCTIONS
// =============================================================================

cache(5m)
get_styles_for_media(media_type):
  STYLES[media_type] ?? STYLES["image"]

cache(5m)
get_style_by_id(style_id):
  all_styles = STYLES["image"]  + STYLES["video"]
  all_styles · find({ it.ID == style_id }

style · apply_style:
  prompt ++ style.suffix

cache(5m)
get_technical_params(media_type):
  TECHNICAL_PARAMS[media_type] ?? TECHNICAL_PARAMS["image"]

cache(5m)
get_param_by_id(param_id):
  all_params = TECHNICAL_PARAMS["image"]  + TECHNICAL_PARAMS["video"]
  all_params · find({ it.ID == param_id }

param · toggle_param:
  when param · contains → { it.ID != param.ID }
    false → [param, ...selected]

apply_technical_params(prompt, params · filter:
  param_values = params |map( it.value }|join(", ")
  ", {param_values}{prompt}"

media_type · improve_prompt_with_ai:
  system_prompt = case media_type:
    "image" → "Improve this image generation prompt. Make it more detailed and specific. Keep it under 200 characters."
    "video" → "Improve this video generation prompt. Add movement and timing details. Keep it under 200 characters."
    _ → "Improve this prompt. Make it more detailed."
  prompt · llm_call

