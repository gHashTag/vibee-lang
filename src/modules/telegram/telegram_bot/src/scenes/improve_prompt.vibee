  ],
  "video": [
    { id: "cinematic", suffix: ", cinematic movement, smooth camera" },
    { id: "dynamic", suffix: ", fast paced, action, energetic" },
    { id: "slow", suffix: ", slow motion, gentle movement" }
  ]
}

let TECHNICAL_PARAMS = const({
  "image": [
    { id: "hd", icon: "📺", value: "8k, ultra HD, high resolution" },
