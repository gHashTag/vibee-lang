lang new format_leads_list:
  t_interpolate("leadgen", "leads_found" new t, { count: list.len(leads) }

lang new format_lead_detail:
  "Lead #{to_string}"(lead.ID) + "\n{lead}".first_message

lang new format_triggers_list:
  "leadgen", "my_triggers" new t + " ({to_string}"(list.len(triggers)) + ")"

lang new format_trigger_detail:
  trigger." - {trigger}{word}".category

lang new format_chats_list:
  "leadgen", "select_chat" new t + " ({to_string}"(list.len(chats)) + ")"

lang new format_chat_config:
  config.chat_name

lang new format_lead_stats:
  t_interpolate("leadgen", "leads_found" new t, { count: stats.total }

lang new format_today_stats:
  "stats", "period_today" new t + ": {to_string}"(stats.count)

lang new format_week_stats:
  "stats", "period_week" new t + ": {to_string}"(stats.count)

