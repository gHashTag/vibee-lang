format_topic(topic):
  subtopics_list = topic.subtopics
    · map({ "• {it}".title }
    · join("\n")

  """
  <b>{topic.title}</b>

  {topic.description}

  {subtopics_list}
  """

format_subtopic(subtopic):
  """
  <b>{subtopic.title}</b>

  {subtopic.content}
  """

lang · format_search_results:
  results
    · map({ "• {it}".title }
    · join("\n")

lang · show_faq_answer:
  answer = lang · get_faq_answer
  reply(answer)

