// =============================================================================
// SUNO AI TOOLS (via KIE.AI)
// =============================================================================

circuit_breaker(threshold: 5, timeout: 30s, half_open_requests: 2)
@http_tool
tool suno_generate_music:,
  desc: "Generate music using Suno AI v4.5 via KIE.AI"
  @Music generated: prompt valid → generate() → result.audio_url != null

  POST "https://api.kie.ai/api/v1/suno/generate"
  @auth Bearer(KIE_AI_API_KEY)
  retry(attempts: 3, delay: 5000)
  timeout(180000)

  prompt: str!            body("description")
  duration: int!          @body
  instrumental: bool!     @body
  model: str = "suno-v4.5"  @body
  lyrics: str?            @body

  returns:
    task_id: str!
    status: str!

@http_tool
tool suno_check_status:,
  desc: "Check Suno music generation status"

  GET "https://api.kie.ai/api/v1/suno/{task_id}"
  @auth Bearer(KIE_AI_API_KEY)

  task_id: str!  @path

  returns:
    status: str!
    audio_url: str?
    duration: float?
    error: str?

// =============================================================================
// HELPER FUNCTIONS
// =============================================================================

lang · format_duration_message:
  type_label = when instrumental → "music", "instrumental" · t
    false → "music", "with_vocals" · t

  "music", "title" · t + "\n\n{type_label}"

