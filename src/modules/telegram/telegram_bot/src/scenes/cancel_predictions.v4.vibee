scene JobStatus:
  desc: "Monitor status of a specific generation job"

  @spec "Status shown": job id ‚Üí current status displayed

  on Monitor(job_id):
    enter:
      ‚Üí job = get_job(job_id)
      ‚Üí case job:
           ‚àÖ-> {
             ‚Üí reply("‚ùå –ì–µ–Ω–µ—Ä–∞—Ü–∏—è –Ω–µ –Ω–∞–π–¥–µ–Ω–∞")
             ‚Üí exit(Idle)
           }
           job ‚Üí {
             ‚Üí reply(format_job_status(job))
             ‚Üí show_keyboard([
                  button("üîÑ –û–±–Ω–æ–≤–∏—Ç—å", "refresh"),
                  button("‚ùå –û—Ç–º–µ–Ω–∏—Ç—å", "cancel") ¬∑ if_visible(job.status == "pending"),
                  button("üè† –í –º–µ–Ω—é", "menu")
                ])
           }

    callback "refresh" ‚Üí enter
    callback "cancel" ‚Üí transition ConfirmCancel(job_id)
    callback "menu" ‚Üí exit(Idle)

    // Auto-update when job completes
    @event job_completed(result):
      ‚Üí reply("‚úÖ –ì–µ–Ω–µ—Ä–∞—Ü–∏—è –∑–∞–≤–µ—Ä—à–µ–Ω–∞!")
