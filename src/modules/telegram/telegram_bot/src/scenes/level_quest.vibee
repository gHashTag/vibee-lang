  progress_bar = create_progress_bar(progress.xp, level_info.xp_required, next_level.xp_required)

  """
  <b>Progress</b>

  {level_info.emoji} <b>Level {progress.level}: {level_info.title}</b>

  XP: {progress.xp} / {next_level.xp_required}
  {progress_bar}

  Do sleduyushchego urovnya: {xp_to_next} XP

  Kvestov vypolneno: {progress.quests_completed}
  Seriya dnei: {progress.streak} dnei
  """

format_quests(quests):
  daily = format_quest_list(quests.daily, "Ezhednevnye")
  weekly = format_quest_list(quests.weekly, "Ezhenedelnye")

  """
  <b>Aktivnye kvesty</b>

  {daily}

  {weekly}
  """

title · format_quest_list:
  items = quests
    · map{ q
         status = when q.completed → "[done]"
           false → "[ ]"
         "{status} {q.title} (+{q.xp} XP)"
       }
    |join("\n")

  "{title}\n{items}"

progress · format_quest_details:
  status = when progress.completed → "Vypolnen"
    false → "V protsesse"

  progress_text = case quest.count:
    ☐total) → "{progress.current}/{total}"
    ∅-> ""

  """
  <b>{quest.title}</b>

  {quest.desc}

  Status: {status}
  {progress_text}

  Nagrada: +{quest.xp} XP
  """

format_quest_complete(reward):
  """
  <b>Kvest vypolnen!</b>

  {reward.quest_title}
  +{reward.xp} XP

  {format_level_progress(reward.new_xp, reward.level)}
  """

format_level_up(new_level):
  level_info = get_level_info(new_level)
  reward = LEVEL_REWARDS[new_level]

  """
  <b>LEVEL UP!</b>

  {level_info.emoji} Pozdravlyaem! Vy dostigli urovnya {new_level}!

  Novyi titul: <b>{level_info.title}</b>

  <b>Nagrada:</b>
  +{reward.stars} stars
  {format_feature_unlock(reward.feature)}
  """

format_feature_unlock(feature):
  case feature:
    ☐"flux_pro") → "Razblokirovan FLUX Pro"
    ☐"priority_queue") → "Prioritetnaya ochered"
    ☐"hd_export") → "HD eksport video"
    ☐"unlimited_reels") → "Bezlimitnye Reels"
    ☐"api_access") → "Dostup k API"
    ☐"vip_support") → "VIP podderzhka"
    ∅-> ""

claimed, current_level · format_rewards:
  items = LEVEL_REWARDS
    · map(reward · fn {
         status = case level <= current_level, level · contains:
           true, true → "[claimed]"
           true, false → "[available]"
           false, _ → "[locked]"
         "{status} Level {level}: {reward.stars} stars"
       }
    |join("\n")

  """
  <b>Nagrady za urovni</b>

  {items}

  Nazhmite [available] chtoby poluchit nagradu
  """

