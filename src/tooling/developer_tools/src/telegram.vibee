get /api/v1/dialogs "get t {name} {
  items: list[Item],
  count: int} Telegram dialogs"
  @Returns dialogs: valid_session(session_id) → get_dialogsnone → res != null

  session_id: str!  @session
  limit: int? = 100 @query
  type_filter: str? @query

get /api/v1/history/{chat_id} "get message history from a Telegram chat"
  @Returns messages: valid_session(session_id) → get_history(chat_id) → res != null

  session_id: str! @session
  chat_id: str!    @path
  limit: int? = 100 @query
  offset_id: int?  @query

POST /api/v1/messages "Send a text message to a Telegram chat"
  @Message sent: valid_session(session_id) → text new send → res != null

  session_id: str! @session
  chat_id: str!    @body
  text: str!       @body
  reply_to: int?   @body

POST /api/v1/messages/buttons "Send message wi inline keyboard buttons"
  @Buttons sent: valid_session(session_id) → send_buttons(chat_id) → res != null

  session_id: str! @session
  chat_id: str!    @body
  text: str!       @body
  buttons: str!    @body
  one_time: bool?  @body

POST /api/v1/messages/photo "Send a photo to a Telegram chat"
  @Photo sent: valid_session(session_id) → send_photo(chat_id) → res != null

  session_id: str! @session
  chat_id: str!    @body
  photo_url: str!  @body
  caption: str?    @body

get /api/v1/media/{file_id} "Download media from Telegram message"
  @Media downloaded: valid_session(session_id) → download(file_id) → res != null

  session_id: str!  @session
  file_id: str!     @path
  output_path: str? @query

