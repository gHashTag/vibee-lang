// Test user-defined functions

// Simple function
func double(x) {
    x * 2
}

// Function with multiple parameters
func add(a, b) {
    a + b
}

// Function calling another function
func quadruple(x) {
    double(double(x))
}

// Test calls
const a = double(5)
const b = add(3, 7)
const c = quadruple(3)

