  # ═══════════════════════════════════════════════════════════════════════════
  # ⲖⲀⲚⲄⲨⲀⲄⲈ ⲂⲀⲔⲔⲈⲚⲆⲤ
  # ═══════════════════════════════════════════════════════════════════════════
  
  ⲃⲁⲕⲕⲉⲛⲇⲥ:
    ⲍⲓⲅ:
      ⲉⲝⲧⲉⲛⲥⲓⲟⲛ: ".zig"
      ⲥⲧⲣⲩⲕⲧ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        pub const {name} = struct {{
        {fields}
        {methods}
        }};
      ⲫⲩⲛⲕ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        pub fn {name}({params}) {return_type} {{
        {body}
        }}
      ⲧⲉⲥⲧ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        test "{name}" {{
        {body}
        }}
        
    ⲣⲩⲥⲧ:
      ⲉⲝⲧⲉⲛⲥⲓⲟⲛ: ".rs"
      ⲥⲧⲣⲩⲕⲧ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        pub struct {name} {{
        {fields}
        }}
        impl {name} {{
        {methods}
        }}
      ⲫⲩⲛⲕ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        pub fn {name}({params}) -> {return_type} {{
        {body}
        }}
      ⲧⲉⲥⲧ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        #[test]
        fn {name}() {{
        {body}
        }}
        
    ⲅⲟ:
      ⲉⲝⲧⲉⲛⲥⲓⲟⲛ: ".go"
      ⲥⲧⲣⲩⲕⲧ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        type {name} struct {{
        {fields}
        }}
      ⲫⲩⲛⲕ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        func {name}({params}) {return_type} {{
        {body}
        }}
      ⲧⲉⲥⲧ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        func Test{name}(t *testing.T) {{
        {body}
        }}
        
    ⲡⲩⲧⲏⲟⲛ:
      ⲉⲝⲧⲉⲛⲥⲓⲟⲛ: ".py"
      ⲥⲧⲣⲩⲕⲧ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        @dataclass
        class {name}:
        {fields}
        {methods}
      ⲫⲩⲛⲕ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        def {name}({params}) -> {return_type}:
        {body}
      ⲧⲉⲥⲧ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        def test_{name}():
        {body}
        
    ⲕ:
      ⲉⲝⲧⲉⲛⲥⲓⲟⲛ: ".c"
      ⲥⲧⲣⲩⲕⲧ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        typedef struct {{
        {fields}
        }} {name};
      ⲫⲩⲛⲕ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        {return_type} {name}({params}) {{
        {body}
        }}
      ⲧⲉⲥⲧ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        void test_{name}() {{
        {body}
        }}
        
    ⲧⲩⲡⲉⲥⲕⲣⲓⲡⲧ:
      ⲉⲝⲧⲉⲛⲥⲓⲟⲛ: ".ts"
      ⲥⲧⲣⲩⲕⲧ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        export interface {name} {{
        {fields}
        }}
        export class {name}Impl implements {name} {{
        {methods}
        }}
      ⲫⲩⲛⲕ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        export function {name}({params}): {return_type} {{
        {body}
        }}
      ⲧⲉⲥⲧ_ⲧⲉⲙⲡⲗⲁⲧⲉ: |
        test('{name}', () => {{
        {body}
        }});

