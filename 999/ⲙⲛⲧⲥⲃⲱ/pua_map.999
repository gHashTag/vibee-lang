// Unicode Private Use Area: U+E000 - U+E0FF
// Зарезервировано для алфавита 999

// Ключевые слова: U+E000 - U+E01F
Ⲙ ⲔⲖⲨⲎ = {
  0xE000: "void",   0xE001: "true",   0xE002: "false",
  0xE009: "fn",     0xE00A: "struct", 0xE00B: "enum",
  0xE00C: "let",    0xE00D: "if",     0xE00E: "else",
  0xE00F: "for",    0xE010: "while",  0xE011: "return",
  0xE012: "break",  0xE013: "continue", 0xE014: "use",
  0xE015: "int",    0xE016: "float",  0xE017: "string",
  0xE018: "bool",   0xE019: "list",   0xE01A: "map"
}

// Строчные: U+E020 - U+E03A (a-z + _)
