    âµ£.fn is_prime(n: u32) -> bool {
        if (n < 2) return false
        if (n == 2) return true
        if (n % 2 == 0) return false
        var i: u32 = 3
        while (i * i <= n) {
            if (n % i == 0) return false
            i += 2
        }
        return true
    }
    
