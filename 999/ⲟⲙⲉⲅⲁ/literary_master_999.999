    âµ£.fn find_sacred_simple(n: u32) -> SacredSimple {
        // ĞĞ°Ñ…Ğ¾Ğ´Ğ¸Ğ¼ n, k, m Ğ´Ğ»Ñ V = n Ã— 3^k Ã— Ï€^m â‰ˆ chapter_number
        var best_error: f64 = 1e10
        var best = SacredSimple{.n = 1, .k = 0, .m = 0}
        
        for (1..100) |n_val| {
            for (-3..10) |k_val| {
                for (-5..5) |m_val| {
                    var candidate = SacredSimple{
                        .n = n_val,
                        .k = @intCast(i32, k_val),
                        .m = @intCast(i32, m_val),
                    }
                    var value = candidate.compute()
                    var error = âµ£.abs(value - @intToFloat(f64, n))
                    if (error < best_error) {
                        best_error = error
                        best = candidate
                    }
                }
            }
        }
        return best
    }
    
