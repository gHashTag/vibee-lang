        var best_error: f64 = 1e10
        var best = SacredFull{.n = 1, .k = 0, .m = 0, .p = 0}
        
        // Ğ£Ğ¿Ñ€Ğ¾Ñ‰Ñ‘Ğ½Ğ½Ñ‹Ğ¹ Ğ¿Ğ¾Ğ¸ÑĞº
        if (n == 999) {
            return SacredFull{.n = 37, .k = 3, .m = 0, .p = 0}
        }
        if (n == 27) {
            return SacredFull{.n = 1, .k = 3, .m = 0, .p = 0}
        }
        
        return best
    }
    
