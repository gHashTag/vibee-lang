        â²â²—â²…â²Ÿâ²£â²“â²§â²â²™: """
        fn findWhitespace(input: []const u8) u32 {
            if (input.len >= 32) {
                const chunk: @Vector(32, u8) = input[0..32].*;
                const space = chunk == @splat(@as(u8, ' '));
                const tab = chunk == @splat(@as(u8, '\t'));
                const nl = chunk == @splat(@as(u8, '\n'));
                const cr = chunk == @splat(@as(u8, '\r'));
                return @bitCast(space | tab | nl | cr);
            }
            // scalar fallback
            var mask: u32 = 0;
            for (input, 0..) |c, i| {
                if (c == ' ' or c == '\t' or c == '\n' or c == '\r')
