        fn skipWhitespace(self: *Parser) void {
            while (self.pos < self.input.len) {
                const remaining = self.input[self.pos..];
                if (remaining.len >= 32) {
                    const mask = findWhitespace(remaining);
                    if (mask == 0xFFFFFFFF) {
