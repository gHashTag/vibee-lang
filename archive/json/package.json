{
  "name": "vibee-browser",
  "version": "162.0.0",
  "description": "VIBEE Browser Automation - Specification-first browser automation with quantum capabilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "vibee": "./bin/vibee",
    "vibee-agent": "./bin/vibee-agent"
  },
  "scripts": {
    "build": "zig build",
    "test": "zig build test",
    "test:browser": "for f in trinity/output/*browser*.zig; do zig test $f; done",
    "test:quantum": "for f in trinity/output/q_*.zig; do zig test $f; done",
    "gen": "./bin/vibee gen",
    "lint": "zig fmt --check src/",
    "docs": "typedoc --out docs/api src/"
  },
  "keywords": [
    "browser",
    "automation",
    "cdp",
    "chrome-devtools-protocol",
    "websocket",
    "headless",
    "puppeteer-alternative",
    "playwright-alternative",
    "quantum",
    "vibee",
    "zig"
  ],
  "author": "VIBEE Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gHashTag/vibee-lang.git"
  },
  "bugs": {
    "url": "https://github.com/gHashTag/vibee-lang/issues"
  },
  "homepage": "https://github.com/gHashTag/vibee-lang#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "bin/",
    "dist/",
    "specs/",
    "trinity/output/",
    "docs/"
  ],
  "vibee": {
    "phi": 1.618033988749895,
    "trinity": 3,
    "phoenix": 999,
    "identity": "φ² + 1/φ² = 3"
  },
  "dependencies": {
    "puppeteer": "^24.35.0"
  }
}
