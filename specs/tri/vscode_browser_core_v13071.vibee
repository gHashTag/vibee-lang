# VS Code Browser Core v13071
# Full VS Code Web integration in browser
# VIBEE v13071 | φ² + 1/φ² = 3 | PHOENIX = 999

name: vscode_browser_core_v13071
version: "13071.0.0"
language: zig
module: vscode_browser_core

types:
  VSCodeInstance:
    fields:
      instance_id: String
      workspace_path: String
      active_file: String
      theme: String
      ready: Bool
      
  EditorState:
    fields:
      file_path: String
      content: String
      cursor_line: Int
      cursor_column: Int
      selection_start: Int
      selection_end: Int
      
  WorkspaceConfig:
    fields:
      root_path: String
      settings_json: String
      extensions: String
      
  FileTreeNode:
    fields:
      path: String
      name: String
      is_directory: Bool
      children_count: Int

behaviors:
  - name: initialize_vscode
    given: "Workspace path provided"
    when: "Initialize called"
    then: "VS Code ready in <500ms"
    
  - name: open_file
    given: "File path provided"
    when: "Open file called"
    then: "File opened in editor in <50ms"
    
  - name: save_file
    given: "Editor has changes"
    when: "Save called"
    then: "File saved in <10ms"
    
  - name: navigate_file_tree
    given: "Workspace loaded"
    when: "Tree navigation requested"
    then: "Tree rendered in <100ms"
    
  - name: apply_settings
    given: "Settings JSON provided"
    when: "Settings applied"
    then: "Editor updated immediately"
