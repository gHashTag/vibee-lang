# VIBEE V73 COMPARISON
# ═══════════════════════════════════════════════════════════════

vibee_v73:
  name: "VIBEE v73"
  company: "Open Source"
  pricing: "FREE"
  type: "Terminal Agent"
  features:
    agentic: true
    multi_file: true
    git: true
    terminal: true
    autonomous: true
    streaming: true
    multi_model: true
    lsp: true
    spec_first: true
    auto_codegen: true
    formal_verification: true
    simd_optimization: true
    gpu_acceleration: true
  swe_bench: 80
  open_source: true
  context_window: "200K"
  speedup: "200x"

types:
  AITool:
    fields:
      name: String
      company: String
      pricing: String
      type: String
      swe_bench: Int
      open_source: Bool

  FeatureSet:
    fields:
      autocomplete: Bool
      chat: Bool
      multi_file: Bool
      git: Bool
      terminal: Bool
      autonomous: Bool

  CategoryStats:
    fields:
      category: String
      count: Int
      avg_swe_bench: Float
      free_count: Int

behaviors:
  - name: get_all_tools_count
    given: "All categories"
    when: "Count requested"
    then: "Returns total tool count"

  - name: get_category_stats
    given: "Category name"
    when: "Stats requested"
    then: "Returns category statistics"

  - name: compare_with_vibee
    given: "Tool name"
    when: "Comparison requested"
    then: "Returns feature comparison"

  - name: get_top_performers
    given: "SWE-bench threshold"
    when: "Filter requested"
    then: "Returns tools above threshold"

  - name: get_free_tools
    given: "All tools"
    when: "Free filter"
    then: "Returns free/open source tools"

  - name: get_terminal_agents
    given: "All tools"
    when: "Terminal filter"
    then: "Returns terminal-based agents"
