name: test_fixtures_v19840
version: "1.0.0"
language: zig
module: test_fixtures_v19840

types:
  Fixture:
    fields:
      name: String
      data: String
      scope: String

  FixtureFactory:
    fields:
      fixtures: Map<String,String>
      shared: Bool

behaviors:
  - name: fixture_create
    given: "Fixture definition"
    when: "Factory creates"
    then: "Returns fixture instance"

  - name: fixture_load
    given: "Fixture name"
    when: "Factory loads"
    then: "Returns loaded fixture"

  - name: fixture_cleanup
    given: "Fixture instance"
    when: "Factory cleans up"
    then: "Removes fixture"
