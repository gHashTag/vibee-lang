# vscode_core_v13311.vibee
# KOSCHEI CYCLE 9 - VS Code Core Integration
# Open Source VS Code in Browser | φ² + 1/φ² = 3

name: vscode_core
version: "13311.0.0"
language: zig
module: vscode_core

types:
  VSCodeConfig:
    fields:
      workspace_path: String
      theme: String
      font_size: Int
      extensions: String
      settings: String

  VSCodeInstance:
    fields:
      instance_id: String
      workspace: String
      active_editor: String
      status: String

  WorkspaceState:
    fields:
      root_path: String
      open_files: String
      active_file: String
      dirty_files: String

  VSCodeEvent:
    fields:
      event_type: String
      data: String
      timestamp: Int

behaviors:
  - name: create_vscode_config
    given: Configuration parameters
    when: Config creation
    then: Returns VSCodeConfig

  - name: initialize_vscode
    given: VSCodeConfig
    when: Initialization
    then: Returns VSCodeInstance

  - name: open_workspace
    given: Workspace path
    when: Open requested
    then: Returns WorkspaceState

  - name: get_workspace_state
    given: VSCodeInstance
    when: State query
    then: Returns WorkspaceState

  - name: subscribe_events
    given: Event pattern
    when: Subscription
    then: Returns event stream

  - name: dispose_vscode
    given: VSCodeInstance
    When: Dispose requested
    then: Returns cleanup status
