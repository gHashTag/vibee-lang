    given: "Router initialized"
    when: "Navigate 100 times"
    then: "Navigation performance measured"
    test_cases:
      - name: bench_nav
        input: { navigations: 100 }
        expected: { mean_ms_less_than: 5.0 }

  - name: bench_param_extraction
    given: "Routes with params"
    when: "Extract params 1000 times"
    then: "Extraction performance measured"
    test_cases:
      - name: bench_params
        input: { routes_with_params: 50, iterations: 1000 }
        expected: { mean_ms_less_than: 0.5 }

  - name: bench_query_parsing
    given: "URLs with query strings"
    when: "Parse 1000 queries"
    then: "Parsing performance measured"
    test_cases:
      - name: bench_query
        input: { iterations: 1000 }
        expected: { mean_ms_less_than: 0.1 }

