name: typescript_impl
version: "9.4.9"
language: zig
module: typescript_impl

sacred_constants:
  phi: 1.618033988749895
  trinity: 3.0
  phoenix: 999

creation_pattern:
  source: VIBEESpec
  transformer: TypeScriptGenerator
  result: TypeScriptModule

types:
  TypeScriptModule:
    fields:
      index_ts: String
      types_ts: String
      tests: String

  TSInterface:
    fields:
      name: String
      properties: List<String>
      exported: Bool

behaviors:
  - name: generate_interface
    given: "VIBEE type"
    when: "Interface generation"
    then: "TypeScript interface"
    pas_pattern: ALG
    test_cases:
      - name: test_interface
        input: '{"name": "User", "fields": [{"name": "id", "type": "Int"}, {"name": "name", "type": "String"}]}'
        expected: '{"code": "export interface User {\\n  id: number;\\n  name: string;\\n}"}'

  - name: generate_class
    given: "VIBEE type with behaviors"
    when: "Class generation"
    then: "TypeScript class"
    pas_pattern: ALG
    test_cases:
      - name: test_class
        input: '{"name": "UserService", "methods": [...]}'
        expected: '{"code": "export class UserService { ... }"}'

  - name: generate_tests
    given: "VIBEE test cases"
    when: "Test generation"
    then: "Jest tests"
    pas_pattern: ALG
    test_cases:
      - name: test_tests
        input: '{"test_cases": [...]}'
        expected: '{"code": "describe(\\"User\\", () => { it(\\"should create\\", () => { ... }); });"}'

  - name: verify_sacred_constants
    given: "Code"
    when: "Verification"
    then: "Constants verified"
    pas_pattern: ALG
    test_cases:
      - name: test_phi
        input: '{"phi": 1.618}'
        expected: '{"trinity": 3.0}'

typescript_type_mapping:
  String: "string"
  Int: "number"
  Float: "number"
  Bool: "boolean"
  List: "Array"
  Option: "| undefined"
  Object: "Record<string, unknown>"
  Timestamp: "Date"

