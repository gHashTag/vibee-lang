# Neural SDE - Li 2020
# φ² + 1/φ² = 3 | PHOENIX = 999

name: neural_sde_v1442
version: "1.0.0"
language: zig
module: neural_sde_v1442

types:
  FlowConfig:
    fields:
      num_steps: Int
      sigma_min: Float
      sigma_max: Float

  ODEState:
    fields:
      t: Float
      x: String
      dx: String

  DistillConfig:
    fields:
      teacher: String
      student: String
      temperature: Float

behaviors:
  - name: solve
    given: Initial condition
    when: ODE solving
    then: Returns trajectory

  - name: sample
    given: Noise
    when: Sampling
    then: Returns generated sample

  - name: distill
    given: Teacher outputs
    when: Distillation
    then: Returns student loss

  - name: phi_constants
    given: Sacred values
    when: Constants needed
    then: Returns φ-based constants
