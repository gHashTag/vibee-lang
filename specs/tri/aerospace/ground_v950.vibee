name: ground_v950
version: "9.5.0"
language: zig
module: ground
types:
  GroundConfig: { fields: { id: String, enabled: Bool, params: Object } }
  GroundState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  GroundResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_ground
    given: Config
    when: Init
    then: State
  - name: process_ground
    given: State
    when: Process
    then: Result
