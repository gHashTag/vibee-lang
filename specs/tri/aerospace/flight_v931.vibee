name: flight_v931
version: "9.3.1"
language: zig
module: flight
types:
  FlightConfig: { fields: { id: String, enabled: Bool, params: Object } }
  FlightState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  FlightResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_flight
    given: Config
    when: Init
    then: State
  - name: process_flight
    given: State
    when: Process
    then: Result
