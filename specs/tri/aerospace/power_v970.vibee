name: power_v970
version: "9.7.0"
language: zig
module: power
types:
  PowerConfig: { fields: { id: String, enabled: Bool, params: Object } }
  PowerState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  PowerResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_power
    given: Config
    when: Init
    then: State
  - name: process_power
    given: State
    when: Process
    then: Result
