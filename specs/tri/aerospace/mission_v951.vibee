name: mission_v951
version: "9.5.1"
language: zig
module: mission
types:
  MissionConfig: { fields: { id: String, enabled: Bool, params: Object } }
  MissionState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  MissionResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_mission
    given: Config
    when: Init
    then: State
  - name: process_mission
    given: State
    when: Process
    then: Result
