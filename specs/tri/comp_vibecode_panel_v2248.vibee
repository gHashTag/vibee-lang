behaviors:
  - name: render_vibecode_panel
    given: "Vibecode configuration"
    when: "Panel mounted"
    then: "Vibecoding panel UI rendered"
    test_cases:
      - name: test_render
        input: {}
        expected: { rendered: true }

  - name: generate_from_intent
    given: "Natural language intent"
    when: "Generate button clicked"
    then: "Code generated from intent"
    test_cases:
      - name: test_generate
        input: { intent: "Create a function to sort an array" }
        expected: { code_generated: true }

  - name: autocomplete_code
    given: "Partial code input"
    when: "Typing in editor"
    then: "AI suggestions displayed"
    test_cases:
      - name: test_autocomplete
        input: { partial: "fn sort" }
        expected: { suggestions_shown: true }

  - name: refactor_code
    given: "Selected code"
    when: "Refactor action triggered"
    then: "Refactored code suggested"
    test_cases:
      - name: test_refactor
        input: { code: "for i in 0..n { arr[i] = i; }" }
        expected: { refactored: true }

  - name: explain_code
    given: "Selected code"
    when: "Explain action triggered"
    then: "Code explanation displayed"
    test_cases:
      - name: test_explain
        input: { code: "const phi = 1.618;" }
        expected: { explanation_shown: true }

  - name: generate_tests
    given: "Function code"
    when: "Generate tests action"
    then: "Test cases generated"
    test_cases:
      - name: test_gen_tests
        input: { function: "fn add(a, b) { return a + b; }" }
        expected: { tests_generated: true }

  - name: generate_docs
    given: "Code block"
    when: "Generate docs action"
    then: "Documentation generated"
    test_cases:
      - name: test_gen_docs
        input: { code: "pub fn process(data: []u8) !void" }
        expected: { docs_generated: true }

  - name: apply_suggestion
    given: "AI suggestion"
    when: "Accept button clicked"
    then: "Suggestion applied to editor"
    test_cases:
      - name: test_apply
        input: { suggestion: "optimized_code" }
        expected: { applied: true }

