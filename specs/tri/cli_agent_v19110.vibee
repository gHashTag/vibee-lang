name: cli_agent_v19110
version: "1.0.0"
language: zig
module: cli_agent_v19110

types:
  AgentCommand:
    fields:
      action: String
      target: String
      context: String
      options: Map<String,String>

  AgentResponse:
    fields:
      success: Bool
      result: String
      actions_taken: List<String>
      tokens_used: Int

behaviors:
  - name: agent_chat
    given: "User message"
    when: "Agent processes chat"
    then: "Returns AI response"

  - name: agent_code
    given: "Code generation request"
    when: "Agent generates code"
    then: "Returns generated code"

  - name: agent_browse
    given: "Browse command"
    when: "Agent browses web"
    then: "Returns page content"
