name: crispr_v821
version: "8.2.1"
language: zig
module: crispr
types:
  CrisprConfig: { fields: { id: String, enabled: Bool, params: Object } }
  CrisprState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  CrisprResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_crispr
    given: Config
    when: Init
    then: State
  - name: process_crispr
    given: State
    when: Process
    then: Result
