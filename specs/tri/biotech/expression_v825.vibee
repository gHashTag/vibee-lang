name: expression_v825
version: "8.2.5"
language: zig
module: expression
types:
  ExpressionConfig: { fields: { id: String, enabled: Bool, params: Object } }
  ExpressionState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  ExpressionResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_expression
