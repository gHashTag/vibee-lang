name: variant_v820
version: "8.2.0"
language: zig
module: variant
types:
  VariantConfig: { fields: { id: String, enabled: Bool, params: Object } }
  VariantState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  VariantResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_variant
    given: Config
    when: Init
    then: State
  - name: process_variant
    given: State
    when: Process
    then: Result
