name: differentiation_v840
version: "8.4.0"
language: zig
module: differentiation
types:
  DifferentiationConfig: { fields: { id: String, enabled: Bool, params: Object } }
  DifferentiationState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  DifferentiationResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_differentiation
    given: Config
    when: Init
    then: State
  - name: process_differentiation
    given: State
    when: Process
    then: Result
