name: sequencing_v816
version: "8.1.6"
language: zig
module: sequencing
types:
  SequencingConfig: { fields: { id: String, enabled: Bool, params: Object } }
  SequencingState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  SequencingResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_sequencing
    given: Config
    when: Init
    then: State
  - name: process_sequencing
    given: State
    when: Process
    then: Result
