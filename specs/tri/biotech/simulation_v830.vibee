name: simulation_v830
version: "8.3.0"
language: zig
module: simulation
types:
  SimulationConfig: { fields: { id: String, enabled: Bool, params: Object } }
  SimulationState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  SimulationResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_simulation
    given: Config
    when: Init
    then: State
  - name: process_simulation
    given: State
    when: Process
    then: Result
