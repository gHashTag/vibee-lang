# RELATED GOLDEN IDENTITIES
golden_identities:
  primary:
    expression: "φ² + 1/φ² = 3"
    proof: "φ² = φ+1, 1/φ² = 2-φ, sum = 3"
    
  fibonacci_limit:
    expression: "lim(F(n+1)/F(n)) = φ"
    proof: "Fibonacci ratio converges to φ"
    
  continued_fraction:
    expression: "φ = 1 + 1/(1 + 1/(1 + ...))"
    proof: "Infinite continued fraction"
    
  nested_radical:
    expression: "φ = √(1 + √(1 + √(1 + ...)))"
    proof: "Infinite nested radical"
    
  trigonometric:
    expression: "φ = 2cos(π/5)"
    proof: "Pentagon geometry"
    
  power_identity:
    expression: "φⁿ = F(n)φ + F(n-1)"
    proof: "Fibonacci-golden ratio connection"
    
  sum_identity:
    expression: "φ + 1/φ = √5"
    proof: "Direct calculation"
    
  product_identity:
    expression: "φ × 1/φ = 1"
    proof: "Definition of inverse"
    
  difference_identity:
    expression: "φ - 1/φ = 1"
    proof: "φ - (φ-1) = 1"
    
  lucas_identity:
    expression: "φⁿ + (-1/φ)ⁿ = L(n)"
    proof: "Lucas number formula"

