# VIBEE v9016: Combinatorics Foundations
name: combinatorics_foundations
version: "9016.0.0"
language: zig
module: combinatorics_foundations

types:
  CombinatorialStructure:
    fields:
      type: String
      size: Int
      enumeration: Int
      generating_function: String

  Permutation:
    fields:
      elements: List<Int>
      cycles: List<String>
      order: Int
      parity: Bool

  Combination:
    fields:
      n: Int
      k: Int
      count: Int
      multiset: Bool

  Partition:
    fields:
      n: Int
      parts: List<Int>
      count: Int
      conjugate: String

behaviors:
  - name: count_structures
    given: "Combinatorial specification"
    when: "Enumerating"
    then: "Returns count"

  - name: generate_all
    given: "Structure type and size"
    when: "Listing"
    then: "Returns all structures"
