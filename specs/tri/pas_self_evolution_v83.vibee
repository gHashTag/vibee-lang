    then: "Self-improving agents"

  # === PRB PATTERN FOR EVOLUTION ===
  - name: prb_mutation
    given: "PRB pattern"
    when: "Apply to mutation"
    then: "Stochastic exploration"

  - name: prb_selection
    given: "PRB pattern"
    when: "Apply to selection"
    then: "Probabilistic survival"

  # === D&C PATTERN FOR EVOLUTION ===
  - name: dc_population
    given: "D&C pattern"
    when: "Apply to population"
    then: "Parallel evolution"

  - name: dc_islands
    given: "D&C pattern"
    when: "Apply to islands"
    then: "Island model GA"

  # === KEY PAPERS ===
  - name: paper_neat
    given: "NEAT (Stanley 2002)"
    when: "Analyze"
    then: "MLS: Neuroevolution"

  - name: paper_alphadev
    given: "AlphaDev (Mankowitz 2023)"
    when: "Analyze"
    then: "MLS: RL for algorithms"

  - name: paper_alphatensor
    given: "AlphaTensor (Fawzi 2022)"
    when: "Analyze"
    then: "MLS: Matrix discovery"

  - name: paper_automl
    given: "AutoML (Hutter 2019)"
    when: "Analyze"
    then: "MLS: Self-tuning"

  - name: paper_gpt4
    given: "GPT-4 (OpenAI 2023)"
    when: "Analyze"
    then: "MLS: Emergent abilities"

  - name: paper_self_instruct
    given: "Self-Instruct (Wang 2023)"
    when: "Analyze"
    then: "MLS: Self-improvement"

  # === SYNTHESIS ===
  - name: synthesize_patterns
    given: "50 papers"
    when: "Synthesize"
    then: "MLS 70%, PRB 20%, D&C 10%"

  - name: calculate_speedup
    given: "Pattern application"
    when: "Calculate"
    then: "100x amplification achieved"

