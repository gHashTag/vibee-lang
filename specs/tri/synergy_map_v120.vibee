# GOLDEN RATIO SYNERGY FORMULA
phi_synergy:
  phi: 1.618033988749895
  formula: "S = φ^n"
  table:
    - n: 1
      multiplier: 1.618
    - n: 2
      multiplier: 2.618
    - n: 3
      multiplier: 4.236
    - n: 5
      multiplier: 11.09
    - n: 7
      multiplier: 29.03
    - n: 10
      multiplier: 122.99
    - n: 15
      multiplier: 1364.00
    - n: 20
      multiplier: 15127.00

# φ² + 1/φ² = 3 | PHOENIX = 999
