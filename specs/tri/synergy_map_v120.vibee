    multiplier: 6.0
    integration: "Type-aware code generation"
    
  # Core × PAS
  core_pas:
    branches: ["core", "pas"]
    synergies: [S020]
    multiplier: 10.0
    integration: "Pattern-aware parsing"
    
  # Codegen × PAS
  codegen_pas:
    branches: ["codegen", "pas"]
    synergies: [S010, S011]
    multiplier: 8.0
