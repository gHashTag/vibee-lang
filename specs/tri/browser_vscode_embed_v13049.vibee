  - name: vscode_embed_theme
    given: Theme name
    when: Apply theme
    then: Theme applied
    test_cases:
      - name: test_theme
        input: { theme: "dark" }
        expected: { applied: true }

  - name: vscode_embed_extension
    given: Extension ID
    when: Install
    then: Extension active
    test_cases:
      - name: test_extension
        input: { extension: "zig-language" }
        expected: { active: true }

