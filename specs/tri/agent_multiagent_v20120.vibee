name: agent_multiagent_v20120
version: "1.0.0"
language: zig
module: agent_multiagent_v20120

types:
  AgentTeam:
    fields:
      agents: List<String>
      roles: Map<String,String>
      communication: String

  CoordinationResult:
    fields:
      consensus: Bool
      decision: String
      votes: Map<String,String>

behaviors:
  - name: coordinate_consensus
    given: "Multiple agents"
    when: "Team reaches consensus"
    then: "Returns agreed decision"

  - name: coordinate_delegate
    given: "Task and agents"
    when: "Leader delegates"
    then: "Returns task assignments"

  - name: coordinate_merge
    given: "Partial results"
    when: "Team merges outputs"
    then: "Returns combined result"
