# agent_data_extractor_v12392.vibee - Data Extraction
# Извлечение данных
# φ² + 1/φ² = 3 | PHOENIX = 999

name: agent_data_extractor_v12392
version: "12392.0.0"
language: zig
module: agent_data_extractor_v12392

types:
  ExtractionSchema:
    fields:
      schema_id: String
      fields: List
      selectors: Object
      transformations: List

  ExtractedData:
    fields:
      data: Object
      source_url: String
      extracted_at: Timestamp
      confidence: Float

  ExtractionField:
    fields:
      field_name: String
      selector: String
      field_type: String
      multiple: Bool

  ExtractionConfig:
    fields:
      wait_for_selector: String
      timeout_ms: Int
      pagination: Object

  ExtractionResult:
    fields:
      items: List
      total_count: Int
      pages_scraped: Int
      errors: List

behaviors:
  - name: extract_data
    given: ExtractionSchema
    when: Extraction
    then: Return ExtractionResult

  - name: detect_schema
    given: Page URL
    when: Schema detection
    then: Return ExtractionSchema

  - name: extract_table
    given: Table selector
    when: Table extraction
    then: Return table data

  - name: extract_links
    given: Link pattern
    when: Link extraction
    then: Return link list

  - name: export_data
    given: Data and format
    when: Export
    then: Return exported file

sacred_constants:
  phi: 1.618033988749895
  phoenix: 999
