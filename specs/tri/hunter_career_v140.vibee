# ТИПИЧНЫЕ ОШИБКИ

common_mistakes:
  mistake_1:
    error: "Спешка с отчётами"
    solution: "Качество важнее количества"
    
  mistake_2:
    error: "Игнорирование правил программы"
    solution: "Всегда читайте scope"
    
  mistake_3:
    error: "Только автоматизация"
    solution: "Ручное тестирование находит уникальные баги"
    
  mistake_4:
    error: "Сдаваться после дубликатов"
    solution: "Это нормально, продолжайте"
    
  mistake_5:
    error: "Не учиться на отказах"
    solution: "Каждый отказ - урок"

# ПЛАН НА ГОД

yearly_plan:
  month_1_3:
    focus: "Основы"
    goals:
      - "Закончить PortSwigger Academy"
      - "Зарегистрироваться на платформах"
      - "Первые 5 отчётов"
    expected: "$0-100"
    
  month_4_6:
    focus: "Практика"
    goals:
      - "Специализация в одной области"
      - "20+ отчётов"
      - "Первый medium/high баг"
    expected: "$100-1000"
    
  month_7_9:
    focus: "Рост"
    goals:
      - "Автоматизация разведки"
      - "Приватные программы"
      - "Networking"
    expected: "$1000-3000"
    
  month_10_12:
    focus: "Масштабирование"
    goals:
      - "Стабильный доход"
      - "Репутация на платформе"
      - "Планы на следующий год"
    expected: "$3000-10000"

# ФИНАЛЬНЫЕ СОВЕТЫ

final_advice:
  mindset:
    - "Это марафон, не спринт"
    - "Учитесь каждый день"
    - "Не сравнивайте себя с другими"
    - "Celebrate small wins"
    
  practical:
    - "Начните с одной платформы"
    - "Выберите одну специализацию"
    - "Документируйте всё"
    - "Стройте отношения"
    
  ethics:
    - "Всегда действуйте этично"
    - "Репутация важнее денег"
    - "Помогайте сообществу"

