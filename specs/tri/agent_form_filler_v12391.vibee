# agent_form_filler_v12391.vibee - Form Auto-fill
# Автозаполнение форм
# φ² + 1/φ² = 3 | PHOENIX = 999

name: agent_form_filler_v12391
version: "12391.0.0"
language: zig
module: agent_form_filler_v12391

types:
  FormField:
    fields:
      field_id: String
      field_type: String
      label: String
      selector: String
      required: Bool

  FormData:
    fields:
      form_id: String
      fields: List
      submit_selector: String

  FillRequest:
    fields:
      form_data: Object
      values: Object
      auto_submit: Bool

  FillResult:
    fields:
      filled_fields: List
      skipped_fields: List
      validation_errors: List

  FormProfile:
    fields:
      profile_id: String
      name: String
      data: Object
      encrypted: Bool

behaviors:
  - name: detect_form
    given: Page target
    when: Form detection
    then: Return FormData

  - name: fill_form
    given: FillRequest
    when: Form filling
    then: Return FillResult

  - name: save_profile
    given: FormProfile
    when: Profile save
    then: Return profile_id

  - name: load_profile
    given: Profile ID
    when: Profile load
    then: Return FormProfile

  - name: suggest_values
    given: FormField
    when: Suggestion
    then: Return suggested values

sacred_constants:
  phi: 1.618033988749895
  phoenix: 999
