name: optimization_v990
version: "9.9.0"
language: zig
module: optimization
types:
  OptimizationConfig: { fields: { id: String, enabled: Bool, params: Object } }
  OptimizationState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  OptimizationResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_optimization
    given: Config
    when: Init
    then: State
  - name: process_optimization
    given: State
    when: Process
    then: Result
