name: circular_v996
version: "9.9.6"
language: zig
module: circular
types:
  CircularConfig: { fields: { id: String, enabled: Bool, params: Object } }
  CircularState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  CircularResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_circular
    given: Config
    when: Init
    then: State
  - name: process_circular
    given: State
    when: Process
    then: Result
