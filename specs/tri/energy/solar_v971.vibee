name: solar_v971
version: "9.7.1"
language: zig
module: solar
types:
  SolarConfig: { fields: { id: String, enabled: Bool, params: Object } }
  SolarState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  SolarResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_solar
    given: Config
    when: Init
    then: State
  - name: process_solar
    given: State
    when: Process
    then: Result
