name: demand_v987
version: "9.8.7"
language: zig
module: demand
types:
  DemandConfig: { fields: { id: String, enabled: Bool, params: Object } }
  DemandState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  DemandResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_demand
    given: Config
    when: Init
    then: State
  - name: process_demand
    given: State
    when: Process
    then: Result
