name: fuel_v998
version: "9.9.8"
language: zig
module: fuel
types:
  FuelConfig: { fields: { id: String, enabled: Bool, params: Object } }
  FuelState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  FuelResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_fuel
    given: Config
    when: Init
    then: State
  - name: process_fuel
    given: State
    when: Process
    then: Result
