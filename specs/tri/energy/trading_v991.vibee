name: trading_v991
version: "9.9.1"
language: zig
module: trading
types:
  TradingConfig: { fields: { id: String, enabled: Bool, params: Object } }
  TradingState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  TradingResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_trading
    given: Config
    when: Init
    then: State
  - name: process_trading
    given: State
    when: Process
    then: Result
