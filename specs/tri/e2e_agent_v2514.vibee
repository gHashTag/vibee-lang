name: e2e_agent_v2514
version: "2514.0.0"
language: zig
module: e2e_agent_v2514

types:
  AgentTestCase:
    fields:
      command: String
      expected_actions: String
      success_criteria: String

  AgentTestResult:
    fields:
      command_understood: Bool
      actions_executed: Int
      goal_achieved: Bool
      duration_ms: Float

behaviors:
  - name: test_simple_navigation
    given: Command "Go to google.com"
    when: Agent processes command
    then: Browser navigates to Google

  - name: test_search_task
    given: Command "Search for VIBEE on GitHub"
    when: Agent executes task
    then: Search results displayed

  - name: test_form_filling
    given: Command "Fill login form with test@test.com"
    when: Agent finds and fills form
    then: Email field contains value

  - name: test_multi_step_task
    given: Complex multi-step command
    when: Agent plans and executes
    then: All steps completed successfully

sacred_constants:
  phi: 1.618033988749895
  phi_squared_plus_inverse_squared: 3
  phoenix: 999
