name: absolute_infinity_v35000
version: "35000.0.0"
language: zig
module: absolute_infinity_v35000

types:
  AbsoluteInfinityCore:
    fields:
      dimension: Int
      magnitude: Float
      infinite: Bool
      
  InfinityState:
    fields:
      level: Int
      transcended: Bool

behaviors:
