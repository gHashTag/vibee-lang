# MILESTONE MATRIX V118 - Achievement Matrix by Phases
# VIBEE AMPLIFICATION MODE: Measurable Progress Tracking
name: milestone_matrix_v118
version: "118.0.0"
language: zig
module: milestone_matrix

creation_pattern:
  source: PhasesAndGoals
  transformer: MilestoneTracker
  result: AchievementMatrix

# MILESTONE TRACKING SYSTEM
# Each phase has:
# - Entry criteria
# - Exit criteria
# - Key milestones
# - Success metrics

types:
  Milestone:
    fields:
      id: String
      phase: Int
      name: String
      description: String
      entry_criteria: List<String>
      exit_criteria: List<String>
      deliverables: List<String>
      success_metrics: List<Metric>
      dependencies: List<String>
      status: String

  Metric:
    fields:
      name: String
      baseline: Float
      target: Float
      current: Float
      unit: String
      measurement_method: String

  PhaseGate:
    fields:
      phase: Int
      gate_name: String
      required_milestones: List<String>
      approval_criteria: List<String>
      risk_assessment: String
      go_no_go: String

  AchievementLevel:
    fields:
      level: Int
      name: String
      description: String
      unlocked_capabilities: List<String>
      amplification_factor: String

  ProgressReport:
    fields:
      phase: Int
      completed_milestones: Int
      total_milestones: Int
      completion_percentage: Float
      on_track: Bool
      blockers: List<String>

  MilestoneRisk:
    fields:
      milestone_id: String
      risk_type: String
      probability: Float
      impact: Float
      mitigation: String
      contingency: String

  PhaseTransition:
    fields:
      from_phase: Int
      to_phase: Int
      transition_criteria: List<String>
      handoff_items: List<String>
      lessons_learned: List<String>

  AchievementMatrix:
    fields:
      phases: List<PhaseGate>
      milestones: List<Milestone>
      levels: List<AchievementLevel>
      current_phase: Int

behaviors:
  - name: define_milestones
    given: "Phase goals"
    when: "Milestone definition"
    then: "Measurable milestones"
    test_cases:
      - name: milestones
        input: "5 phases"
        expected: "50 milestones"

  - name: track_progress
    given: "Current state"
    when: "Progress tracking"
    then: "Completion percentage"
    test_cases:
      - name: progress
        input: "milestone status"
        expected: "percentage calculated"

  - name: evaluate_gate
    given: "Phase gate criteria"
    when: "Gate evaluation"
    then: "Go/No-Go decision"
    test_cases:
      - name: gate_eval
        input: "gate criteria"
        expected: "decision made"

  - name: assess_risks
    given: "Milestone risks"
    when: "Risk assessment"
    then: "Mitigation plan"
    test_cases:
      - name: risk_assess
        input: "risk factors"
        expected: "mitigations"

  - name: calculate_achievement
    given: "Completed milestones"
    when: "Achievement calculation"
    then: "Achievement level"
    test_cases:
      - name: achievement
        input: "completions"
        expected: "level assigned"

  - name: plan_transition
    given: "Phase completion"
    when: "Transition planning"
    then: "Handoff plan"
    test_cases:
      - name: transition
        input: "phase end"
        expected: "transition plan"

