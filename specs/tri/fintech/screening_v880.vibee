name: screening_v880
version: "8.8.0"
language: zig
module: screening
types:
  ScreeningConfig: { fields: { id: String, enabled: Bool, params: Object } }
  ScreeningState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  ScreeningResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_screening
    given: Config
    when: Init
    then: State
  - name: process_screening
    given: State
    when: Process
    then: Result
