name: kyc_v875
version: "8.7.5"
language: zig
module: kyc
types:
  KycConfig: { fields: { id: String, enabled: Bool, params: Object } }
  KycState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  KycResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_kyc
    given: Config
    when: Init
    then: State
  - name: process_kyc
    given: State
    when: Process
    then: Result
