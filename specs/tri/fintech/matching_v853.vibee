name: matching_v853
version: "8.5.3"
language: zig
module: matching
types:
  MatchingConfig: { fields: { id: String, enabled: Bool, params: Object } }
  MatchingState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  MatchingResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_matching
    given: Config
    when: Init
    then: State
  - name: process_matching
    given: State
    when: Process
    then: Result
