name: reporting_v881
version: "8.8.1"
language: zig
module: reporting
types:
  ReportingConfig: { fields: { id: String, enabled: Bool, params: Object } }
  ReportingState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  ReportingResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_reporting
    given: Config
    when: Init
    then: State
  - name: process_reporting
    given: State
    when: Process
    then: Result
