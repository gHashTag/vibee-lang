name: settlement_v855
version: "8.5.5"
language: zig
module: settlement
types:
  SettlementConfig: { fields: { id: String, enabled: Bool, params: Object } }
  SettlementState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  SettlementResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_settlement
    given: Config
    when: Init
    then: State
  - name: process_settlement
    given: State
    when: Process
    then: Result
