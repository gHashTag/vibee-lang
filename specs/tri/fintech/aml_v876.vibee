name: aml_v876
version: "8.7.6"
language: zig
module: aml
types:
  AmlConfig: { fields: { id: String, enabled: Bool, params: Object } }
  AmlState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  AmlResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_aml
    given: Config
    when: Init
    then: State
  - name: process_aml
    given: State
    when: Process
    then: Result
