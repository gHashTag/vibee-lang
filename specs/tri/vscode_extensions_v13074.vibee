# VS Code Extensions v13074
# Extension host and marketplace integration
# VIBEE v13074 | φ² + 1/φ² = 3 | PHOENIX = 999

name: vscode_extensions_v13074
version: "13074.0.0"
language: zig
module: vscode_extensions

types:
  Extension:
    fields:
      id: String
      name: String
      version: String
      publisher: String
      enabled: Bool
      
  ExtensionManifest:
    fields:
      contributes: String
      activation_events: String
      dependencies: String
      
  MarketplaceEntry:
    fields:
      extension_id: String
      display_name: String
      description: String
      downloads: Int
      rating: Float

behaviors:
  - name: list_extensions
    given: "Extensions installed"
    when: "List requested"
    then: "All extensions returned"
    
  - name: install_extension
    given: "Extension ID provided"
    when: "Install called"
    then: "Extension installed and activated"
    
  - name: uninstall_extension
    given: "Extension installed"
    when: "Uninstall called"
    then: "Extension removed cleanly"
    
  - name: search_marketplace
    given: "Search query provided"
    when: "Search called"
    then: "Results returned in <200ms"
    
  - name: activate_extension
    given: "Extension installed"
    when: "Activation event fires"
    then: "Extension activated correctly"
