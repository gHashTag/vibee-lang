types:
  ToolFeatures:
    fields:
      name: String
      autocomplete: Bool
      multi_file_edit: Bool
      git_integration: Bool
      terminal_execution: Bool
      autonomous_mode: Bool
      multi_model: Bool
      lsp_integration: Bool
      streaming: Bool
      context_window: Int
      swe_bench: Int
      pricing: String

  FeatureComparison:
    fields:
      feature: String
      vibee: Bool
      claude_code: Bool
      cursor: Bool
      aider: Bool
      cline: Bool

  UniqueFeature:
    fields:
      name: String
      description: String
      competitors_count: Int
      advantage: String

behaviors:
  - name: count_features
    given: "Tool name"
    when: "Feature count requested"
    then: "Returns number of true features"

  - name: compare_features
    given: "Two tools"
    when: "Comparison requested"
    then: "Returns feature diff"

  - name: get_unique_features
    given: "Tool name"
    when: "Unique features requested"
    then: "Returns features not in competitors"

  - name: calculate_feature_score
    given: "Tool features"
    when: "Score calculation"
    then: "Returns weighted score"

  - name: get_best_value
    given: "Price and features"
    when: "Value calculation"
    then: "Returns value score"
