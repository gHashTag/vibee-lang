# Криптографические атаки
crypto_attacks:
  - name: "Differential Cryptanalysis"
    target: "DES, block ciphers"
    complexity: "2^47 for DES"
    
  - name: "Linear Cryptanalysis"
    target: "DES, block ciphers"
    complexity: "2^43 for DES"
    
  - name: "Birthday Attack"
    target: "Hash functions"
    complexity: "O(2^(n/2))"
    
  - name: "Length Extension"
    target: "MD5, SHA-1"
    complexity: "O(1)"
    
  - name: "Padding Oracle"
    target: "CBC mode"
    complexity: "O(n)"
    
  - name: "BEAST"
    target: "TLS 1.0 CBC"
    complexity: "Practical"
    
  - name: "POODLE"
    target: "SSL 3.0"
    complexity: "Practical"
    
  - name: "DROWN"
    target: "SSLv2 + TLS"
    complexity: "Practical"

