# VIBEE PAS Papers: USENIX Browser Security v201
# Comprehensive survey of USENIX publications on browser security

name: usenix_security_v201
version: "201.0.0"
language: zig
module: usenix_security

types:
  USENIXPaper:
    fields:
      title: String
      authors: List<String>
      venue: String
      year: Int
      citations: Int

  SecurityArea:
    fields:
      isolation: String
      xss: String
      csrf: String
      privacy: String
      sandboxing: String

behaviors:
  - name: survey_process_isolation
    given: "USENIX Security papers"
    when: "Isolation research"
    then: "Catalog isolation papers"
    papers:
      - title: "Site Isolation: Process Separation for Web Sites"
        authors: ["Reis, C.", "Moshchuk, A.", "Oskov, N."]
        venue: "USENIX Security 2019"
        citations: 200
        
      - title: "Isolating Web Programs in Modern Browser Architectures"
        authors: ["Grier, C.", "Tang, S.", "King, S."]
        venue: "EuroSys 2008"
        citations: 300
        
      - title: "The Multi-Principal OS Construction of the Gazelle Web Browser"
        authors: ["Wang, H.", "Grier, C.", "Moshchuk, A."]
        venue: "USENIX Security 2009"
        citations: 400

  - name: survey_xss_protection
    given: "USENIX Security papers"
    when: "XSS research"
    then: "Catalog XSS papers"
    papers:
      - title: "Noncespaces: Using Randomization to Defeat XSS"
        authors: ["Gundy, M.", "Chen, H."]
        venue: "NDSS 2009"
        citations: 200
        
      - title: "XSS-GUARD: Precise Dynamic Prevention of XSS Attacks"
        authors: ["Bisht, P.", "Venkatakrishnan, V."]
        venue: "DIMVA 2008"
        citations: 250
        
      - title: "Blueprint: Robust Prevention of XSS with Positive Tainting"
        authors: ["Louw, M.", "Venkatakrishnan, V."]
        venue: "IEEE S&P 2009"
        citations: 300

  - name: survey_privacy
    given: "USENIX Security papers"
    when: "Privacy research"
    then: "Catalog privacy papers"
    papers:
      - title: "The Web Never Forgets: Persistent Tracking Mechanisms"
        authors: ["Acar, G.", "Eubank, C.", "Englehardt, S."]
        venue: "CCS 2014"
        citations: 500
        
      - title: "Fingerprinting the Fingerprinters"
        authors: ["Iqbal, U.", "Englehardt, S.", "Shafiq, Z."]
        venue: "IEEE S&P 2021"
        citations: 100

test_cases:
  - name: test_isolation_papers
    input: { area: "isolation" }
    expected: { count: 3 }

  - name: test_xss_papers
    input: { area: "xss" }
    expected: { count: 3 }

  - name: test_privacy_papers
    input: { area: "privacy" }
    expected: { count: 2 }
