behaviors:
  # === PAS PATTERNS FOR AMPLIFICATION ===
  - name: apply_precomputation
    given: "PRE pattern for spec templates"
    when: "Cache common spec patterns"
    then: "1.3x speedup via template library"

  - name: apply_divide_conquer
    given: "D&C pattern for multi-file"
    when: "Split large specs into modules"
    then: "1.5x speedup via parallel gen"

  - name: apply_ml_guided
    given: "MLS pattern for AI assist"
    when: "AI suggests spec improvements"
    then: "1.5x speedup via smart completion"

  - name: apply_frequency_domain
    given: "FDT pattern for LSP"
    when: "Real-time spec validation"
    then: "2.0x speedup via instant feedback"

  # === SCIENTIFIC PAPERS ===
  - name: analyze_codex_paper
    given: "Evaluating LLMs on Code (Chen 2021)"
    when: "Apply to amplification"
    then: "MLS pattern validates 9x speedup"

  - name: analyze_copilot_study
    given: "GitHub Copilot Productivity (Peng 2023)"
    when: "Compare to amplification"
    then: "Copilot 55% faster, VIBEE 9x faster"

  - name: analyze_spec_first_paper
    given: "Specification-First Development (Jackson 2012)"
    when: "Apply to VIBEE"
    then: "PRE pattern confirms spec-first advantage"

  - name: analyze_code_gen_survey
    given: "Code Generation Survey (Zan 2023)"
    when: "Compare approaches"
    then: "Spec-first outperforms prompt-based"

  # === IMPROVEMENT PREDICTIONS ===
  - name: predict_template_library
    given: "Current: manual spec writing"
    when: "Add template library"
    then: "v78: 12x amplification (+33%)"

  - name: predict_watch_mode
    given: "Current: manual regeneration"
    when: "Add watch mode"
    then: "v79: 14x amplification (+17%)"

  - name: predict_lsp_server
    given: "Current: no IDE support"
    when: "Add VibeeSpec LSP"
    then: "v80: 28x amplification (+100%)"

  - name: calculate_ultimate_speedup
    given: "All improvements applied"
    when: "Calculate total"
    then: "v82: 63x amplification"

