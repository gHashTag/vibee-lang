# BENCHMARKS (Current vs Target)

benchmarks:
  parser:
    module: "vibeec/parser"
    baseline_v1: 100
    current_v150: 250
    target_v200: 750
    unit: "MB/s"
    improvement: "150%"

  codegen:
    module: "vibeec/codegen"
    baseline_v1: 50
    current_v150: 120
    target_v200: 300
    unit: "specs/s"
    improvement: "140%"

  test_execution:
    module: "trinity/output"
    baseline_v1: 10
    current_v150: 157
    target_v200: 500
    unit: "tests"
    improvement: "1470%"

  binary_size:
    module: "bin/vibee"
    baseline_v1: 5.0
    current_v150: 3.2
    target_v200: 2.0
    unit: "MB"
    improvement: "-36%"

  memory_usage:
    module: "runtime"
    baseline_v1: 100
    current_v150: 65
    target_v200: 40
    unit: "MB"
    improvement: "-35%"

  compile_time:
    module: "full_build"
    baseline_v1: 30
    current_v150: 12
    target_v200: 5
    unit: "seconds"
    improvement: "-60%"

