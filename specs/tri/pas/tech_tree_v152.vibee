# TECHNOLOGY TREE V152 - Complete Development Roadmap
# φ² + 1/φ² = 3 | PHOENIX = 999
name: tech_tree_v152
version: "152.0.0"
language: zig
module: tech_tree

creation_pattern:
  source: Technologies
  transformer: TreeBuilder
  result: TechTree

types:
  TechNode:
    fields:
      id: String
      tier: Int
      name: String
      description: String
      prerequisites: List<String>
      daemon: String
      hours: Int
      papers: Int

  TechBranch:
    fields:
      name: String
      nodes: List<String>
      total_hours: Int

  Dependency:
    fields:
      from_node: String
      to_node: String
      dep_type: String

  TechTree:
    fields:
      branches: List<TechBranch>
      nodes: List<TechNode>
      dependencies: List<Dependency>

behaviors:
  - name: add_node
    given: "Node data"
    when: "Adding"
    then: "Node added"
    test_cases:
      - name: add
        input: "node"
        expected: "added"

  - name: get_prerequisites
    given: "Node ID"
    when: "Query"
    then: "Prerequisites"
    test_cases:
      - name: prereq
        input: "node_id"
        expected: "list"

  - name: calculate_path
    given: "Start, End"
    when: "Path finding"
    then: "Optimal path"
    test_cases:
      - name: path
        input: "start, end"
        expected: "path"

  - name: estimate_time
    given: "Path"
    when: "Estimation"
    then: "Hours"
    test_cases:
      - name: time
        input: "path"
        expected: "hours"

  - name: get_branch
    given: "Branch name"
    when: "Query"
    then: "Branch nodes"
    test_cases:
      - name: branch
        input: "name"
        expected: "nodes"

