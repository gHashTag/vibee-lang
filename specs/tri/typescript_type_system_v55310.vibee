name: typescript_type_system_v55310
version: "55310.0.0"
language: zig
module: typescript_type_system

# TYPESCRIPT TYPE SYSTEM
# Маппинг VIBEE типов в TypeScript типы

types:
  TypeScriptPrimitiveMap:
    fields:
      vibee_string: String
      vibee_number: String
      vibee_boolean: String
      vibee_any: String

  TypeScriptCollectionMap:
    fields:
      vibee_array: String
      vibee_record: String
      vibee_map: String

  TypeScriptUnionMap:
    fields:
      optional_suffix: String
      undefined_type: String
      null_type: String

behaviors:
  - name: ts_map_string
    given: VIBEE String
    when: Mapping requested
    then: Returns string

  - name: ts_map_int
    given: VIBEE Int
    when: Mapping requested
    then: Returns number

  - name: ts_map_float
    given: VIBEE Float
    when: Mapping requested
    then: Returns number

  - name: ts_map_bool
    given: VIBEE Bool
    when: Mapping requested
    then: Returns boolean

  - name: ts_map_option
    given: VIBEE Option of T
    when: Mapping requested
    then: Returns T or undefined

  - name: ts_map_list
    given: VIBEE List of T
    when: Mapping requested
    then: Returns T array

  - name: ts_map_map
    given: VIBEE Map of K V
    when: Mapping requested
    then: Returns Record of K V
