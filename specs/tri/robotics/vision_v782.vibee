name: vision_v782
version: "7.8.2"
language: zig
module: vision
types:
  VisionConfig: { fields: { id: String, enabled: Bool, params: Object } }
  VisionState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  VisionResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_vision
    given: Config
    when: Init
    then: State
  - name: process_vision
    given: State
    when: Process
    then: Result
