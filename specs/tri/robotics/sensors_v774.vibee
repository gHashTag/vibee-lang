name: sensors_v774
version: "7.7.4"
language: zig
module: sensors
types:
  SensorsConfig: { fields: { id: String, enabled: Bool, params: Object } }
  SensorsState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  SensorsResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_sensors
    given: Config
    when: Init
    then: State
  - name: process_sensors
    given: State
    when: Process
    then: Result
