name: serving_v809
version: "8.0.9"
language: zig
module: serving
types:
  ServingConfig: { fields: { id: String, enabled: Bool, params: Object } }
  ServingState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  ServingResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_serving
    given: Config
    when: Init
    then: State
  - name: process_serving
    given: State
    when: Process
    then: Result
