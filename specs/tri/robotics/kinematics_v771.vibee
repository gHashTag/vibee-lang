name: kinematics_v771
version: "7.7.1"
language: zig
module: kinematics
types:
  KinematicsConfig: { fields: { id: String, enabled: Bool, params: Object } }
  KinematicsState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  KinematicsResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_kinematics
    given: Config
    when: Init
    then: State
  - name: process_kinematics
    given: State
    when: Process
    then: Result
