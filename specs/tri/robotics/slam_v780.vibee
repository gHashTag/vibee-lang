name: slam_v780
version: "7.8.0"
language: zig
module: slam
types:
  SlamConfig: { fields: { id: String, enabled: Bool, params: Object } }
  SlamState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  SlamResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_slam
    given: Config
    when: Init
    then: State
  - name: process_slam
    given: State
    when: Process
    then: Result
