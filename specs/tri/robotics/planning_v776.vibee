name: planning_v776
version: "7.7.6"
language: zig
module: planning
types:
  PlanningConfig: { fields: { id: String, enabled: Bool, params: Object } }
  PlanningState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  PlanningResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_planning
    given: Config
    when: Init
    then: State
  - name: process_planning
    given: State
    when: Process
    then: Result
