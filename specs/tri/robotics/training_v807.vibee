name: training_v807
version: "8.0.7"
language: zig
module: training
types:
  TrainingConfig: { fields: { id: String, enabled: Bool, params: Object } }
  TrainingState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  TrainingResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_training
    given: Config
    when: Init
    then: State
  - name: process_training
    given: State
    when: Process
    then: Result
