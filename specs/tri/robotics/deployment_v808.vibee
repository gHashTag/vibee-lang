name: deployment_v808
version: "8.0.8"
language: zig
module: deployment
types:
  DeploymentConfig: { fields: { id: String, enabled: Bool, params: Object } }
  DeploymentState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  DeploymentResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_deployment
    given: Config
    when: Init
    then: State
  - name: process_deployment
    given: State
    when: Process
    then: Result
