name: inference_v806
version: "8.0.6"
language: zig
module: inference
types:
  InferenceConfig: { fields: { id: String, enabled: Bool, params: Object } }
  InferenceState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  InferenceResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_inference
    given: Config
    when: Init
    then: State
  - name: process_inference
    given: State
    when: Process
    then: Result
