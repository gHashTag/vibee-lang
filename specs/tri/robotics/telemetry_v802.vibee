name: telemetry_v802
version: "8.0.2"
language: zig
module: telemetry
types:
  TelemetryConfig: { fields: { id: String, enabled: Bool, params: Object } }
  TelemetryState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  TelemetryResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_telemetry
    given: Config
    when: Init
    then: State
  - name: process_telemetry
    given: State
    when: Process
    then: Result
