name: control_v773
version: "7.7.3"
language: zig
module: control
types:
  ControlConfig: { fields: { id: String, enabled: Bool, params: Object } }
  ControlState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  ControlResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_control
    given: Config
    when: Init
    then: State
  - name: process_control
    given: State
    when: Process
    then: Result
