name: browser_navigation_v13042
version: "13042.0.0"
language: zig
module: browser_navigation_v13042

types:
  NavigationConfig:
    fields:
      url: String
      wait_for: String
      timeout_ms: Int

  NavigationResult:
    fields:
      success: Bool
      load_time_ms: Int
      final_url: String

behaviors:
  - name: navigation_goto
    given: URL
    when: Navigation
    then: Load page
    test_cases:
      - name: test_goto
        input: { url: "https://github.com" }
        expected: { loaded: true }

  - name: navigation_wait
    given: Selector
    when: Wait for element
    then: Element visible
    test_cases:
      - name: test_wait
        input: { selector: "#main" }
        expected: { visible: true }

  - name: navigation_back
    given: History
    when: Go back
    then: Previous page
    test_cases:
      - name: test_back
        input: {}
        expected: { navigated: true }

  - name: navigation_smart
    given: Intent
    when: Smart navigation
    then: Auto-detect URL
    test_cases:
      - name: test_smart
        input: { intent: "open github" }
        expected: { url: "https://github.com" }

self_evolution:
  enabled: true
  generation: 13042
