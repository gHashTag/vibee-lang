# COMPETITIVE ANALYSIS V114 - Market & Competitor Analysis
name: competitive_analysis_v114
version: "114.0.0"
language: zig
module: competitive_analysis

creation_pattern:
  source: MarketLandscape
  transformer: CompetitiveAnalyzer
  result: StrategicPosition

types:
  Competitor:
    fields:
      name: String
      category: CompetitorCategory
      swe_bench_score: Float
      strengths: List<String>
      weaknesses: List<String>
      threat_level: Float

  CompetitorCategory:
    variants:
      - AICodeAssistant
      - CodeGenPlatform
      - SpecificationTool
      - IDEPlugin
      - AutonomousAgent

  MarketSegment:
    fields:
      name: String
      size_billion: Float
      growth_rate: Float
      vibee_fit: Float

  CompetitiveAdvantage:
    fields:
      advantage: String
      uniqueness: Float
      defensibility: Float
      duration_years: Int

  ThreatAssessment:
    fields:
      threat: String
      probability: Float
      impact: Float
      response: String

  MarketPosition:
    fields:
      segment: String
      position: Int
      market_share: Float
      growth_potential: Float

behaviors:
  - name: analyze_competitors
    given: "Competitor list"
    when: "Analysis"
    then: "Competitive landscape"
    test_cases:
      - name: analyze_all
        input: "10 competitors"
        expected: "ranked analysis"

  - name: identify_threats
    given: "Competitors"
    when: "Threat identification"
    then: "Threat matrix"
    test_cases:
      - name: threats
        input: "competitive landscape"
        expected: "5 key threats"

  - name: find_opportunities
    given: "Market gaps"
    when: "Opportunity analysis"
    then: "Opportunity list"
    test_cases:
      - name: opportunities
        input: "market analysis"
        expected: "10 opportunities"

  - name: assess_moat
    given: "VIBEE capabilities"
    when: "Moat assessment"
    then: "Defensibility score"
    test_cases:
      - name: moat
        input: "all advantages"
        expected: "moat strength"

  - name: predict_moves
    given: "Competitor behavior"
    when: "Move prediction"
    then: "Expected actions"
    test_cases:
      - name: predict
        input: "competitor history"
        expected: "next moves"

  - name: recommend_strategy
    given: "Full analysis"
    when: "Strategy recommendation"
    then: "Strategic actions"
    test_cases:
      - name: strategy
        input: "complete analysis"
        expected: "action plan"

