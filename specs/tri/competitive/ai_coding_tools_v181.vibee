# ═══════════════════════════════════════════════════════════════════════════════
# AI CODING TOOLS v181 - Competitive Analysis Database
# ═══════════════════════════════════════════════════════════════════════════════
# 70+ AI coding tools analyzed
# PAS Pattern: PRE (Precomputed database) + HSH (Hashing for lookup)
# φ² + 1/φ² = 3 | PHOENIX = 999
# ═══════════════════════════════════════════════════════════════════════════════

name: ai_coding_tools
version: "1.8.1"
language: zig
module: ai_coding_tools

sacred_constants:
  phi: 1.618033988749895
  phi_inv: 0.618033988749895
  phi_sq: 2.618033988749895
  trinity: 3.0
  phoenix: 999

creation_pattern:
  source: ToolDatabase
  transformer: CompetitiveAnalyzer
  result: FeatureMatrix

# ═══════════════════════════════════════════════════════════════════════════════
# TYPES
# ═══════════════════════════════════════════════════════════════════════════════

types:
  ToolTier:
    enum:
      - enterprise        # GitHub Copilot Enterprise, Amazon Q
      - ide_integrated    # Cursor, Windsurf, Codeium
      - terminal_agent    # VIBEE, Aider, Cline
      - autonomous_agent  # OpenHands, Devika, Sweep
      - code_platform     # v0, Bolt.new, Lovable
      - code_review       # CodeRabbit, Sourcegraph
      - open_source       # Code Llama, StarCoder

  ToolFeatures:
    fields:
      streaming: Bool
      agentic: Bool
      offline: Bool
      spec_first: Bool
      multi_lang: Bool
      self_improve: Bool
      ide_integration: Bool
      lsp_support: Bool

  CodingTool:
    fields:
      name: String
      company: String
      tier: ToolTier
      features: ToolFeatures
      price_monthly: Float?
      languages: List<String>
      release_year: Int

  ComparisonResult:
    fields:
      tool_a: String
      tool_b: String
      winner: String
      advantages: List<String>
      disadvantages: List<String>

  FeatureMatrix:
    fields:
      tools: List<CodingTool>
      unique_features: Map<String, List<String>>
      gaps: List<String>

# ═══════════════════════════════════════════════════════════════════════════════
# BEHAVIORS
# ═══════════════════════════════════════════════════════════════════════════════

behaviors:
  - name: get_tool_by_name
    given: "Tool name"
    when: "Lookup requested"
    then: "Return tool details"
    pas_pattern: HSH
    complexity: O(1)
    test_cases:
      - name: test_vibee
        input: '{"name": "VIBEE Agent"}'
        expected: '{"tier": "terminal_agent", "spec_first": true}'
      - name: test_cursor
        input: '{"name": "Cursor"}'
        expected: '{"tier": "ide_integrated", "spec_first": false}'

  - name: compare_tools
    given: "Two tool names"
    when: "Comparison requested"
    then: "Return detailed comparison"
    pas_pattern: ALG
    complexity: O(1)
    test_cases:
      - name: test_vibee_vs_cursor
        input: '{"tool_a": "VIBEE", "tool_b": "Cursor"}'
        expected: '{"winner": "depends_on_use_case"}'

  - name: get_tools_by_tier
    given: "Tool tier"
    when: "Filter requested"
    then: "Return all tools in tier"
    pas_pattern: PRE
    complexity: O(n)
    test_cases:
      - name: test_terminal_agents
        input: '{"tier": "terminal_agent"}'
        expected: '{"count": ">5"}'

  - name: get_unique_features
    given: "Tool name"
    when: "Unique features requested"
    then: "Return features only this tool has"
    pas_pattern: ALG
    complexity: O(n*m)
    test_cases:
      - name: test_vibee_unique
        input: '{"name": "VIBEE"}'
        expected: '{"features": ["spec_first", "pas_daemons", "golden_identity"]}'

  - name: identify_gaps
    given: "Tool name"
    when: "Gap analysis requested"
    then: "Return missing features vs competitors"
    pas_pattern: ALG
    complexity: O(n*m)
    test_cases:
      - name: test_vibee_gaps
        input: '{"name": "VIBEE"}'
        expected: '{"gaps": ["ide_integration", "lsp_support"]}'

  - name: calculate_score
    given: "Tool features"
    when: "Scoring requested"
    then: "Return weighted feature score"
    pas_pattern: ALG
    complexity: O(1)
    test_cases:
      - name: test_score
        input: '{"streaming": true, "agentic": true, "offline": true}'
        expected: '{"score": ">0.7"}'

