name: paper_webvoyager_v510
version: "510.0.0"
language: zig
module: paper_webvoyager_v510

types:
  WebVoyagerAgent:
    fields:
      agent_id: String
      vision_model: String
      action_space: List<String>
      max_trajectory_length: Int

  WebVoyagerTask:
    fields:
      task_id: String
      website: String
      instruction: String
      difficulty: String
      expected_steps: Int

  WebVoyagerObservation:
    fields:
      screenshot: String
      set_of_marks: List<Object>
      previous_actions: List<String>
      current_url: String

  SetOfMark:
    fields:
      mark_id: Int
      element_type: String
      bounding_box: Object
      label: String
      interactable: Bool

  WebVoyagerTrajectory:
    fields:
      trajectory_id: String
      observations: List<Object>
      actions: List<Object>
      success: Bool
      total_steps: Int

behaviors:
  - name: capture_observation
    given: Browser state
    when: Observation needed
    then: Return screenshot with marks

  - name: annotate_screenshot
    given: Screenshot
    when: Annotation needed
    then: Return set-of-marks overlay

  - name: select_action
    given: Observation and task
    when: Action selection
    then: Return chosen action

  - name: execute_action
    given: Action and mark_id
    when: Execution requested
    then: Perform browser action

  - name: evaluate_trajectory
    given: Complete trajectory
    when: Evaluation needed
    then: Return success metrics

  - name: compare_models
    given: Model list and tasks
    when: Comparison needed
    then: Return comparison results

  - name: analyze_failures
    given: Failed trajectories
    when: Analysis needed
    then: Return failure patterns

  - name: generate_synthetic_tasks
    given: Website and difficulty
    when: Task generation
    then: Return new tasks

  - name: benchmark_on_dataset
    given: Task dataset
    when: Benchmark requested
    then: Return benchmark results
