    given: "Cursor position and context"
    when: "Completion requested"
    then: "Relevant completions returned"
    test_cases:
      - name: test_completion
        input: { prefix: "fn ", language: "zig" }
        expected: { suggestions_count_gt: 0 }

  - name: get_inline_suggestion
    given: "Current line context"
    when: "Inline suggestion"
    then: "Ghost text suggestion"
    test_cases:
      - name: test_inline
        input: { line: "const x = " }
        expected: { has_suggestion: true }

  - name: rank_suggestions
    given: "Multiple suggestions"
    when: "Ranking applied"
    then: "Sorted by relevance"
    test_cases:
      - name: test_rank
        input: { suggestions: 10 }
        expected: { sorted: true }

pas_analysis:
  current_algorithm: "Frequency-based ranking"
  predicted_improvement: "ML-based ranking"
  confidence: 0.80
  patterns_applied: [MLS, PRE]
  timeline: "2026 Q2"

self_evolution:
