types:
  E2ETest:
    fields:
      name: String
      steps: List<TestStep>
      expected: Object
      timeout: Int

  TestStep:
    fields:
      action: String
      selector: String
      value: Option<String>

  E2EResult:
    fields:
      test_name: String
      passed: Bool
      duration: Int
      error: Option<String>
      screenshot: Option<String>

behaviors:
  - name: test_home_navigation
    given: "App loaded"
    when: "Click Home link"
    then: "Home page displayed"
    test_cases:
      - name: e2e_home
        input: { click: "nav-home" }
        expected: { url: "/", title: "Home" }

  - name: test_modules_navigation
    given: "App loaded"
    when: "Click Modules link"
    then: "Modules page displayed"
    test_cases:
      - name: e2e_modules
        input: { click: "nav-modules" }
        expected: { url: "/modules", modules_visible: true }

  - name: test_pas_navigation
    given: "App loaded"
    when: "Click PAS link"
    then: "PAS page displayed"
    test_cases:
      - name: e2e_pas
        input: { click: "nav-pas" }
        expected: { url: "/pas", pas_visible: true }

  - name: test_back_navigation
    given: "Navigated to page"
    when: "Click back button"
    then: "Previous page displayed"
    test_cases:
      - name: e2e_back
        input: { navigate: "/modules", back: true }
        expected: { url: "/" }

  - name: test_deep_link
    given: "Direct URL access"
    when: "Load deep link"
    then: "Correct page displayed"
    test_cases:
      - name: e2e_deep_link
        input: { url: "/modules/llm/openai" }
        expected: { breadcrumbs: 4 }

