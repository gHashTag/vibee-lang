# E2E GAIA Tests
# Test GAIA benchmark tasks
# Level 1, 2, 3 tasks

name: e2e_gaia_tests
version: "1.0.0"
language: zig
module: e2e_gaia_tests

types:
  GAIATest:
    fields:
      test_id: String
      task_id: String
      level: Int
      expected_answer: String

  Level1Test:
    fields:
      question: String
      tools_needed: List<String>
      expected_answer: String

  Level2Test:
    fields:
      question: String
      tools_needed: List<String>
      steps_expected: Int

  Level3Test:
    fields:
      question: String
      tools_needed: List<String>
      complexity: String

  ToolChainTest:
    fields:
      tools: List<String>
      expected_result: String

behaviors:
  - name: test_level1_task
    given: Simple task
    when: Level 1 test
    then: Returns test result

  - name: test_level2_task
    given: Medium task
    when: Level 2 test
    then: Returns test result

  - name: test_level3_task
    given: Complex task
    when: Level 3 test
    then: Returns test result

  - name: test_web_search
    given: Search query
    when: Web search test
    then: Returns test result

  - name: test_code_execution
    given: Code task
    When: Code execution test
    then: Returns test result

  - name: test_file_processing
    given: File task
    when: File processing test
    then: Returns test result

  - name: test_multi_tool_chain
    given: Tool chain
    when: Chain test
    then: Returns test result
