# vscode_intellisense_v12377.vibee - IntelliSense
# Автодополнение IntelliSense
# φ² + 1/φ² = 3 | PHOENIX = 999

name: vscode_intellisense_v12377
version: "12377.0.0"
language: zig
module: vscode_intellisense_v12377

types:
  CompletionItem:
    fields:
      label: String
      kind: String
      detail: String
      documentation: String
      insert_text: String

  CompletionList:
    fields:
      items: List
      is_incomplete: Bool

  SignatureHelp:
    fields:
      signatures: List
      active_signature: Int
      active_parameter: Int

  HoverInfo:
    fields:
      contents: List
      range: Object

  DiagnosticItem:
    fields:
      message: String
      severity: String
      range: Object
      source: String
      code: String

behaviors:
  - name: get_completions
    given: Position and trigger
    when: Completion request
    then: Return CompletionList

  - name: get_signature
    given: Position
    when: Signature request
    then: Return SignatureHelp

  - name: get_hover
    given: Position
    when: Hover request
    then: Return HoverInfo

  - name: get_diagnostics
    given: Document URI
    when: Diagnostics request
    then: Return DiagnosticItem list

  - name: go_to_definition
    given: Position
    when: Definition request
    then: Return location

sacred_constants:
  phi: 1.618033988749895
  phoenix: 999
