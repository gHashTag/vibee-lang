# ═══════════════════════════════════════════════════════════════════════════════
# BEHAVIORS: 75+ Scientific Formulas
# ═══════════════════════════════════════════════════════════════════════════════

behaviors:
  # UI/UX (18 papers)
  - name: fitts_law
    formula: MT = a + b * log2(2D/W + 1)
  - name: hick_law
    formula: RT = a + b * log2(n + 1)
  - name: steering_law
    formula: T = a + b * (A/W)
  - name: throughput
    formula: TP = ID / MT
  - name: klm_model
    formula: T = K + P + H + M + R
    
  # Diff Algorithms (18 papers)
  - name: myers_diff
    complexity: O(ND)
  - name: hirschberg_lcs
    complexity: O(mn) time, O(m+n) space
  - name: wu_diff
    complexity: O(NP)
  - name: patience_diff
    complexity: O(n log n)
  - name: tree_edit_distance
    complexity: O(n^3)
    
  # Diffusion (15 papers)
  - name: ddpm_forward
    formula: x_t = sqrt(alpha_bar) * x0 + sqrt(1 - alpha_bar) * epsilon
  - name: ddpm_reverse
    formula: x_{t-1} = (1/sqrt(alpha)) * (x_t - beta/sqrt(1-alpha_bar) * eps) + sigma * z
  - name: ddim_step
    formula: x_{t-1} = sqrt(alpha_bar_{t-1}) * x0_pred + sqrt(1 - alpha_bar_{t-1}) * eps
  - name: cfg
    formula: eps = eps_u + s * (eps_c - eps_u)
  - name: dpm_solver
    formula: Exponential integrator
    
  # Generative (12 papers)
  - name: vae_elbo
    formula: ELBO = E[log p(x|z)] - beta * KL
  - name: kl_divergence
    formula: KL = -0.5 * (1 + log_var - mu^2 - exp(log_var))
  - name: flow_likelihood
    formula: log p(x) = log p(z) + sum(log |det J|)
  - name: wgan_loss
    formula: L = E[D(x)] - E[D(G(z))]
    
  # Rendering (12 papers)
  - name: rendering_equation
    formula: L_o = L_e + integral(BRDF * L_i * cos(theta))
  - name: ggx_distribution
    formula: D = alpha^2 / (pi * ((n.h)^2 * (alpha^2 - 1) + 1)^2)
  - name: schlick_fresnel
    formula: F = F0 + (1 - F0) * (1 - v.h)^5
  - name: cook_torrance
    formula: f_r = DFG / (4 * n.v * n.l)
  - name: disney_brdf
    formula: Multi-lobe with 11 parameters
    
  # GP (12 papers)
  - name: rbf_kernel
    formula: k = sigma^2 * exp(-||x-x'||^2 / (2*l^2))
  - name: matern_32
    formula: k = (1 + sqrt(3)*r/l) * exp(-sqrt(3)*r/l)
  - name: matern_52
    formula: k = (1 + sqrt(5)*r/l + 5*r^2/(3*l^2)) * exp(-sqrt(5)*r/l)
  - name: spectral_mixture
    formula: k = sum(w * exp(-2*pi^2*tau^2*v) * cos(2*pi*tau*mu))
  - name: expected_improvement
    formula: EI = (mu - f*) * Phi(Z) + sigma * phi(Z)
  - name: ucb
    formula: UCB = mu + sqrt(beta) * sigma
    
  # Uncertainty (12 papers)
  - name: mc_dropout
    formula: Var[y] = (1/T) * sum((y_t - mean)^2)
  - name: deep_ensemble
    formula: Var = mean(sigma^2 + mu^2) - mean(mu)^2
  - name: ece
    formula: ECE = sum(|B_m|/n * |acc(B_m) - conf(B_m)|)
  - name: brier_score
    formula: BS = (1/N) * sum((p - y)^2)
  - name: evidential
    formula: u = K / (sum(alpha) - K)
    
  # PAS DAEMONS
  - name: pas_confidence
    formula: confidence = base_rate * time_factor * gap_factor * ml_boost
  - name: golden_identity
    formula: phi^2 + 1/phi^2 = 3
  - name: lucas_numbers
    formula: L(n) = L(n-1) + L(n-2)
  - name: fibonacci
    formula: F(n) = F(n-1) + F(n-2)

