name: koschei_vscode_deep_v13004
version: "13004.0.0"
language: zig
module: koschei_vscode_deep_v13004

types:
  VSCodeDeepConfig:
    fields:
      workspace: String
      deep_sync: Bool
      ai_assist: Bool

  VSCodeDeepInstance:
    fields:
      id: String
      load_time_ms: Int
      sync_latency_ms: Int

behaviors:
  - name: vscode_deep_load
    given: Deep mode
    when: Load requested
    then: 100ms load time
    test_cases:
      - name: test_load
        input: { deep: true }
        expected: { load_ms: 100 }

  - name: vscode_deep_sync
    given: File change
    when: Sync triggered
    then: 25ms sync latency
    test_cases:
      - name: test_sync
        input: { file: "main.zig" }
        expected: { sync_ms: 25 }

  - name: vscode_deep_lsp
    given: LSP connection
    when: Completion request
    then: 5ms response
    test_cases:
      - name: test_lsp
        input: { lsp: true }
        expected: { response_ms: 5 }

  - name: vscode_deep_debug
    given: Debug session
    when: Breakpoint hit
    then: 5ms jump time
    test_cases:
      - name: test_debug
        input: { breakpoint: true }
        expected: { jump_ms: 5 }

benchmark_vs_v12908:
  v12908_load_ms: 200
  v13004_load_ms: 100
  speedup: 2x

self_evolution:
  enabled: true
  generation: 13004
