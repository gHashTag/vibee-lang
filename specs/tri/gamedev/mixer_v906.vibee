name: mixer_v906
version: "9.0.6"
language: zig
module: mixer
types:
  MixerConfig: { fields: { id: String, enabled: Bool, params: Object } }
  MixerState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  MixerResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_mixer
    given: Config
    when: Init
    then: State
  - name: process_mixer
    given: State
    when: Process
    then: Result
