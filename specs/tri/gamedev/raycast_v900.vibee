name: raycast_v900
version: "9.0.0"
language: zig
module: raycast
types:
  RaycastConfig: { fields: { id: String, enabled: Bool, params: Object } }
  RaycastState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  RaycastResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_raycast
    given: Config
    when: Init
    then: State
  - name: process_raycast
    given: State
    when: Process
    then: Result
