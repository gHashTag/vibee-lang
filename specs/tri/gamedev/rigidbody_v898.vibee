name: rigidbody_v898
version: "8.9.8"
language: zig
module: rigidbody
types:
  RigidbodyConfig: { fields: { id: String, enabled: Bool, params: Object } }
  RigidbodyState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  RigidbodyResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_rigidbody
    given: Config
    when: Init
    then: State
  - name: process_rigidbody
    given: State
    when: Process
    then: Result
