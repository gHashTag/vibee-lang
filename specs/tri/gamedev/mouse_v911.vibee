name: mouse_v911
version: "9.1.1"
language: zig
module: mouse
types:
  MouseConfig: { fields: { id: String, enabled: Bool, params: Object } }
  MouseState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  MouseResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_mouse
