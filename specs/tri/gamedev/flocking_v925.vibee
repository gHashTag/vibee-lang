name: flocking_v925
version: "9.2.5"
language: zig
module: flocking
types:
  FlockingConfig: { fields: { id: String, enabled: Bool, params: Object } }
  FlockingState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  FlockingResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_flocking
    given: Config
    when: Init
    then: State
  - name: process_flocking
    given: State
    when: Process
    then: Result
