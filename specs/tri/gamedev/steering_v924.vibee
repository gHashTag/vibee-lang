name: steering_v924
version: "9.2.4"
language: zig
module: steering
types:
  SteeringConfig: { fields: { id: String, enabled: Bool, params: Object } }
  SteeringState: { fields: { status: String, data: Object, timestamp: Timestamp } }
  SteeringResult: { fields: { success: Bool, output: Object, error: Option<String> } }
behaviors:
  - name: init_steering
    given: Config
    when: Init
    then: State
  - name: process_steering
    given: State
    when: Process
    then: Result
