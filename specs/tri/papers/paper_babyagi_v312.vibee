name: paper_babyagi_v312
version: "1.0.0"
language: zig
module: paper_babyagi_v312

types:
  BabyAGITask:
    fields:
      id: Int
      name: String
      status: String
      result: Option<String>

  BabyAGITaskList:
    fields:
      tasks: List<BabyAGITask>
      objective: String
      completed: Int

  BabyAGIAgent:
    fields:
      execution_agent: String
      task_creation_agent: String
      prioritization_agent: String

  BabyAGILoop:
    fields:
      task_list: BabyAGITaskList
      iteration: Int
      context: List<String>

  BabyAGIConfig:
    fields:
      objective: String
      initial_task: String
      max_iterations: Int
      model: String

behaviors:
  - name: execute_task
    given: Task from list
    When: Execution agent runs
    then: Task completed with result

  - name: create_new_tasks
    given: Task result available
    When: Creation agent runs
    then: New tasks generated

  - name: prioritize_tasks
    given: Task list updated
    When: Prioritization runs
    then: Tasks reordered by priority

  - name: run_loop
    given: Objective set
    When: Main loop runs
    then: Tasks executed until objective met

  - name: store_context
    given: Task completed
    When: Context update runs
    then: Result stored for future tasks

  - name: apply_to_browser
    given: Browser testing objective
    When: BabyAGI applied
    then: Test tasks generated and executed

  - name: benchmark_babyagi
    given: Test objectives
    When: Benchmark runs
    then: BabyAGI performance measured

  - name: compare_to_autogpt
    given: Same objectives
    When: Comparison runs
    then: Differences quantified
