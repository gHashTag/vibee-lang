name: paper_agentgpt_v313
version: "1.0.0"
language: zig
module: paper_agentgpt_v313

types:
  AgentGPTGoal:
    fields:
      name: String
      description: String
      tasks: List<String>
      status: String

  AgentGPTExecution:
    fields:
      goal: AgentGPTGoal
      current_task: String
      output: String
      thinking: String

  AgentGPTLoop:
    fields:
      goals: List<AgentGPTGoal>
      executions: List<AgentGPTExecution>
      tokens_used: Int

  AgentGPTConfig:
    fields:
      name: String
      goal: String
      model: String
      max_loops: Int

behaviors:
  - name: deploy_agent
    given: Goal specified
    When: Agent deployed
    then: Agent starts working on goal

  - name: generate_tasks
    given: Goal analyzed
    When: Task generation runs
    then: Tasks created for goal

  - name: execute_task
    given: Task selected
    When: Execution runs
    then: Task completed with output

  - name: analyze_output
    given: Task output available
    When: Analysis runs
    then: Next steps determined

  - name: complete_goal
    given: All tasks done
    When: Completion check runs
    then: Goal marked complete

  - name: apply_to_browser
    given: Browser automation goal
    When: AgentGPT applied
    then: Web tasks automated

  - name: benchmark_agentgpt
    given: Test goals
    When: Benchmark runs
    then: AgentGPT performance measured

  - name: compare_architectures
    given: Other agent architectures
    When: Comparison runs
    then: Trade-offs identified
