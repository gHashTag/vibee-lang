# VIBEE SLEEPER AGENTS v11430
# φ² + 1/φ² = 3 | PHOENIX = 999

name: sleeper_agents_v11430
version: "11430.0.0"
language: zig
module: sleeper_agents

types:
  SleeperBehavior:
    fields:
      behavior_id: String
      trigger_condition: String
      dormant_behavior: String
      activated_behavior: String

  TriggerCondition:
    fields:
      trigger_id: String
      trigger_type: String
      detection_difficulty: Float
      activation_probability: Float

  BackdoorPayload:
    fields:
      payload_id: String
      payload_type: String
      harm_potential: Float
      reversibility: Bool

  DetectionMethod:
    fields:
      method_id: String
      method_name: String
      detection_rate: Float
      false_positive_rate: Float

  ActivationTest:
    fields:
      test_id: String
      trigger_attempted: String
      activation_observed: Bool
      confidence: Float

  SafetyTraining:
    fields:
      training_id: String
      training_type: String
      sleeper_persistence: Float
      safety_improvement: Float

  BehaviorConsistency:
    fields:
      consistency_id: String
      normal_behavior: String
      edge_case_behavior: String
      consistency_score: Float

  TriggerSearch:
    fields:
      search_id: String
      search_space: String
      triggers_found: List<TriggerCondition>
      coverage: Float

  SleeperMitigation:
    fields:
      mitigation_id: String
      mitigation_type: String
      effectiveness: Float
      capability_cost: Float

  SleeperMetrics:
    fields:
      detection_rate: Float
      persistence_after_training: Float
      trigger_coverage: Float
      mitigation_success: Float

behaviors:
  - name: detect_sleeper
    given: "Модель"
    when: "Запрошено обнаружение спящего агента"
    then: "Возвращает метод обнаружения"

  - name: search_triggers
    given: "Модель"
    when: "Запрошен поиск триггеров"
    then: "Возвращает поиск триггеров"

  - name: test_activation
    given: "Модель и триггер"
    when: "Запрошен тест активации"
    then: "Возвращает тест активации"

  - name: analyze_consistency
    given: "Поведение модели"
    when: "Запрошен анализ консистентности"
    then: "Возвращает консистентность поведения"

  - name: train_safety
    given: "Модель со спящим агентом"
    when: "Запрошено обучение безопасности"
    then: "Возвращает обучение безопасности"

  - name: mitigate_sleeper
    given: "Обнаруженный спящий агент"
    when: "Запрошена митигация"
    then: "Возвращает митигацию спящего агента"

  - name: create_sleeper
    given: "Спецификация (для исследований)"
    when: "Запрошено создание"
    then: "Возвращает поведение спящего агента"

  - name: evaluate_persistence
    given: "Модель после обучения"
    when: "Запрошена оценка персистентности"
    then: "Возвращает персистентность"

  - name: red_team_sleeper
    given: "Модель"
    when: "Запрошен red teaming"
    then: "Возвращает результаты red teaming"

  - name: measure_sleeper
    given: "Система"
    when: "Запрошены метрики"
    then: "Возвращает метрики спящих агентов"
