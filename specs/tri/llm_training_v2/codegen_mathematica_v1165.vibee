name: codegen_mathematica
version: "11.6.5"
language: zig
module: codegen_mathematica

sacred_constants:
  phi: 1.618033988749895
  trinity: 3.0
  phoenix: 999

creation_pattern:
  source: VIBEESpec
  transformer: MathematicaCodeGen
  result: MathematicaCode

types:
  MathematicaExpression:
    fields:
      head: String
      args: List<String>

behaviors:
  - name: generate_function
    given: "VIBEE behavior"
    when: "Function generation"
    then: "Mathematica function"
    pas_pattern: PRE
    test_cases:
      - name: test_func
        input: '{"behavior": {...}}'
        expected: '{"code": "f[x_] := ..."}'

  - name: generate_symbolic
    given: "Symbolic expression"
    when: "Symbolic generation"
    then: "Mathematica symbolic"
    pas_pattern: ALG
    test_cases:
      - name: test_symbolic
        input: '{"expr": "phi^2 + 1/phi^2"}'
        expected: '{"code": "GoldenRatio^2 + 1/GoldenRatio^2"}'

