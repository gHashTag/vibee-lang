name: koschei_e2e_parallel_v13014
version: "13014.0.0"
language: zig
module: koschei_e2e_parallel_v13014

types:
  ParallelE2EConfig:
    fields:
      parallel_browsers: Int
      sharding: Bool
      visual_diff_v2: Bool

  ParallelE2EReport:
    fields:
      total_tests: Int
      duration_s: Float
