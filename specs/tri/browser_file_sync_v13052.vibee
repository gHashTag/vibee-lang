name: browser_file_sync_v13052
version: "13052.0.0"
language: zig
module: browser_file_sync_v13052

types:
  FileSyncConfig:
    fields:
      local_path: String
      remote_path: String
      bidirectional: Bool

  FileSyncResult:
    fields:
      synced: Bool
      latency_ms: Int
      conflicts: Int

behaviors:
  - name: file_sync_push
    given: Local change
    when: Push
    then: Remote updated
    test_cases:
      - name: test_push
        input: { file: "main.zig" }
        expected: { pushed: true }

  - name: file_sync_pull
    given: Remote change
    when: Pull
    then: Local updated
    test_cases:
      - name: test_pull
        input: { file: "main.zig" }
        expected: { pulled: true }

  - name: file_sync_watch
    given: Directory
    when: Watch
    then: Auto-sync
    test_cases:
      - name: test_watch
        input: { dir: "/src" }
        expected: { watching: true }

  - name: file_sync_conflict
    given: Conflict
    when: Resolution
    then: Merged
    test_cases:
      - name: test_conflict
        input: { conflict: true }
        expected: { resolved: true }

self_evolution:
  enabled: true
  generation: 13052
