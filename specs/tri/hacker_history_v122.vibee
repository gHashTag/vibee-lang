  sha1:
    algorithm: "SHA-1"
    year_broken: 2017
    attack: "SHAttered collision"
    before: "2^80 collision"
    after: "2^63 collision"
    quantum: "Grover reduces further"
    
  rsa_512:
    algorithm: "RSA-512"
    year_broken: 1999
    attack: "NFS factoring"
    before: "Secure"
    after: "Factorable"
