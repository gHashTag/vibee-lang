# e2e_vscode_tests_v12402.vibee - VSCode E2E Tests
# E2E тесты VSCode
# φ² + 1/φ² = 3 | PHOENIX = 999

name: e2e_vscode_tests_v12402
version: "12402.0.0"
language: zig
module: e2e_vscode_tests_v12402

types:
  VSCodeTest:
    fields:
      test_id: String
      name: String
      workspace: String
      commands: List
      assertions: List

  EditorTestAction:
    fields:
      action_type: String
      file_path: String
      content: String
      position: Object

  EditorTestAssertion:
    fields:
      assertion_type: String
      file_path: String
      expected: Object

  VSCodeTestResult:
    fields:
      test_id: String
      passed: Bool
      duration_ms: Int
      editor_state: Object

  VSCodeTestConfig:
    fields:
      extensions: List
      settings: Object
      workspace_path: String

behaviors:
  - name: run_vscode_test
    given: VSCodeTest
    when: Test execution
    then: Return VSCodeTestResult

  - name: open_file
    given: File path
    when: File open
    then: Return editor state

  - name: edit_file
    given: EditorTestAction
    when: File edit
    then: Return success

  - name: assert_editor
    given: EditorTestAssertion
    when: Assertion
    then: Return assertion result

  - name: test_intellisense
    given: Position
    when: IntelliSense test
    then: Return completions

sacred_constants:
  phi: 1.618033988749895
  phoenix: 999
