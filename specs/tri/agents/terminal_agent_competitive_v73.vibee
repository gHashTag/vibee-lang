# Terminal Agent Competitive Analysis V73
# 70+ AI Coding Assistants Comparison

name: terminal_agent_competitive_v73
version: "73.0.0"
language: zig
module: terminal_agent_competitive_v73

# Categories of AI Coding Tools
categories:
  ide_integrated:
    - name: "GitHub Copilot"
      type: "IDE Plugin"
      pricing: "$19/month"
      features: ["autocomplete", "chat", "inline_suggestions"]
      swe_bench: 55
      
    - name: "Cursor"
      type: "IDE Fork"
      pricing: "$20/month"
      features: ["autocomplete", "chat", "composer", "agent"]
      swe_bench: 65
      
    - name: "Windsurf"
      type: "IDE Fork"
      pricing: "$15/month"
      features: ["cascade", "flows", "autocomplete"]
      swe_bench: 60
      
    - name: "JetBrains AI"
      type: "IDE Plugin"
      pricing: "$10/month"
      features: ["autocomplete", "chat", "refactor"]
      swe_bench: 50

  terminal_agents:
    - name: "Claude Code"
      type: "Terminal"
      pricing: "$20/month"
      features: ["agentic", "multi_file", "git", "terminal"]
      swe_bench: 72.7
      
    - name: "Gemini CLI"
      type: "Terminal"
      pricing: "FREE"
      features: ["chat", "code_gen", "explain"]
      swe_bench: 45
      
    - name: "Aider"
      type: "Terminal"
      pricing: "FREE"
      features: ["git", "multi_file", "diff"]
      swe_bench: 48
      
    - name: "Cline"
      type: "Terminal"
      pricing: "FREE"
      features: ["autonomous", "browser", "terminal"]
      swe_bench: 52
      
    - name: "OpenCode"
      type: "Terminal"
      pricing: "FREE"
      features: ["multi_model", "lsp", "diff"]
      swe_bench: 50

  autonomous_agents:
    - name: "Devin"
      type: "Autonomous"
      pricing: "$500/month"
      features: ["full_autonomy", "browser", "deploy"]
      swe_bench: 48
      
    - name: "OpenHands"
      type: "Autonomous"
      pricing: "FREE"
      features: ["sandbox", "browser", "terminal"]
      swe_bench: 53
      
    - name: "Sweep AI"
      type: "Autonomous"
      pricing: "$120/month"
      features: ["pr_generation", "issue_to_pr"]
      swe_bench: 40

  cloud_platforms:
    - name: "Replit Agent"
      type: "Cloud IDE"
      pricing: "$25/month"
      features: ["deploy", "collaborate", "agent"]
      swe_bench: 45
      
    - name: "v0 by Vercel"
      type: "UI Generator"
      pricing: "$20/month"
      features: ["ui_gen", "react", "tailwind"]
      swe_bench: 35
      
    - name: "Bolt.new"
      type: "Full Stack"
      pricing: "$20/month"
      features: ["full_stack", "deploy", "preview"]
      swe_bench: 42

# Feature Matrix for Terminal Agents
feature_matrix:
  multi_file_edit:
    vibee: true
    claude_code: true
    cursor: true
    aider: true
    cline: true
    
  git_integration:
    vibee: true
    claude_code: true
    cursor: false
    aider: true
    cline: true
    
  terminal_execution:
    vibee: true
    claude_code: true
    cursor: false
    aider: false
    cline: true
    
  autonomous_mode:
    vibee: true
    claude_code: true
    cursor: false
    aider: false
    cline: true
    
  multi_model:
    vibee: true
    claude_code: false
    cursor: true
    aider: true
    cline: true
    
  lsp_integration:
    vibee: true
    claude_code: false
    cursor: true
    aider: false
    cline: false
    
  streaming:
    vibee: true
    claude_code: true
    cursor: true
    aider: true
    cline: true
    
  context_window:
    vibee: "200K"
    claude_code: "200K"
    cursor: "128K"
    aider: "128K"
    cline: "200K"

types:
  Competitor:
    fields:
      name: String
      type: String
      pricing: String
      swe_bench: Int
      open_source: Bool

  FeatureComparison:
    fields:
      feature: String
      vibee: Bool
      claude_code: Bool
      cursor: Bool
      aider: Bool

behaviors:
  - name: compare_swe_bench
    given: "All competitors"
    when: "Comparison requested"
    then: "Returns ranking"

  - name: count_features
    given: "Feature matrix"
    when: "Count requested"
    then: "Returns feature count per tool"
