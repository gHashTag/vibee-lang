name: browser_webgpu_core_v920
version: "9.0.0"
language: zig
module: browser_webgpu_core

sacred_constants:
  phi: 1.618033988749895
  phi_identity: 3
  phoenix: 999
  yolo_mode: 9
  matryoshka_depth: 9

types:
  GPUAdapter:
    fields:
      name: String
      vendor: String
      device_id: Int
      features: List<String>
      limits: String

  GPUDevice:
    fields:
      adapter: String
      queue: String
      lost: Bool
      features: List<String>

  GPUBuffer:
    fields:
      size: Int
      usage: Int
      mapped: Bool
      label: String

  GPUTexture:
    fields:
      width: Int
      height: Int
      depth: Int
      format: String
      usage: Int

behaviors:
  - name: request_adapter
    given: GPU options
