  - name: request_device
    given: GPUAdapter
    when: Request
    then: Device obtained
  - name: create_buffer
    given: Buffer descriptor
    when: Create
    then: Buffer created
  - name: create_texture
    given: Texture descriptor
    when: Create
    then: Texture created
  - name: submit_commands
    given: Command buffer
    when: Submit
    then: Commands submitted

test_cases:
  - name: test_browser_webgpu_core_adapter
    input: {options: default}
    expected: {adapter: obtained}
  - name: test_browser_webgpu_core_device
    input: {adapter: valid}
    expected: {device: obtained}
  - name: test_browser_webgpu_core_buffer
    input: {size: 1024}
    expected: {buffer: created}
  - name: test_browser_webgpu_core_texture
    input: {width: 256, height: 256}
