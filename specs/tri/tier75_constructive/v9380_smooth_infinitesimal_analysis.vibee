# VIBEE v9380: Smooth Infinitesimal Analysis
name: smooth_infinitesimal_analysis
version: "9380.0.0"
language: zig
module: smooth_infinitesimal_analysis

types:
  BishopStructure:
    fields:
      carrier: String
      equality: String
      operations: List<String>
      constructive: Bool

  HoTTConcept:
    fields:
      name: String
      type_former: String
      introduction: String
      elimination: String

  SyntheticObject:
    fields:
      internal_definition: String
      external_model: String
      axioms: List<String>
      coherence: Bool

behaviors:
  - name: construct_object
    given: "Specification"
    when: "Building constructively"
    then: "Returns construction"

  - name: verify_hott
    given: "HoTT proof"
    when: "Type checking"
    then: "Returns verification"
