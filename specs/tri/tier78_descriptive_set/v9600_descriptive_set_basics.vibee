# VIBEE v9600: Descriptive Set Theory Basics
name: descriptive_set_basics
version: "9600.0.0"
language: zig
module: descriptive_set_basics

types:
  DescriptiveSet:
    fields:
      space: String
      definition: String
      complexity: String
      regularity: List<String>

  HierarchyLevel:
    fields:
      sigma: Int
      pi: Int
      boldface: Bool
      complete_set: String

  DeterminacyGame:
    fields:
      payoff_set: String
      player_i_strategy: String
      player_ii_strategy: String
      determined: Bool

behaviors:
  - name: classify_set
    given: "Definable set"
    when: "Finding complexity"
    then: "Returns hierarchy level"

  - name: check_determinacy
    given: "Game specification"
    when: "Testing determinacy"
    then: "Returns determinacy status"
