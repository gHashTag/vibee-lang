behaviors:
  - name: yolo_auto_execute
    given: Action queue
    when: YOLO mode enabled
    then: Execute without confirmation (10x speedup)
    test_cases:
      - name: test_auto
        input: { actions: 10, yolo: true }
        expected: { speedup: 10, confirmed: false }

  - name: yolo_pipeline_parallel
    given: Independent actions
    when: Parallel execution
    then: Execute in parallel (4x speedup)
    test_cases:
      - name: test_parallel
        input: { actions: 4, parallel: true }
        expected: { speedup: 4 }

