    given: Before risky action
    when: Checkpoint requested
    then: Save state for rollback
    test_cases:
      - name: test_checkpoint
        input: { action: "delete" }
        expected: { checkpoint_created: true }

  - name: yolo_rollback
    given: Error occurred
    when: Rollback triggered
    then: Restore to last checkpoint
    test_cases:
      - name: test_rollback
        input: { error: true }
        expected: { rolled_back: true }

  - name: yolo_confidence_gate
    given: Action with low confidence
    when: Threshold check
    then: Pause for confirmation if below threshold
    test_cases:
      - name: test_gate
        input: { confidence: 0.5, threshold: 0.8 }
        expected: { paused: true }

  - name: yolo_batch_commit
    given: Multiple changes
    when: Batch complete
    then: Commit all atomically
    test_cases:
      - name: test_batch
        input: { changes: 10 }
        expected: { committed: true, atomic: true }

