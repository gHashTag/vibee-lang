name: gleam_type_system_v55410
version: "55410.0.0"
language: zig
module: gleam_type_system

# GLEAM TYPE SYSTEM
# Маппинг VIBEE типов в Gleam типы

types:
  GleamPrimitiveMap:
    fields:
      vibee_string: String
      vibee_int: String
      vibee_float: String
      vibee_bool: String

  GleamCollectionMap:
    fields:
      vibee_list: String
      vibee_dict: String
      vibee_set: String

  GleamOptionMap:
    fields:
      option_type: String
      result_type: String

behaviors:
  - name: gleam_map_string
    given: VIBEE String
    when: Mapping requested
    then: Returns String

  - name: gleam_map_int
    given: VIBEE Int
    when: Mapping requested
    then: Returns Int

  - name: gleam_map_float
    given: VIBEE Float
    when: Mapping requested
    then: Returns Float

  - name: gleam_map_bool
    given: VIBEE Bool
    when: Mapping requested
    then: Returns Bool

  - name: gleam_map_option
    given: VIBEE Option of T
    when: Mapping requested
    then: Returns Option of T

  - name: gleam_map_list
    given: VIBEE List of T
    when: Mapping requested
    then: Returns List of T

  - name: gleam_map_map
    given: VIBEE Map of K V
    when: Mapping requested
    then: Returns Dict of K V
