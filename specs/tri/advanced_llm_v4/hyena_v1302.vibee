name: hyena_v1302
version: "1.0.0"
language: zig
module: hyena_v1302

types:
  ModuleUnit:
    fields:
      name: String
      imports: List<String>
      code: String

  FunctionDef:
    fields:
      name: String
      params: List<String>
      body: String

  TypeDef:
    fields:
      name: String
      fields: List<String>
      methods: List<String>

behaviors:
  - name: generate_module
    given: VIBEE spec
    when: Module generation
    then: Returns generated module

  - name: generate_function
    given: VIBEE behavior
    when: Function generation
    then: Returns generated function

  - name: generate_type
    given: VIBEE type
    when: Type generation
    then: Returns generated type

  - name: phi_constants
    given: Sacred values
    when: Constants needed
    then: Returns phi constants
