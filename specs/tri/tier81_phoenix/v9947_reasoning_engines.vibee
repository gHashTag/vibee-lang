name: reasoning_engines
version: "9947.0.0"
language: zig
module: reasoning_engines

types:
  PhoenixConcept:
    fields:
      name: String
      transcendence_level: Int
      self_reference: Bool
      emergence: Bool

  ConsciousnessState:
    fields:
      awareness: Float
      self_model: String
      meta_cognition: Bool
