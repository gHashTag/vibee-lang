    given: "VIBEE behavior"
    when: "Function generation"
    then: "AsmArm function"
    pas_pattern: ALG
    test_cases:
      - name: test_fn
        input: '{"name": "create"}'
        expected: '{"code": "..."}'
  - name: generate_tests
    given: "Test cases"
    when: "Test generation"
    then: "AsmArm tests"
    pas_pattern: ALG
    test_cases:
      - name: test_asm_arm
        input: '{"test_cases": [...]}'
        expected: '{"code": "..."}'
