# DART GRAMMAR v1012 - Dart Language Grammar Rules
# ═══════════════════════════════════════════════════════════════════════════════
# φ² + 1/φ² = 3 | PHOENIX = 999
# ═══════════════════════════════════════════════════════════════════════════════

name: dart_grammar
version: "10.1.2"
language: zig
module: dart_grammar

sacred_constants:
  phi: 1.618033988749895
  trinity: 3.0
  phoenix: 999

creation_pattern:
  source: VIBEESpec
  transformer: DartGrammar
  result: DartCode

types:
  DartAST:
    fields:
      library: String
      imports: List<String>
      declarations: List<String>

behaviors:
  - name: generate_class
    given: "VIBEE type"
    when: "Class generation"
    then: "Dart class"
    pas_pattern: ALG
    test_cases:
      - name: test_class
        input: '{"name": "User"}'
        expected: '{"code": "class User { ... }"}'

  - name: generate_function
    given: "VIBEE behavior"
    when: "Function generation"
    then: "Dart function"
    pas_pattern: ALG
    test_cases:
      - name: test_fn
        input: '{"name": "create"}'
        expected: '{"code": "Future<void> create() async { ... }"}'

  - name: generate_tests
    given: "Test cases"
    when: "Test generation"
    then: "Dart tests"
    pas_pattern: ALG
    test_cases:
      - name: test_dart
        input: '{"test_cases": [...]}'
        expected: '{"code": "test(\\"...\\" () { ... });"}'

dart_keywords: ["abstract", "as", "assert", "async", "await", "break", "case", "catch", "class", "const", "continue", "covariant", "default", "deferred", "do", "dynamic", "else", "enum", "export", "extends", "extension", "external", "factory", "false", "final", "finally", "for", "Function", "get", "hide", "if", "implements", "import", "in", "interface", "is", "late", "library", "mixin", "new", "null", "on", "operator", "part", "required", "rethrow", "return", "set", "show", "static", "super", "switch", "sync", "this", "throw", "true", "try", "typedef", "var", "void", "while", "with", "yield"]

