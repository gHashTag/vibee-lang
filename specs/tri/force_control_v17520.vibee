name: force_control_v17520
version: "1.0.0"
language: zig
module: force_control

types:
  ForceProfile:
    fields:
      forces: String
      torques: String

  ImpedanceParams:
    fields:
      stiffness: String
      damping: String

behaviors:
  - name: control_force
    given: Desired force
    when: Force control
