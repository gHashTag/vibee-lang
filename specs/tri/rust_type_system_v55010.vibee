name: rust_type_system_v55010
version: "55010.0.0"
language: zig
module: rust_type_system

# RUST TYPE SYSTEM
# Маппинг VIBEE типов в Rust типы

types:
  RustPrimitiveMap:
    fields:
      vibee_string: String
      vibee_int: String
      vibee_float: String
      vibee_bool: String

  RustCollectionMap:
    fields:
      vibee_list: String
      vibee_map: String
      vibee_set: String

  RustOptionMap:
    fields:
      option_wrapper: String
      result_wrapper: String

  RustLifetime:
    fields:
      name: String
      is_static: Bool

  RustGeneric:
    fields:
      name: String
      bounds: String

behaviors:
  - name: rust_map_string
    given: VIBEE String
    when: Mapping requested
    then: Returns String

  - name: rust_map_int
    given: VIBEE Int
    when: Mapping requested
    then: Returns i64

  - name: rust_map_float
    given: VIBEE Float
    when: Mapping requested
    then: Returns f64

  - name: rust_map_bool
    given: VIBEE Bool
    when: Mapping requested
    then: Returns bool

  - name: rust_map_option
    given: VIBEE Option of T
    when: Mapping requested
    then: Returns Option of T

  - name: rust_map_list
    given: VIBEE List of T
    when: Mapping requested
    then: Returns Vec of T

  - name: rust_map_map
    given: VIBEE Map of K V
    when: Mapping requested
    then: Returns HashMap of K V
