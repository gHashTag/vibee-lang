pas_analysis:
  source_paper: "Intuitive physics research"
  current_complexity: "O(n²) pairwise interactions"
  theoretical_lower_bound: "O(n) learned dynamics"
  gap: "Quadratic to linear via GNN"
  patterns_applicable:
    - symbol: PRE
      name: "Precomputation"
      success_rate: 0.16
      rationale: "Pre-trained physics priors"
    - symbol: MLS
      name: "ML-Guided Search"
      success_rate: 0.06
      rationale: "Learn physical dynamics"
    - symbol: D&C
      name: "Divide-and-Conquer"
      success_rate: 0.31
      rationale: "Object-centric decomposition"
    - symbol: ALG
      name: "Algebraic Reorganization"
      success_rate: 0.22
      rationale: "Physics equations"
  confidence: 0.76
  predicted_improvement: "Human-like physical intuition"

creation_pattern:
  source: PhysicalScene
  transformer: PhysicsEngine
  result: FuturePrediction

behaviors:
  - name: stability_prediction
    given: "Stack of objects"
    when: "Predict stability"
    then: "Will it fall?"
    test_cases:
      - name: block_tower
        input:
          blocks: 5
          arrangement: "precarious"
        expected:
          prediction: "unstable"
          accuracy: 0.9

  - name: trajectory_prediction
    given: "Moving object"
    when: "Predict future position"
    then: "Trajectory forecast"
    test_cases:
      - name: ball_trajectory
        input:
          initial_velocity: [5, 10]
          gravity: 9.8
        expected:
          parabolic: true
          landing_error: 0.1

  - name: collision_prediction
    given: "Two moving objects"
    when: "Predict collision"
    then: "Collision outcome"
    test_cases:
      - name: billiard_collision
        input:
          ball_a: {v: [1, 0], m: 1}
          ball_b: {v: [0, 0], m: 1}
        expected:
          momentum_conserved: true
          energy_conserved: true

  - name: counterfactual_physics
    given: "Physical scenario"
    when: "Ask 'what if'"
    then: "Alternative outcome"
    test_cases:
      - name: what_if
        input:
          scenario: "ball_on_ramp"
          intervention: "remove_obstacle"
        expected:
          counterfactual: "ball_rolls_further"
          causal_reasoning: true

algorithms:
  interaction_network:
    objects: "Node features"
    relations: "Edge features"
    dynamics: "Message passing"
    
  physics_informed_nn:
    constraints: "Physical laws as loss"
    conservation: "Energy, momentum"
    
  probabilistic_physics:
    uncertainty: "Aleatoric and epistemic"
    sampling: "Monte Carlo"

physical_concepts:
  kinematics: "Position, velocity, acceleration"
  dynamics: "Forces, momentum, energy"
  statics: "Equilibrium, stability"
  fluids: "Flow, pressure"

metrics:
  stability_accuracy: 0.9
  trajectory_error: 0.1
  collision_accuracy: 0.95
  counterfactual_accuracy: 0.8
