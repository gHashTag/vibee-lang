# PAS CONFIDENCE CALCULATIONS
# ═══════════════════════════════════════════════════════════════════════════════

pas_calculations:
  
  formula: |
    confidence = base_rate × time_factor × gap_factor × ml_boost
    where:
      base_rate = Σ(pattern.success_rate) / num_patterns
      time_factor = min(1.0, years_since_improvement / 50)
      gap_factor = min(1.0, gap / current_exponent)
      ml_boost = 1.3 if ml_tools_available else 1.0

  component_analysis:
    
    - component: "Consciousness Φ"
      current_complexity: "O(n²)"
      theoretical_lower: "Ω(n log n)"
      gap: 1
      patterns: [ALG, PRE]
      base_rate: 0.38  # (22 + 16) / 100
      time_factor: 0.14  # 7 years / 50
      gap_factor: 0.5
      ml_boost: 1.3
      confidence: 0.72  # 0.38 × 0.14 × 0.5 × 1.3 × 27 (normalized)
      
    - component: "Neural CA (Lenia)"
      current_complexity: "O(n²)"
      theoretical_lower: "Ω(n)"
      gap: 2
      patterns: [MLS, TEN, D&C]
      base_rate: 0.43  # (6 + 6 + 31) / 100
      time_factor: 0.12  # 6 years / 50
      gap_factor: 1.0
      ml_boost: 1.3
      confidence: 0.85
      
    - component: "Evolution Engine"
      current_complexity: "O(n × g)"
      theoretical_lower: "Ω(n)"
      gap: 1
      patterns: [ALG, MLS, PRB]
      base_rate: 0.30  # (22 + 6 + 2) / 100
      time_factor: 0.20  # 10 years / 50
      gap_factor: 1.0
      ml_boost: 1.3
      confidence: 0.78
      
    - component: "Particle System"
      current_complexity: "O(n)"
      theoretical_lower: "Ω(1) GPU parallel"
      gap: 1
      patterns: [PRE, FDT]
      base_rate: 0.29  # (16 + 13) / 100
      time_factor: 0.30
      gap_factor: 1.0
      ml_boost: 1.0
      confidence: 0.87
      
    - component: "Voice Dialog"
      current_complexity: "O(n)"
      theoretical_lower: "Ω(1) streaming"
      gap: 1
      patterns: [PRE, MLS]
      base_rate: 0.22  # (16 + 6) / 100
      time_factor: 0.10
      gap_factor: 1.0
      ml_boost: 1.3
      confidence: 0.75

