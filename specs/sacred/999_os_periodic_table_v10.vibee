# =============================================================================
# BEHAVIORS
# =============================================================================

behaviors:
  - name: periodic_table_display
    given: "User opens periodic table screen"
    when: "Screen loads"
    then: "Display all periods with constants organized by π power"
    test_cases:
      - name: show_all_periods
        input: { screen: "periodic_table" }
        expected: { periods_shown: 8, constants_count: 15 }
        
  - name: e8_visualization
    given: "User requests E8 information"
    when: "Voice command 'E8' or navigation"
    then: "Display E8 structure with 248 generators"
    test_cases:
      - name: show_e8_structure
        input: { command: "E8" }
        expected: { dimension: 248, rank: 8 }
        
  - name: string_theory_display
    given: "User requests string theory"
    when: "Voice command 'струны'"
    then: "Display dimensions 26, 10, 11 with Fibonacci connections"
    test_cases:
      - name: show_dimensions
        input: { command: "strings" }
        expected: { bosonic: 26, super: 10, m_theory: 11 }
        
  - name: holographic_calculation
    given: "User requests holographic principle"
    when: "Voice command 'голография'"
    then: "Display entropy formula and universe information"
    test_cases:
      - name: show_entropy
        input: { command: "holographic" }
        expected: { universe_bits: 1e122, pi_connection: "40π" }
        
  - name: fibonacci_mapping
    given: "User requests Fibonacci connections"
    when: "Voice command 'Фибоначчи'"
    then: "Display particle-Fibonacci number mappings"
    test_cases:
      - name: show_mappings
        input: { command: "fibonacci" }
        expected: { mappings_count: 7 }
        
  - name: pas_prediction_display
    given: "User requests predictions"
    when: "Voice command 'предсказание'"
    then: "Display PAS predictions with confidence levels"
    test_cases:
      - name: show_predictions
        input: { command: "prediction" }
        expected: { high: 3, medium: 3, low: 3 }

