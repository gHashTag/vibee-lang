behaviors:
  - name: analyze_algorithm
    given: "Запись алгоритма"
    when: "Анализ"
    then: "Применимые паттерны"
  - name: generate_prediction
    given: "Алгоритм + паттерны"
    when: "Предсказание"
    then: "Prediction с confidence"
  - name: validate_retrodiction
    given: "Историческое открытие"
    when: "Ретродикция"
    then: "Точность предсказания"
