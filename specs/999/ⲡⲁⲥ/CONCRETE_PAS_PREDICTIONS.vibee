    min: -8
    max: 8
    mean: 0.27
    most_common: [-1, 0, -5]
    pattern: "Симметрично около 0"
    
  m_distribution:  # Степень π
    min: -8
    max: 8
    mean: -0.5
    most_common: [-1, 0, -3]
    pattern: "Слегка отрицательный сдвиг"
    
  p_distribution:  # Степень φ
    min: -8
    max: 8
    mean: -0.3
    most_common: [-3, 6, -2]
    pattern: "Бимодальное: отрицательные и положительные"
    
  q_distribution:  # Степень e
    min: -2
    max: 2
    mean: -0.1
    most_common: [0, -1, 2, -2]
    pattern: "Малые целые числа"
    
  correlations:
