pas_patterns_applied:
  - pattern: ALG
    name: "Algebraic Reorganization"
    success_rate: 0.31
    applications:
      - "φ² + 1/φ² = 3 связывает φ и 3"
      - "e^π - π ≈ 20 связывает e и π"
      - "Факторизация n через числа Фибоначчи"
      
  - pattern: PRE
    name: "Precomputation"
    success_rate: 0.22
    applications:
      - "Таблицы степеней 3^k для k ∈ [-10, 10]"
      - "Таблицы π^m, φ^p, e^q"
      - "Кэширование промежуточных результатов"
      
  - pattern: D_C
    name: "Divide and Conquer"
    success_rate: 0.16
    applications:
      - "Разделение констант по доменам"
      - "Иерархический поиск: сначала k, потом m, p, q"
      
  - pattern: MLS
    name: "ML-Guided Search"
    success_rate: 0.13
    applications:
      - "Генетический алгоритм для поиска n, k, m, p, q"
      - "Нейросеть для предсказания начальных значений"

