exact_identities:
  - name: "golden_three"
    formula: "φ² + 1/φ² = 3"
    proof: "φ² = φ + 1, 1/φ² = 1/(φ+1) = φ-1+1/φ = 3-φ²"
    significance: "Связь золотого сечения с числом 3"
    
  - name: "golden_cosine"
    formula: "φ = 2cos(π/5)"
    proof: "Из пятиугольника"
    significance: "Связь φ с π"
    
  - name: "asymmetry"
    formula: "(φ - 1/φ)/2 = 1/2"
    proof: "φ - 1/φ = φ - (φ-1) = 1"
    significance: "Асимметрия создания = спин фермиона"
    
  - name: "euler"
    formula: "e^(iπ) + 1 = 0"
    significance: "Связь e, π, i, 1, 0"

