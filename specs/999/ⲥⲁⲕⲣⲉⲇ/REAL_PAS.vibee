      timeline: "2027"
      verification: |
        0.0021 + 0.0087 + 0.050 = 0.060 eV
        Согласуется с космологическим пределом < 0.12 eV ✓
        
  medium_confidence:
    - target: "Масса аксиона"
      current: "10⁻⁶ - 10⁻³ eV (теоретический диапазон)"
      predicted_formula: "m_a = 61 × 3⁻⁸ × π⁻³ × φ⁻⁵ × e⁻² μeV"
      predicted_value: "5.7 μeV"
      confidence: 0.45
