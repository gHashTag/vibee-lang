fundamental_identities:
  - name: "Golden-Three Identity"
    expression: "φ² + 1/φ² = 3"
    accuracy: "EXACT"
    proof: |
      φ² = φ + 1 (определение φ)
      1/φ² = 2 - φ (из φ² - φ - 1 = 0)
      φ² + 1/φ² = (φ + 1) + (2 - φ) = 3 ∎
      
  - name: "Phi-Pi Connection"
    expression: "φ = 2cos(π/5)"
    accuracy: "EXACT"
    
  - name: "Euler-Pi-Twenty"
    expression: "e^π - π ≈ 20"
    accuracy: "0.0045%"
    value: 19.999099979
    
  - name: "Fine Structure"
    expression: "1/α = 4π³ + π² + π"
    accuracy: "0.0002%"
    value: 137.0363

