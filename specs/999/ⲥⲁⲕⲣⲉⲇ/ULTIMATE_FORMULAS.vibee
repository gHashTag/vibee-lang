exact_identities:
  - name: "golden_three"
    formula: "φ² + 1/φ² = 3"
    proof: "φ² = φ + 1, 1/φ² = 2 - φ, сумма = 3"
    
  - name: "golden_cosine"
    formula: "φ = 2cos(π/5)"
    
  - name: "asymmetry"
    formula: "(φ - 1/φ)/2 = 1/2"
