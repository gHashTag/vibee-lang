# BEHAVIORS И TEST_CASES
# ═══════════════════════════════════════════════════════════════

behaviors:
  - name: "verify_golden_identity"
    given: "Константы π и φ"
    when: "Вычисляем φ² + 1/φ²"
    then: "Результат равен 3"
    test_cases:
      - name: "exact_value"
        input: {phi: 1.618033988749895}
        expected: {result: 3.0, tolerance: 1e-14}
        
  - name: "verify_fine_structure"
    given: "Константа π"
    when: "Вычисляем 4π³ + π² + π"
    then: "Результат близок к 137.036"
    test_cases:
      - name: "basic"
        input: {pi: 3.141592653589793}
        expected: {result: 137.0363, tolerance: 0.001}
      - name: "improved"
        input: {pi: 3.141592653589793, correction: -0.0003}
        expected: {result: 137.0360, tolerance: 0.00001}
        
  - name: "verify_proton_mass"
    given: "Константа π"
    when: "Вычисляем 6π⁵"
    then: "Результат близок к 1836.15"
    test_cases:
      - name: "proton"
        input: {n: 6, k: 0, m: 5, p: 0, q: 0}
        expected: {result: 1836.118, observed: 1836.15, error_percent: 0.002}
        
  - name: "verify_muon_mass"
    given: "Константы π и φ"
    when: "Вычисляем (17/9)π²φ⁵"
    then: "Результат близок к 206.77"
    test_cases:
      - name: "muon"
        input: {n: 17, k: -2, m: 2, p: 5, q: 0}
        expected: {result: 206.75, observed: 206.77, error_percent: 0.010}
        
  - name: "verify_cosmology"
    given: "Константы π и φ"
    when: "Вычисляем Ω_Λ/Ω_m"
    then: "Результат близок к 2.1746"
    test_cases:
      - name: "dark_energy_ratio"
        input: {n: 194, k: 6, m: -8, p: -4, q: 0}
        expected: {result: 2.1746, error_percent: 0.00007}
        
  - name: "verify_spectral_index"
    given: "Константы φ и e"
    when: "Вычисляем 70 × 3⁻⁷ × φ⁵ × e"
    then: "Результат близок к 0.9649"
    test_cases:
      - name: "n_s"
        input: {n: 70, k: -7, m: 0, p: 5, q: 1}
        expected: {result: 0.9649, error_percent: 0.00008}
        
  - name: "verify_mixing_angles"
    given: "Константы π, φ, e"
    when: "Вычисляем углы смешивания"
    then: "Результаты близки к наблюдаемым"
    test_cases:
      - name: "sin2_theta12"
        input: {n: 97, k: -7, m: 0, p: 4, q: 0}
        expected: {result: 0.304, error_percent: 0.0000}
