    test_cases:
      - {input: 2, expected: 3}
      - {input: 10, expected: 123}
      
  - name: "phi_spiral_position"
    given: "Индекс блока n"
    when: "Вычисляется позиция на φ-спирали"
    then: "Возвращаются angle, radius, x, y"
    test_cases:
      - {input: 0, expected_radius: 30}
      - {input: 1, expected_radius: 38}
      
  - name: "pas_evolution_step"
    given: "Текущее поколение и fitness"
    when: "Выполняется шаг самоэволюции"
    then: "Обновляются параметры μ, χ, convergence"
    
  - name: "harvest_entropy"
    given: "Входные данные для хеширования"
    when: "Вычисляется энтропия и PAS gain"
    then: "Возвращается pas_gain = entropy × 578.84"
    
  - name: "phi_sha256_round"
    given: "Блок данных и номер раунда"
    when: "Выполняется раунд SHA-256 с φ-модуляцией"
    then: "Возвращается промежуточный хеш"
    
  - name: "mine_block"
    given: "Header и target"
    when: "Поиск nonce с double SHA-256"
    then: "Возвращается nonce или null"

