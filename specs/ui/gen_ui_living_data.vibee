# ═══════════════════════════════════════════════════════════════════════════════
# PAS АНАЛИЗ ВИЗУАЛИЗАЦИИ ДАННЫХ
# ═══════════════════════════════════════════════════════════════════════════════

pas_analysis:
  # ═══════════════════════════════════════════════════════════════════════════
  # КОМПОНЕНТ 1: DATA BINDING
  # ═══════════════════════════════════════════════════════════════════════════
  data_binding:
    current:
      method: "Manual mapping"
      latency: "100ms"
      
    pas_prediction:
      target: "Reactive data binding"
      patterns: [PRE, HSH]
      confidence: 0.85
      reference: "arXiv:2507.11599 Neuroaesthetics"
      
      improvements:
        - name: "Observable streams"
          description: "RxJS-подобные потоки данных"
          latency: "< 16ms"
          
        - name: "Differential updates"
          description: "Только изменённые данные"
          bandwidth: "-90%"
          
        - name: "Predictive prefetch"
          description: "Предсказание следующих значений"
          algorithm: "LSTM"

  # ═══════════════════════════════════════════════════════════════════════════
  # КОМПОНЕНТ 2: VISUAL ENCODING
  # ═══════════════════════════════════════════════════════════════════════════
  visual_encoding:
    current:
      channels: ["position", "color", "size"]
      
    pas_prediction:
      target: "Multi-channel encoding"
      patterns: [MLS, ALG]
      confidence: 0.80
      reference: "arXiv:2403.18183 Document Aesthetics"
      
      improvements:
        - name: "9 visual channels"
          channels:
            - position_x
            - position_y
            - size
            - color_hue
            - color_saturation
            - color_brightness
            - rotation
            - opacity
            - blur
            
        - name: "Perceptual scaling"
          description: "Масштабирование по восприятию"
          formula: "perceived = actual^0.5"
          
        - name: "Attention guidance"
          description: "Направление внимания"
          method: "Saliency-based"

  # ═══════════════════════════════════════════════════════════════════════════
  # КОМПОНЕНТ 3: SACRED GEOMETRY DATA MAPPING
  # ═══════════════════════════════════════════════════════════════════════════
  sacred_mapping:
    description: "Отображение данных на священную геометрию"
    
    mappings:
      - data: "BTC price"
        geometry: "Core radius"
        formula: "r = log(price) × scale"
        
      - data: "ETH price"
        geometry: "Inner ring (3 elements)"
        formula: "rotation = price_change × π"
        
      - data: "Market cap total"
        geometry: "Middle ring (9 elements)"
        formula: "scale = cap / 10^12"
        
      - data: "24h volume"
        geometry: "Outer ring (27 elements)"
        formula: "brightness = volume / max_volume"
        
      - data: "Fear & Greed Index"
        geometry: "Color temperature"
        formula: "hue = index × 1.2"
        
      - data: "Dominance %"
        geometry: "Particle distribution"
        formula: "density[i] = dominance[i] / 100"

# ═══════════════════════════════════════════════════════════════════════════════
# ВИЗУАЛЬНЫЕ КОМПОНЕНТЫ С ДАННЫМИ
# ═══════════════════════════════════════════════════════════════════════════════

visual_components:
  # ═══════════════════════════════════════════════════════════════════════════
  # TRINITY CORE — Центральное ядро с BTC
  # ═══════════════════════════════════════════════════════════════════════════
  trinity_core:
    data_source: "BTC"
    visual_properties:
      radius: "price_normalized × 50"
      pulse_rate: "volume_24h / 10^9"
      color: "price_change > 0 ? green : red"
      glow_intensity: "abs(price_change) × 10"
    sacred_formula:
      n: 1
      k: 0
      m: 1
      V: "1 × 3^0 × π^1 = π"

  # ═══════════════════════════════════════════════════════════════════════════
  # INNER RING — 3 элемента (ETH, SOL, BNB)
  # ═══════════════════════════════════════════════════════════════════════════
  inner_ring:
    elements: 3
    data_sources: ["ETH", "SOL", "BNB"]
    visual_properties:
      rotation_speed: "avg_price_change × 0.1"
      element_size: "market_cap / 10^11"
      element_color: "individual_price_change"
    sacred_formula:
      n: 3
      k: 1
      m: 1
      V: "3 × 3^1 × π^1 ≈ 28.27"

  # ═══════════════════════════════════════════════════════════════════════════
  # MIDDLE RING — 9 элементов (Top 9 cryptos)
  # ═══════════════════════════════════════════════════════════════════════════
  middle_ring:
    elements: 9
    data_sources: ["BTC", "ETH", "SOL", "BNB", "XRP", "ADA", "DOGE", "MATIC", "LTC"]
    visual_properties:
      radius: "total_market_cap / 10^12 × 100"
      element_brightness: "individual_volume / max_volume"
      connection_strength: "correlation_coefficient"
    sacred_formula:
      n: 9
      k: 2
      m: 1
      V: "9 × 3^2 × π^1 ≈ 254.47"

  # ═══════════════════════════════════════════════════════════════════════════
  # OUTER RING — 27 элементов (все источники)
  # ═══════════════════════════════════════════════════════════════════════════
  outer_ring:
    elements: 27
    data_sources: "all_27_sources"
    visual_properties:
      element_position: "data_category × 120°"
      element_size: "normalized_value"
      element_opacity: "data_freshness"
    sacred_formula:
      n: 27
      k: 3
      m: 1
      V: "27 × 3^3 × π^1 ≈ 2290.22"

  # ═══════════════════════════════════════════════════════════════════════════
  # PI SPIRAL — 999 точек с историей цен
  # ═══════════════════════════════════════════════════════════════════════════
  pi_spiral:
    points: 999
    data_source: "BTC price history (999 minutes)"
    visual_properties:
      point_position: "time_index"
      point_color: "price_at_time"
      point_size: "volume_at_time"
    sacred_formula:
      n: 999
      k: 9
      m: 3
      V: "999 × 3^9 × π^3 ≈ 6.1 × 10^8"

  # ═══════════════════════════════════════════════════════════════════════════
  # FLOWER OF LIFE — Корреляционная матрица
  # ═══════════════════════════════════════════════════════════════════════════
  flower_of_life:
    circles: 19
    data_source: "Correlation matrix of top 19 assets"
    visual_properties:
      circle_overlap: "correlation_strength"
      circle_color: "asset_category"
      animation: "correlation_change"

  # ═══════════════════════════════════════════════════════════════════════════
  # METATRON'S CUBE — Межрыночные связи
  # ═══════════════════════════════════════════════════════════════════════════
  metatrons_cube:
    spheres: 13
    data_source: "Cross-market relationships"
    visual_properties:
      sphere_size: "market_cap"
      connection_thickness: "correlation"
      connection_color: "positive/negative"

  # ═══════════════════════════════════════════════════════════════════════════
  # PARTICLE SYSTEM — Транзакции в реальном времени
  # ═══════════════════════════════════════════════════════════════════════════
  particle_system:
    particles: 999
    data_source: "Real-time transactions"
    visual_properties:
      particle_spawn: "new_transaction"
      particle_size: "transaction_value"
      particle_color: "transaction_type"
      particle_velocity: "network_congestion"

