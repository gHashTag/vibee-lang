  # ═══════════════════════════════════════════════════════════════════════════
  right_panel:
    title: "GLOBAL"
    metrics:
      - name: "World Population"
        value: "X,XXX,XXX,XXX"
        rate: "+X/sec"
        
      - name: "Internet Users"
