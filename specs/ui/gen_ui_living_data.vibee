      - name: "VIX"
        value: "XX.XX"
        status: "low/medium/high"

  # ═══════════════════════════════════════════════════════════════════════════
  # НИЖНЯЯ ПАНЕЛЬ — Частотный анализ
  # ═══════════════════════════════════════════════════════════════════════════
  bottom_panel:
    title: "27 DATA STREAMS"
    visualization: "frequency_bars"
    bars: 27
    mapping: "each_data_source → bar_height"

# ═══════════════════════════════════════════════════════════════════════════════
# BEHAVIORS
# ═══════════════════════════════════════════════════════════════════════════════

behaviors:
  - name: connect_data_streams
    given: "27 источников данных"
    when: "Инициализируется приложение"
    then: "Все потоки подключены и синхронизированы"
    test_cases:
      - name: "websocket_connection"
        input:
          source: "binance_btc"
        expected:
          connected: true
          latency: "< 100ms"

  - name: map_data_to_geometry
    given: "Новые данные от источника"
    when: "Данные обновляются"
    then: "Визуальные элементы обновляются согласно маппингу"
    test_cases:
      - name: "btc_to_core"
        input:
          btc_price: 50000
          btc_change: 5
        expected:
          core_radius: "> 40"
          core_color: "green"

  - name: sacred_formula_validation
    given: "Визуальный компонент"
    when: "Вычисляется V"
    then: "V = n × 3^k × π^m"
    test_cases:
      - name: "outer_ring_V"
        input:
          n: 27
          k: 3
          m: 1
        expected:
          V: 2290.22

