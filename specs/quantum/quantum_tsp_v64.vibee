algorithms:
  # QAOA - Quantum Approximate Optimization Algorithm
  qaoa:
    description: "Variational algorithm for combinatorial optimization"
    steps:
      - "Encode TSP as QUBO (Quadratic Unconstrained Binary Optimization)"
      - "Initialize qubits in |+âŸ© superposition"
      - "Apply p layers of (cost Hamiltonian, mixer Hamiltonian)"
      - "Measure and optimize parameters (Î³, Î²)"
      - "Decode optimal route from measurement"
    complexity: "O(nÂ² Ã— p)"
    qubits_required: "nÂ² for n cities"
    
  # VQE - Variational Quantum Eigensolver
  vqe:
    description: "Find ground state of TSP Hamiltonian"
    steps:
      - "Construct TSP Hamiltonian H_TSP"
      - "Prepare parameterized ansatz |Ïˆ(Î¸)âŸ©"
      - "Measure âŸ¨Ïˆ(Î¸)|H_TSP|Ïˆ(Î¸)âŸ©"
      - "Classical optimizer updates Î¸"
      - "Repeat until convergence"
    complexity: "O(nÂ² Ã— d Ã— shots)"
    ansatz: "Hardware-efficient ansatz"
    
  # Grover's Algorithm for TSP
  grover_tsp:
    description: "Quadratic speedup for searching optimal route"
    steps:
      - "Encode all n! permutations in superposition"
      - "Oracle marks routes shorter than threshold"
      - "Apply Grover diffusion operator"
      - "Repeat O(âˆš(n!)) times"
      - "Measure optimal route"
    complexity: "O(âˆš(n!))"
    speedup: "Quadratic over brute force"
    
  # Quantum Annealing
  quantum_annealing:
    description: "Adiabatic evolution to ground state"
    steps:
      - "Start in ground state of simple Hamiltonian"
      - "Slowly evolve to TSP Hamiltonian"
      - "System remains in ground state (adiabatic theorem)"
      - "Measure final state = optimal route"
    complexity: "O(1/Î”Â²) where Î” = energy gap"
    hardware: "D-Wave, neutral atoms"

