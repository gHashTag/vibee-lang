swe_agents:
  name: "Software Engineering Agents"
  arxiv: ["2512.24630", "2512.24040", "2512.23631"]
  
  agent_types:
    - name: "CodeAgent"
      role: "Code generation and modification"
      capabilities:
        - "Read and understand codebase"
        - "Generate new code from specs"
        - "Refactor existing code"
        - "Fix bugs automatically"
      
    - name: "TestAgent"
      role: "Testing and verification"
      capabilities:
        - "Generate test cases"
        - "Run tests"
        - "Analyze coverage"
        - "Report failures"
      
    - name: "DocAgent"
      role: "Documentation generation"
      capabilities:
        - "Generate API docs"
        - "Create diagrams"
        - "Update README"
        - "Write tutorials"
      
    - name: "EvolutionAgent"
      role: "Self-improvement"
      capabilities:
        - "Monitor performance"
        - "Identify improvements"
        - "Apply optimizations"
        - "Learn from feedback"

  road_framework:
    arxiv: "2512.24040"
    name: "Reflective Optimization via Automated Debugging"
    components:
      - "Reflection Module"
      - "Optimization Engine"
      - "Automated Debugger"
      - "Feedback Loop"
    
  boad_framework:
    arxiv: "2512.23631"
    name: "Bandit Optimization for Agentic Debugging"
    approach: "Hierarchical multi-armed bandit"
    benefit: "Efficient exploration of solution space"

  creation_pattern:
    source: CodebaseState
    transformer: SWEAgentPipeline
    result: ImprovedCodebase

