# ═══════════════════════════════════════════════════════════════════════════════
# MATHEMATICAL MODELS
# ═══════════════════════════════════════════════════════════════════════════════

mathematical_models:
  
  training_time:
    model: "T(t) = T₀ × e^(-αt²)"
    type: "Super-exponential"
    parameters:
      T0: 86400  # seconds (24 hours)
      alpha: 0.15
    predictions:
      2020: "24 hours"
      2022: "5 seconds"
      2025: "1 second"
      2027: "0.1 seconds"
      
  inference_time:
    model: "I(t) = I₀ × e^(-e^(βt))"
    type: "Double-exponential"
    parameters:
      I0: 5400  # seconds (1.5 hours)
      beta: 0.8
    predictions:
      2022: "1.5 hours"
      2024: "0.1 seconds"
      2026: "0.01 seconds"
      
  quality_psnr:
    model: "Q(t) = Q_max - (Q_max - Q₀) × e^(-γt)"
    type: "Logarithmic (approaching asymptote)"
    parameters:
      Q_max: 35  # dB
      Q0: 31     # dB
      gamma: 0.5
    predictions:
      2020: "31 dB"
      2023: "33 dB"
      2026: "34.5 dB"
      2030: "35 dB (limit)"
      
  mse_improvement:
    model: "MSE(t) = MSE₀ × e^(-αt²)"
    type: "Super-exponential"
    parameters:
      MSE0: 1.0
      alpha: 0.15
    predictions:
      2020: "60x (ReSTIR)"
      2021: "166x (ReSTIR GI)"
      2025: "500x"
      2027: "1000x"
      2030: "10000x"

