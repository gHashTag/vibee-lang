# ═══════════════════════════════════════════════════════════════════════════════
# PAS CONFIDENCE FORMULA
# ═══════════════════════════════════════════════════════════════════════════════

confidence_formula:
  equation: "confidence = base_rate × time_factor × gap_factor × ml_boost"
  
  parameters:
    base_rate:
      description: "Historical success rate of pattern"
      values:
        D&C: 0.31
        ALG: 0.22
        PRE: 0.16
        FDT: 0.13
        MLS: 0.08
        TEN: 0.06
        HSH: 0.05
        PRB: 0.04
        
    time_factor:
      description: "Decay based on prediction horizon"
      formula: "e^(-t/τ), τ=3 years"
      
    gap_factor:
      description: "Normalized gap to theoretical bound"
      formula: "1 - 1/gap"
      
    ml_boost:
      description: "ML/GPU acceleration multiplier"
      range: [1.5, 3.0]

