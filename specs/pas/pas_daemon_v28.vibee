# BEHAVIORS
# ═══════════════════════════════════════════════════════════════════════════════

behaviors:
  - name: calculate_confidence
    given: "Pattern combination and timeline"
    when: "Prediction confidence requested"
    then: "Returns confidence using PAS formula"
    test_cases:
      - name: "1000_fps_prediction"
        input:
          patterns: [ALG, PRE, D&C]
          timeline_years: 0.5
          gap: 20
          ml_boost: 2.0
        expected:
          confidence: 0.92
          
      - name: "sub_second_training"
        input:
          patterns: [HSH, TEN, PRE]
          timeline_years: 1.5
          gap: 5
          ml_boost: 2.5
        expected:
          confidence: 0.85
          
  - name: predict_gap_closure
    given: "Current gap and improvement rate"
    when: "Gap closure prediction requested"
    then: "Returns year when gap closes"
    test_cases:
      - name: "training_gap"
        input:
          current_gap: 100
          improvement_rate: 10  # per year
        expected:
          closure_year: 2027
          
  - name: identify_patterns
    given: "Algorithm description"
    when: "Pattern identification requested"
    then: "Returns applicable PAS patterns"
    test_cases:
      - name: "restir_patterns"
        input:
          algorithm: "ReSTIR"
          description: "Reservoir-based importance resampling"
        expected:
          primary: PRB
          secondary: [PRE, D&C]

